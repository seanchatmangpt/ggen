@prefix ggen: <http://ggen.dev/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/template1> a ggen:Template ;
  ggen:templateName "Template with Invalid Variable Name" ;
  ggen:templateVersion "1.0.0" ;
  ggen:hasVariable [
    a ggen:Variable ;
    ggen:variableName "123-invalid-name" ;
    ggen:variableType "string" ;
    ggen:isRequired "true"^^xsd:boolean
  ] .
