{
  "version": "1.0.0",
  "timestamp": "{{ timestamp | default(value="2026-02-11T00:00:00Z") }}",
  "workflow_id": "{{ workflow_id | default(value="ln_ctrl") }}",
  "artifacts": [
    {%- for row in results %}
    {
      "path": "{{ row.path }}",
      "format": "{{ row.format }}",
      {%- if row.validator %}
      "validator": "{{ row.validator }}",
      {%- endif %}
      {%- if row.hash_algorithm %}
      "hash_algorithm": "{{ row.hash_algorithm }}",
      {%- else %}
      "hash_algorithm": "sha256",
      {%- endif %}
      "hash": "",
      "metadata": {
        {%- if row.artifact_type %}
        "type": "{{ row.artifact_type }}",
        {%- endif %}
        {%- if row.description %}
        "description": "{{ row.description }}",
        {%- endif %}
        "generated_at": "{{ timestamp | default(value="2026-02-11T00:00:00Z") }}"
      }
    }{% if not loop.last %},{% endif %}
    {%- endfor %}
  ],
  "statistics": {
    "total_artifacts": {{ results | length }}
  },
  "validation": {
    "manifest_hash": "",
    "all_files_present": false,
    "all_hashes_valid": false,
    "last_verified": null
  }
}
