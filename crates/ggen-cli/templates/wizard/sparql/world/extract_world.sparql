PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX world: <https://ggen.io/ontology/world#>

SELECT ?artifact ?path ?format ?validator ?hash_algorithm
WHERE {
  ?artifact a world:Artifact ;
            world:path ?path ;
            world:format ?format .
  OPTIONAL { ?artifact world:validator ?validator . }
  OPTIONAL { ?artifact world:hash_algorithm ?hash_algorithm . }
}
ORDER BY ?path
