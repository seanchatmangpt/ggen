@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ggen: <https://ggen.io/ontology#> .
@prefix world: <https://ggen.io/ontology/world#> .

# World Manifest Ontology
# Generated by ggen wizard
# Defines the closed output universe

world:Manifest a rdfs:Class ;
    rdfs:label "World Manifest" ;
    rdfs:comment "Complete manifest of all outputs in the closed world" .

world:Artifact a rdfs:Class ;
    rdfs:label "Artifact" ;
    rdfs:comment "A generated artifact in the world" .

world:version a rdf:Property ;
    rdfs:domain world:Manifest ;
    rdfs:range rdfs:Literal ;
    rdfs:label "version" ;
    rdfs:comment "Manifest version" .

world:timestamp a rdf:Property ;
    rdfs:domain world:Manifest ;
    rdfs:range rdfs:Literal ;
    rdfs:label "timestamp" ;
    rdfs:comment "Generation timestamp" .

world:artifacts a rdf:Property ;
    rdfs:domain world:Manifest ;
    rdfs:range world:Artifact ;
    rdfs:label "artifacts" ;
    rdfs:comment "List of artifacts in the world" .

world:path a rdf:Property ;
    rdfs:domain world:Artifact ;
    rdfs:range rdfs:Literal ;
    rdfs:label "path" ;
    rdfs:comment "File path of the artifact" .

world:hash a rdf:Property ;
    rdfs:domain world:Artifact ;
    rdfs:range rdfs:Literal ;
    rdfs:label "hash" ;
    rdfs:comment "SHA-256 hash of the artifact" .

world:format a rdf:Property ;
    rdfs:domain world:Artifact ;
    rdfs:range rdfs:Literal ;
    rdfs:label "format" ;
    rdfs:comment "Format of the artifact" .

world:validator a rdf:Property ;
    rdfs:domain world:Artifact ;
    rdfs:range rdfs:Literal ;
    rdfs:label "validator" ;
    rdfs:comment "Validator for this artifact" .

# Define specific outputs
world:WorldManifest a world:Artifact ;
    world:path "generated/world.manifest.json" ;
    world:format "json" ;
    world:validator "json-schema" .

world:WorldVerifier a world:Artifact ;
    world:path "generated/world.verify.mjs" ;
    world:format "javascript" ;
    world:validator "syntax" .

world:ReceiptSchema a world:Artifact ;
    world:path "generated/receipts/receipt.schema.json" ;
    world:format "json" ;
    world:validator "json-schema" .

world:VerdictSchema a world:Artifact ;
    world:path "generated/receipts/verdict.schema.json" ;
    world:format "json" ;
    world:validator "json-schema" .
