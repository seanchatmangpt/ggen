@prefix ex: <http://ggen.example.org/disney/guest-safety-compliance#> .
@prefix wo: <http://ggen.example.org/disney/work-objects#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# ==============================================================================
# Guest Safety & Compliance Process Ontology
# ==============================================================================
# Purpose: Continuous safety inspections, compliance certifications, incident tracking, regulatory reporting
# Audience: Safety officers, compliance managers, regulatory affairs, incident response teams
# Wave: 3 (Regulatory-critical process)
# Status: Mapped to Work Object Model types
# Created: 2026-01-18
# ==============================================================================

ex:GuestSafetyComplianceProcess a owl:Class ;
  rdfs:label "Guest Safety & Compliance Process" ;
  rdfs:comment "Continuous monitoring of guest safety, regulatory compliance, incident reporting, and certification management. Integrates inspections, certifications, incident tracking, and regulatory reporting." ;
  rdfs:isDefinedBy ex:GuestSafetyComplianceOntology .

ex:DailyComplianceCycle a ex:GuestSafetyComplianceProcess ;
  rdfs:label "Daily Compliance Cycle" ;
  rdfs:comment "Morning and evening safety sweeps, incident reviews, and compliance status updates." ;
  ex:executionFrequency "Twice daily (morning at 05:00, evening at 21:00)" ;
  ex:targetCompletionTime "60 minutes per cycle" ;
  ex:estimatedDuration "PT60M"^^xsd:duration ;
  ex:dependencies ex:InspectionApproval, ex:IncidentEscalationReview, ex:ComplianceCertificationReview .

# ==============================================================================
# SHIFTS (Maps to wo:Shift)
# ==============================================================================

ex:SafetyInspectionShift a wo:Shift ;
  rdfs:label "Safety Inspection Shift" ;
  rdfs:comment "Morning and evening safety teams conducting inspections." ;
  ex:role "Safety Officer, Compliance Inspector, Environmental Health Specialist" ;
  ex:startTime "2026-01-19T05:00:00"^^xsd:dateTime ;
  ex:endTime "2026-01-19T14:00:00"^^xsd:dateTime ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "SHIFT-SAFETY-INSP-001" ;
  ex:location ex:SafetyOperationsCenter ;
  ex:staffingRequirement [
    ex:role "Safety Officer" ;
    ex:count 4 ;
    ex:source "Workday" ;
    ex:workdayTeamId "SAFETY-OFFICERS-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Compliance Inspector" ;
    ex:count 3 ;
    ex:source "Workday" ;
    ex:workdayTeamId "COMPLIANCE-INSP-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Environmental Health Specialist" ;
    ex:count 2 ;
    ex:source "Workday" ;
    ex:workdayTeamId "ENV-HEALTH-001"
  ] .

ex:IncidentResponseShift a wo:Shift ;
  rdfs:label "Incident Response Shift" ;
  rdfs:comment "24/7 incident response team on-call." ;
  ex:role "Incident Commander, First Responder Coordinator, Legal Liaison" ;
  ex:continuous "true" ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "SHIFT-INCIDENT-RESPONSE-001" ;
  ex:location ex:IncidentCommandCenter ;
  ex:staffingRequirement [
    ex:role "Incident Commander" ;
    ex:count 1 ;
    ex:source "Workday" ;
    ex:workdayTeamId "INC-CMD-001"
  ] .

# ==============================================================================
# TASKS (Maps to wo:Task)
# ==============================================================================

ex:MorningAttractionInspection a wo:Task ;
  rdfs:label "Morning Attraction Safety Inspection" ;
  rdfs:comment "Comprehensive pre-opening inspection of all attractions; mechanical, electrical, safety systems." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-INSP-MORNING-001" ;
  wo:hasTitle "Pre-Opening Safety Inspection: All Attractions" ;
  wo:taskDescription "Walk all 23 attractions; verify no guest-facing hazards; inspect structural integrity; test safety systems; document findings in CMMS." ;
  wo:taskAssignedTo ex:SafetyOfficerLead ;
  wo:taskEstimatedDuration "PT45M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:InspectionAC1, ex:InspectionAC2, ex:InspectionAC3 ;
  ex:executionOrder 1 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:InspectionDataValidation ;
  ex:externalSystemDependency [
    ex:system "SAP CMMS" ;
    ex:functionality "Asset inspection history, maintenance schedules, open maintenance orders" ;
    ex:requiredBefore "Inspection execution"
  ] ;
  ex:regulatoryMapping "OSHA 1910.147 (Lockout/Tagout), ANSI B77.1 (Amusement Ride Safety)" .

ex:InspectionAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When inspection complete, then all 23 attractions inspected and documented in CMMS within 45 minutes." ;
  wo:criterionVerified "false" .

ex:InspectionAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When inspection complete, then zero safety defects identified that would prevent opening (critical defects escalated immediately)." ;
  wo:criterionVerified "false" .

ex:InspectionAC3 a wo:AcceptanceCriterion ;
  wo:criterionText "When inspection complete, then inspection checklist signed by lead safety officer and uploaded to compliance repository." ;
  wo:criterionVerified "false" .

ex:InspectionDataValidation a wo:Task ;
  rdfs:label "Inspection Data Validation & Anomaly Detection" ;
  rdfs:comment "Review inspection data; flag patterns suggesting recurring issues or systemic risks." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-DATA-VAL-001" ;
  wo:hasTitle "Validate Inspection Data and Detect Anomalies" ;
  wo:taskAssignedTo ex:ComplianceManagerLead ;
  wo:taskEstimatedDuration "PT8M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:ValidationAC1, ex:ValidationAC2 ;
  wo:dependsOn ex:MorningAttractionInspection ;
  ex:executionOrder 2 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:InspectionApproval ;
  ex:externalSystemDependency [
    ex:system "Tableau Analytics" ;
    ex:functionality "Historical defect patterns, trend analysis" ;
    ex:requiredBefore "Anomaly detection"
  ] .

ex:ValidationAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When validation complete, then defects classified by severity (critical, major, minor) and trending tracked." ;
  wo:criterionVerified "false" .

ex:ValidationAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When validation complete, then anomalies (5+ consecutive defects in same system) flagged for engineering review." ;
  wo:criterionVerified "false" .

ex:IncidentEscalationReview a wo:Task ;
  rdfs:label "Incident Escalation & Classification" ;
  rdfs:comment "Review any incidents reported in past 24 hours; classify severity; determine if regulatory report required." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-INCIDENT-ESC-001" ;
  wo:hasTitle "Review and Classify Guest Safety Incidents" ;
  wo:taskAssignedTo ex:IncidentCommanderLead ;
  wo:taskEstimatedDuration "PT15M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:IncidentAC1, ex:IncidentAC2 ;
  ex:executionOrder 3 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:RegulatoryReportingDecision ;
  ex:externalSystemDependency [
    ex:system "ServiceNow Incident Management" ;
    ex:functionality "All incidents reported in past 24 hours with medical detail" ;
    ex:requiredBefore "Escalation decision"
  ] ;
  ex:regulatoryMapping "OSHA 301 (Incident Reporting), State Health Dept. Requirements, Local Fire Code" .

ex:IncidentAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then all incidents classified by severity (critical, major, minor) and causation analyzed." ;
  wo:criterionVerified "false" .

ex:IncidentAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then any reportable incident flagged for legal review and regulatory notification within 2 hours." ;
  wo:criterionVerified "false" .

ex:RegulatoryReportingDecision a wo:Task ;
  rdfs:label "Regulatory Reporting Decision" ;
  rdfs:comment "Determine if incident requires regulatory notification; prepare required reports." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-REG-REPORTING-001" ;
  wo:hasTitle "Evaluate and Execute Regulatory Reporting" ;
  wo:taskAssignedTo ex:LegalLiaisonLead ;
  wo:taskEstimatedDuration "PT20M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:ReportingAC1, ex:ReportingAC2 ;
  wo:dependsOn ex:IncidentEscalationReview ;
  ex:executionOrder 4 ;
  ex:criticalPath "true" ;
  ex:externalSystemDependency [
    ex:system "Salesforce CRM (Legal Module)" ;
    ex:functionality "Incident details, witness statements, medical reports" ;
    ex:requiredBefore "Report generation"
  ] ;
  ex:regulatoryMapping "OSHA Form 301, State Incident Reports, Local Health Dept. Notifications" .

ex:ReportingAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When decision complete, then all reportable incidents identified and regulatory filing deadline calculated." ;
  wo:criterionVerified "false" .

ex:ReportingAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When reports submitted, then all required forms filed with appropriate agencies within compliance deadline." ;
  wo:criterionVerified "false" .

ex:ComplianceCertificationReview a wo:Task ;
  rdfs:label "Compliance Certification Management" ;
  rdfs:comment "Track and manage safety certifications; flag approaching expiration dates." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-CERT-REVIEW-001" ;
  wo:hasTitle "Review Compliance Certifications and Renewal Status" ;
  wo:taskAssignedTo ex:ComplianceManagerLead ;
  wo:taskEstimatedDuration "PT10M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:CertAC1, ex:CertAC2 ;
  ex:executionOrder 5 ;
  ex:criticalPath "true" ;
  ex:externalSystemDependency [
    ex:system "SAP CMMS" ;
    ex:functionality "Certification tracking, expiration dates, renewal schedules" ;
    ex:requiredBefore "Review"
  ] .

ex:CertAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then all 23 attractions have valid safety certifications (none expired)." ;
  wo:criterionVerified "false" .

ex:CertAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then any certifications expiring within 30 days flagged; renewal scheduled with vendor." ;
  wo:criterionVerified "false" .

# ==============================================================================
# APPROVALS (Maps to wo:Approval)
# ==============================================================================

ex:InspectionApproval a wo:Approval ;
  rdfs:label "Safety Inspection Approval" ;
  rdfs:comment "Safety director approval for morning inspection results." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "APPROVAL-INSP-001" ;
  wo:hasTitle "Safety Director Inspection Approval" ;
  wo:approvalsRequired 1 ;
  wo:approvalsReceived 0 ;
  wo:approvalRequiredFrom ex:SafetyDirectorRole ;
  ex:gates [
    ex:gateName "All Attractions Inspected" ;
    ex:dependsOn ex:InspectionDataValidation ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:gates [
    ex:gateName "No Critical Defects" ;
    ex:requiredEvidence "Inspection report with zero critical safety issues" ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:blocksProgression ex:DailyComplianceCycle .

ex:IncidentEscalationApproval a wo:Approval ;
  rdfs:label "Incident Escalation Approval" ;
  rdfs:comment "Chief medical officer and legal approval for incident classification and regulatory action." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "APPROVAL-INC-ESC-001" ;
  wo:hasTitle "Medical and Legal Incident Approval" ;
  wo:approvalsRequired 2 ;
  wo:approvalsReceived 0 ;
  wo:approvalRequiredFrom ex:ChiefMedicalOfficerRole ;
  wo:approvalRequiredFrom ex:GeneralCounselRole ;
  ex:gates [
    ex:gateName "Incident Review Complete" ;
    ex:dependsOn ex:IncidentEscalationReview
  ] ;
  ex:gates [
    ex:gateName "Regulatory Determination Made" ;
    ex:dependsOn ex:RegulatoryReportingDecision
  ] ;
  ex:blocksProgression ex:RegulatoryReporting .

# ==============================================================================
# INCIDENTS (Maps to wo:Incident)
# ==============================================================================

ex:IncidentTemplate_EquipmentDefect a wo:Incident ;
  rdfs:label "Critical Safety Defect Identified" ;
  rdfs:comment "Inspection reveals critical safety defect requiring immediate action." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "INC-SAFETY-DEFECT-TEMPLATE" ;
  ex:isTemplate "true" ;
  ex:incidentSeverity "High" ;
  ex:incidentDescription "Safety inspection identifies critical defect that could pose guest risk." ;
  ex:blocksProcesses ex:DailyComplianceCycle ;
  ex:remediationPath [
    ex:firstStep "Isolate affected attraction; place closure signs" ;
    ex:createTask ex:EmergencyRepairTask ;
    ex:estimatedDelay "4-24 hours depending on severity" ;
    ex:requiresApprovalBefore "Reopening"
  ] ;
  ex:escalationPath [
    ex:severity "High" ;
    ex:escalateTo ex:SafetyDirector
  ] ;
  ex:regulatoryMapping "OSHA 1910.22 (Hazard Correction)" .

ex:IncidentTemplate_GuestInjury a wo:Incident ;
  rdfs:label "Guest Injury Incident" ;
  rdfs:comment "Medical incident involving guest injury; requires documentation and possible regulatory reporting." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "INC-GUEST-INJURY-TEMPLATE" ;
  ex:isTemplate "true" ;
  ex:incidentSeverity "Critical" ;
  ex:incidentDescription "Guest medical incident requiring incident report, witness statements, and regulatory evaluation." ;
  ex:remediationPath [
    ex:firstStep "First aid and emergency medical response" ;
    ex:createTask ex:IncidentDocumentationTask ;
    ex:createTask ex:RegulatoryReportingTask ;
    ex:estimatedDelay "2-48 hours for regulatory reporting"
  ] ;
  ex:escalationPath [
    ex:severity "Critical" ;
    ex:escalateTo ex:IncidentCommanderLead ;
    ex:escalateTo ex:ChiefMedicalOfficer ;
    ex:escalateTo ex:GeneralCounsel
  ] ;
  ex:regulatoryMapping "OSHA Form 301, State Health Dept. Incident Report" .

# ==============================================================================
# RESOURCES (Maps to wo:Resource)
# ==============================================================================

