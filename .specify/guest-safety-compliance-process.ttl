  rdfs:label "Safety Inspection Equipment" ;
  rdfs:comment "Inspection tools, checklists, mobile devices." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "RES-INSP-EQUIP-001" ;
  wo:resourceType "Equipment and Tools" ;
  ex:requiredItems [
    ex:item "Mobile inspection device (tablet with inspection app)" ;
    ex:quantity 4 ;
    ex:systemId "TABLET-INSP-001-004"
  ] ;
  ex:requiredItems [
    ex:item "Inspection checklists (printed and digital)" ;
    ex:quantity 1 ;
    ex:systemId "CHECKLIST-SAFETY-V3"
  ] ;
  ex:requiredItems [
    ex:item "First aid kits (distributed across park)" ;
    ex:quantity 6 ;
    ex:systemId "FIRST-AID-KIT-001-006"
  ] ;
  wo:resourceAvailability "Available" ;
  ex:verificationMethod "Daily inventory check; battery charge verification" .

ex:ComplianceDataSystem a wo:Resource ;
  rdfs:label "Compliance Data Management System" ;
  rdfs:comment "CMMS, incident management, regulatory reporting." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "RES-COMPLIANCE-SYS-001" ;
  wo:resourceType "Software System" ;
  ex:systemComponents [
    ex:component "SAP CMMS (Computerized Maintenance Management System)" ;
    ex:systemId "SAP-CMMS-PROD"
  ] ;
  ex:systemComponents [
    ex:component "ServiceNow Incident Management" ;
    ex:systemId "SERVICENOW-INC-PROD"
  ] ;
  ex:systemComponents [
    ex:component "Regulatory Reporting Module" ;
    ex:systemId "REG-REPORT-V2"
  ] ;
  wo:resourceAvailability "Available" ;
  ex:verificationMethod "System health check; database backup verification" .

# ==============================================================================
# EVENTS (Maps to wo:Event)
# ==============================================================================

ex:EventInspectionComplete a wo:Event ;
  rdfs:label "Morning Safety Inspection Complete" ;
  rdfs:comment "Signal that morning inspection cycle finished." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-INSP-COMPLETE-001" ;
  wo:eventType "milestone" ;
  wo:eventMessage "Morning safety inspection complete. All 23 attractions inspected. No critical defects identified." ;
  wo:eventSource "CMMS System" ;
  ex:eventTrigger ex:InspectionDataValidation ;
  ex:notificationChannel "Slack #safety-operations" .

ex:EventRegulatoryAlert a wo:Event ;
  rdfs:label "Regulatory Reporting Required" ;
  rdfs:comment "Alert that incident requires regulatory notification." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-REG-ALERT-001" ;
  wo:eventType "alert" ;
  wo:eventMessage "Reportable incident detected. Regulatory filing required within 24 hours." ;
  wo:eventSource "Incident Management System" ;
  ex:eventTrigger ex:IncidentEscalationReview ;
  ex:notificationChannel "Slack #incident-escalation, PagerDuty legal-on-call" .

ex:EventCertificationExpiring a wo:Event ;
  rdfs:label "Certification Expiration Alert" ;
  rdfs:comment "Alert for approaching certification expiration." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-CERT-EXP-001" ;
  wo:eventType "alert" ;
  wo:eventSource "CMMS System" ;
  ex:eventTrigger [
    ex:condition "Certification expiring within 30 days" ;
    ex:source "SAP CMMS"
  ] ;
  ex:notificationChannel "Slack #compliance-alerts, compliance-manager@disney.com" .

# ==============================================================================
# PROCESS FLOW & DEPENDENCIES
# ==============================================================================

ex:ComplianceCycleFlow a ex:ProcessFlow ;
  rdfs:label "Compliance Cycle Critical Path" ;
  rdfs:comment "Morning inspection through to approval." ;
  ex:phase1_Inspection "MorningAttractionInspection (start 05:00, PT45M)" ;
  ex:phase2_Validation "InspectionDataValidation (PT8M)" ;
  ex:phase3_Escalation "IncidentEscalationReview (parallel, PT15M)" ;
  ex:phase4_Reporting "RegulatoryReportingDecision (if incident exists, PT20M)" ;
  ex:phase5_Certification "ComplianceCertificationReview (parallel, PT10M)" ;
  ex:phase6_Approval "InspectionApproval (gates park opening)" ;
  ex:criticalPathDuration "PT78M"^^xsd:duration ;
  ex:targetCompleteTime "06:20 (60 minutes total)" ;
  ex:frequency "Twice daily (morning and evening)" ;
  ex:parallelizationOpportunity "Incident escalation and certification review can run in parallel with validation" .

# ==============================================================================
# EXTERNAL SYSTEM MAPPINGS
# ==============================================================================

ex:SAPCMMSMapping a ex:SystemMapping ;
  rdfs:label "SAP CMMS Integration" ;
  rdfs:comment "Maintenance orders, asset history, certification tracking." ;
  ex:maps wo:Task ;
  ex:dataFlow "SAP CMMS → Asset inspection history, open maintenance orders, certification status → Inspection execution and validation" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForCompliance "true" .

ex:ServiceNowMapping a ex:SystemMapping ;
  rdfs:label "ServiceNow Incident Management Integration" ;
  rdfs:comment "All incidents reported in past 24 hours." ;
  ex:maps wo:Task ;
  ex:dataFlow "ServiceNow → Incidents with medical detail, witness info → Escalation and regulatory decision" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" ;
  ex:criticalForCompliance "true" .

ex:TableauMapping a ex:SystemMapping ;
  rdfs:label "Tableau Analytics Integration" ;
  rdfs:comment "Historical defect patterns and trend analysis." ;
  ex:maps wo:Task ;
  ex:dataFlow "Tableau → Defect history, trend analysis → Anomaly detection" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" .

ex:SalesforceMapping a ex:SystemMapping ;
  rdfs:label "Salesforce CRM (Legal Module) Integration" ;
  rdfs:comment "Incident details, witness statements, medical reports for legal tracking." ;
  ex:maps wo:Task ;
  ex:dataFlow "Salesforce → Incident narratives, witness contact info, medical notes → Regulatory reporting" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForCompliance "true" .

ex:SlackMapping a ex:SystemMapping ;
  rdfs:label "Slack Notifications" ;
  rdfs:comment "Safety operation alerts and compliance notifications." ;
  ex:maps wo:Event ;
  ex:channels (
    "#safety-operations"
    "#incident-escalation"
    "#compliance-alerts"
  ) ;
  ex:messageTemplate "Safety Alert: {event} - {details} (Action Required: {action})" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" .

# ==============================================================================
# SUCCESS METRICS & CYCLE TIME IMPROVEMENTS
# ==============================================================================

ex:SuccessMetrics a owl:Class ;
  rdfs:label "Guest Safety & Compliance Success Metrics" ;
  rdfs:comment "Key performance indicators for process improvement." ;
  ex:metric_1 "Safety inspection completion rate (target: 100% daily)" ;
  ex:metric_2 "Critical defect response time (target: <2 hours from identification to isolation)" ;
  ex:metric_3 "Regulatory reporting compliance rate (target: 100% within deadline)" ;
  ex:metric_4 "Incident report accuracy (target: Zero defects in filings)" ;
  ex:metric_5 "Certification renewal rate (target: 100% before expiration)" ;
  ex:metric_6 "Guest incident rate per 1M visitors (target: <5 reportable incidents)" ;
  ex:metric_7 "Compliance audit pass rate (target: 100% on external audits)" .

ex:CycleTimeImprovement a owl:Class ;
  rdfs:label "Cycle Time Improvements" ;
  rdfs:comment "Reduction vs manual process baseline." ;
  ex:currentManualCycleTime "180 minutes (paper checklists, manual data entry)" ;
  ex:targetAutomatedCycleTime "60 minutes (automated validation, incident routing)" ;
  ex:cycleTimeReduction "66.7% (120 minutes faster per cycle, 240 min/day)" ;
  ex:annualTimeRecovery "1,460 inspector hours/year" ;
  ex:complianceImprovement "100% audit pass rate + zero reporting errors + 100% regulatory deadline compliance" ;
  ex:businessImpact "Reduced liability exposure + regulatory confidence + improved safety culture" .

