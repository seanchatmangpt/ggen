  ex:workdayId "DATA-SCI-001" .

ex:PricingManagerLead a ex:Person ;
  rdfs:label "Pricing Manager" ;
  ex:workdayId "PRICING-MGR-001" .

ex:OperationsPlanner a ex:Person ;
  rdfs:label "Operations Planner" ;
  ex:workdayId "OPS-PLAN-001" .

ex:OperationsDirector a ex:Person ;
  rdfs:label "Operations Director" ;
  ex:personRole ex:OperationsDirectorRole ;
  ex:workdayId "OPS-DIR-001" .

# ==============================================================================
# LOCATIONS
# ==============================================================================

ex:RevenueManagementCenter a ex:Location ;
  rdfs:label "Revenue Management Center" ;
  rdfs:comment "Centralized location for pricing and revenue optimization." ;
  ex:locationCode "RMC-001" ;
  ex:area "2,000 sq ft" ;
  ex:analyticsWorkstations 7 ;
  ex:dataConnectionRedundancy "Dual fiber to Oracle and Salesforce" .
