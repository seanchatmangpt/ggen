  rdfs:comment "Validates forecast model outputs." ;
  ex:systemMapping "AD group: DATA_SCI_LEADS" .

ex:RevenueDirector a ex:Person ;
  rdfs:label "Revenue Director" ;
  ex:personRole ex:RevenueDirectorRole ;
