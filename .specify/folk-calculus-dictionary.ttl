# Folk Strategy to Calculus Dictionary
# Complete mapping of 60+ folk strategy terms to mathematical calculus objects
# Purpose: Encode folk entrepreneurship wisdom as queryable RDF ontology
# Version: 1.0
# Created: 2026-01-18

@prefix folk: <http://ggen.io/folk-strategy/> .
@prefix calc: <http://ggen.io/calculus/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ==============================================================================
# CORE VOCABULARY
# ==============================================================================

folk:FolkTerm a owl:Class ;
    rdfs:label "Folk Strategy Term" ;
    rdfs:comment "An informal term used in entrepreneurship to describe phenomena" .

folk:CalculusMapping a owl:Class ;
    rdfs:label "Calculus Mapping" ;
    rdfs:comment "Links a folk term to one or more calculus objects" .

folk:Attribution a owl:Class ;
    rdfs:label "Attribution" ;
    rdfs:comment "What phenomenon class the folk term attributes causality to (luck, skill, art, mystery)" .

folk:ObservableConsequence a owl:Class ;
    rdfs:label "Observable Consequence" ;
    rdfs:comment "Measurable empirical outcome predicted by the folk term" .

# Attribution types
folk:Luck a folk:Attribution ;
    rdfs:label "Luck" ;
    rdfs:comment "Attributed to chance or serendipity" .

folk:Skill a folk:Attribution ;
    rdfs:label "Skill" ;
    rdfs:comment "Attributed to deliberate human action and capability" .

folk:Art a folk:Attribution ;
    rdfs:label "Art" ;
    rdfs:comment "Attributed to intuitive judgment and pattern recognition" .

folk:Mystery a folk:Attribution ;
    rdfs:label "Mystery" ;
    rdfs:comment "Attributed to unknown or incompletely understood phenomenon" .

folk:System a folk:Attribution ;
    rdfs:label "System" ;
    rdfs:comment "Attributed to emergent behavior of interconnected components" .

# Calculus object types
calc:Field a owl:Class ;
    rdfs:label "Scalar Field" ;
    rdfs:comment "Function f: ℝⁿ → ℝ assigning values across parameter space" .

calc:VectorField a owl:Class ;
    rdfs:label "Vector Field" ;
    rdfs:comment "Function F: ℝⁿ → ℝⁿ assigning velocity/direction at each point" .

calc:Derivative a owl:Class ;
    rdfs:label "Derivative" ;
    rdfs:comment "Rate of change df/dt or ∇f" .

calc:Integral a owl:Class ;
    rdfs:label "Integral" ;
    rdfs:comment "Accumulated quantity ∫f(t)dt" .

calc:Threshold a owl:Class ;
    rdfs:label "Threshold" ;
    rdfs:comment "Critical value where system behavior changes qualitatively" .

calc:Attractor a owl:Class ;
    rdfs:label "Attractor" ;
    rdfs:comment "Stable equilibrium state system evolves toward" .

calc:Divergence a owl:Class ;
    rdfs:label "Divergence" ;
    rdfs:comment "∇·F: rate at which field spreads or concentrates" .

calc:Gradient a owl:Class ;
    rdfs:label "Gradient" ;
    rdfs:comment "∇f: direction and magnitude of steepest ascent" .

calc:DynamicalSystem a owl:Class ;
    rdfs:label "Dynamical System" ;
    rdfs:comment "System with time-evolving state: dx/dt = f(x,t)" .

calc:StochasticProcess a owl:Class ;
    rdfs:label "Stochastic Process" ;
    rdfs:comment "Random process with probabilistic evolution" .

# Properties
folk:mapsTo a owl:ObjectProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range calc:CalculusObject ;
    rdfs:label "maps to" ;
    rdfs:comment "Connects folk term to calculus object" .

folk:hasAttribution a owl:ObjectProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range folk:Attribution ;
    rdfs:label "has attribution" ;
    rdfs:comment "Specifies what causality is attributed to" .

folk:hasObservableConsequence a owl:ObjectProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range folk:ObservableConsequence ;
    rdfs:label "has observable consequence" ;
    rdfs:comment "Links to measurable empirical outcomes" .

folk:usageExample a owl:DatatypeProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range xsd:string ;
    rdfs:label "usage example" ;
    rdfs:comment "Typical usage of folk term in entrepreneurship context" .

folk:formula a owl:DatatypeProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range xsd:string ;
    rdfs:label "formula" ;
    rdfs:comment "Mathematical expression or formula associated with term" .

folk:category a owl:ObjectProperty ;
    rdfs:domain folk:FolkTerm ;
    rdfs:range folk:Category ;
    rdfs:label "category" ;
    rdfs:comment "Categorical grouping (timing, momentum, etc.)" .

# ==============================================================================
# TIMING TERMS
# ==============================================================================

folk:TimingCategory a folk:Category ;
    rdfs:label "Timing" ;
    rdfs:comment "Terms related to temporal alignment and window" .

folk:RightTime a folk:FolkTerm ;
    rdfs:label "Right Time" ;
    rdfs:comment "Market conditions aligned with product readiness and demand" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "We shipped at the right time, market was ready for this" ;
    folk:formula "t_ship ≈ t_market_readiness ∧ t_ship ≈ t_demand_peak" ;
    folk:mapsTo calc:CoincidenceOfThresholds .

folk:TooEarly a folk:FolkTerm ;
    rdfs:label "Too Early" ;
    rdfs:comment "Shipping before market demand or technology maturity" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Mystery ;
    folk:usageExample "We were too early. Market didn't exist yet" ;
    folk:formula "t_ship < max(t_demand_threshold, t_tech_threshold)" ;
    folk:mapsTo calc:PrecursorsToThreshold .

folk:TooLate a folk:FolkTerm ;
    rdfs:label "Too Late" ;
    rdfs:comment "Shipping after competition captures market" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "By the time we launched, three competitors had 60% market share" ;
    folk:formula "t_ship > t_competitive_window_close" ;
    folk:mapsTo calc:PassedThreshold .

folk:TimeWindow a folk:FolkTerm ;
    rdfs:label "Time Window" ;
    rdfs:comment "Duration when market conditions favor launch" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "We had a narrow window before funding dried up" ;
    folk:formula "[t_start, t_end] where conditions_favorable(t) for t ∈ [t_start, t_end]" ;
    folk:mapsTo calc:IntervalOfFavorableConditions .

folk:ConeLanding a folk:FolkTerm ;
    rdfs:label "Cone Landing" ;
    rdfs:comment "Final stage of trajectory toward market acceptance" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "We managed a cone landing despite turbulent funding market" ;
    folk:formula "state(t) → attractor_state as t → ∞ within cone |state - target| < ε" ;
    folk:mapsTo calc:AttractiveCone .

folk:Momentum a folk:FolkTerm ;
    rdfs:label "Momentum" ;
    rdfs:comment "Accumulated velocity and energy in growth trajectory" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "We had momentum; each win made the next one easier" ;
    folk:formula "p = m·v where m=market_responsiveness, v=execution_velocity" ;
    folk:mapsTo calc:FirstDerivativeOfGrowth .

folk:Traction a folk:FolkTerm ;
    rdfs:label "Traction" ;
    rdfs:comment "Early adoption signal with positive feedback loop" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "Traction came when users started referring other users" ;
    folk:formula "dU/dt = α·U·(1 - U/K) + β·referrals where α,β > 0" ;
    folk:mapsTo calc:LogisticGrowthWithFeedback .

folk:Flywheel a folk:FolkTerm ;
    rdfs:label "Flywheel" ;
    rdfs:comment "Self-reinforcing loop where success enables further success" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Once we built the flywheel, growth became predictable" ;
    folk:formula "x_{n+1} = f(x_n) where f'(x*) > 1 at equilibrium x*" ;
    folk:mapsTo calc:UnstableEquilibriumWithPositiveFeedback .

folk:Velocity a folk:FolkTerm ;
    rdfs:label "Velocity" ;
    rdfs:comment "Speed of execution and iteration" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "We won on velocity; shipped 10x faster than incumbents" ;
    folk:formula "v = Δfeatures/Δtime where Δfeatures = capability_gap reduction" ;
    folk:mapsTo calc:FirstDerivativeOfCapability .

folk:Acceleration a folk:FolkTerm ;
    rdfs:label "Acceleration" ;
    rdfs:comment "Increasing rate of velocity" ;
    folk:category folk:TimingCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Acceleration kicked in when we hired the first engineer" ;
    folk:formula "a = dv/dt where dv/dt > 0" ;
    folk:mapsTo calc:SecondDerivativeOfGrowth .

# ==============================================================================
# PRODUCT-MARKET FIT TERMS
# ==============================================================================

folk:PMFCategory a folk:Category ;
    rdfs:label "Product-Market Fit" ;
    rdfs:comment "Terms describing alignment between product and market demand" .

folk:ProductMarketFit a folk:FolkTerm ;
    rdfs:label "Product-Market Fit" ;
    rdfs:comment "State where product satisfies strong market demand" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "We found PMF when retention hit 80% month-over-month" ;
    folk:formula "demand_elasticity × product_match ≥ threshold ∧ retention_rate > 70%" ;
    folk:mapsTo calc:JointThresholdCrossing .

folk:Finding a folk:FolkTerm ;
    rdfs:label "Finding" ;
    rdfs:comment "Discovery process of product-market fit" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "Finding took 18 months of iteration" ;
    folk:formula "search_space_explored(t) → (t* where PMF discovered) as t → ∞" ;
    folk:mapsTo calc:SearchTrajectoryConvergence .

folk:PMFLost a folk:FolkTerm ;
    rdfs:label "PMF Lost" ;
    rdfs:comment "Regression from PMF state due to market shift or competitive change" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "We had PMF until a competitor copied our features" ;
    folk:formula "PMF_signal(t) drops below threshold as competitor_advantage_erodes" ;
    folk:mapsTo calc:BoundaryErosion .

folk:WeakPMF a folk:FolkTerm ;
    rdfs:label "Weak PMF" ;
    rdfs:comment "Partial fit with retention problems or limited addressable market" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "Weak PMF: good feedback but churn at 40% monthly" ;
    folk:formula "retention_rate ∈ [50%, 70%) ∨ addressable_market_small" ;
    folk:mapsTo calc:TransitionRegion .

folk:StrongPMF a folk:FolkTerm ;
    rdfs:label "Strong PMF" ;
    rdfs:comment "Deep fit with high retention and organic growth" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "Strong PMF: 90%+ retention, inbound leads exceed sales capacity" ;
    folk:formula "retention_rate > 80% ∧ viral_coefficient > 1.2 ∧ churn < 5%" ;
    folk:mapsTo calc:StableAttractor .

folk:Pivot a folk:FolkTerm ;
    rdfs:label "Pivot" ;
    rdfs:comment "Strategic shift in product, market, or positioning when PMF not found" ;
    folk:category folk:PMFCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "We pivoted from B2B to B2C when we saw organic demand signal" ;
    folk:formula "switch_strategy when gradient(PMF_metric) < ε for duration > threshold" ;
    folk:mapsTo calc:ManifoldCrossing .

# ==============================================================================
# COMPETITION TERMS
# ==============================================================================

folk:CompetitionCategory a folk:Category ;
    rdfs:label "Competition" ;
    rdfs:comment "Terms describing competitive landscape and positioning" .

folk:Moat a folk:FolkTerm ;
    rdfs:label "Moat" ;
    rdfs:comment "Structural barrier protecting market position from competitors" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Network effects created a moat; every new user strengthened it" ;
    folk:formula "barrier_to_entry = min(switching_cost, replication_cost, disruption_cost)" ;
    folk:mapsTo calc:NegativeFeedbackPrevention .

folk:Defensibility a folk:FolkTerm ;
    rdfs:label "Defensibility" ;
    rdfs:comment "Ability to maintain market position against attack" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "Superior unit economics made the position defensible" ;
    folk:formula "competitive_attack_cost > expected_profit_from_displacement" ;
    folk:mapsTo calc:HighBarrierToEntry .

folk:BlueOcean a folk:FolkTerm ;
    rdfs:label "Blue Ocean" ;
    rdfs:comment "Uncontested market space with no direct competitors" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "Our niche was a blue ocean; no competitors for 2 years" ;
    folk:formula "∃ market_segment where competitors = ∅" ;
    folk:mapsTo calc:UnoccupiedFitnessLandscape .

folk:RedOcean a folk:FolkTerm ;
    rdfs:label "Red Ocean" ;
    rdfs:comment "Saturated competitive market with price pressure" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Red ocean commoditized us; margins compressed to 5%" ;
    folk:formula "competitors_dense ∧ price_elasticity_high ∧ differentiation_low" ;
    folk:mapsTo calc:HighlyDampedOscillations .

folk:Racing a folk:FolkTerm ;
    rdfs:label "Racing" ;
    rdfs:comment "Compressed timeline when multiple entrants compete for winner-take-most position" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "We were in a race; venture capital flood meant 5 clones emerged" ;
    folk:formula "Δt_to_critical_mass = O(1) not O(years)" ;
    folk:mapsTo calc:HighCurvatureTrajectory .

folk:Incumbent a folk:FolkTerm ;
    rdfs:label "Incumbent" ;
    rdfs:comment "Established player with distribution, brand, and resources" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "The incumbent had three thousand salespeople" ;
    folk:formula "resources ∝ market_share^2 ∧ switching_cost = barrier" ;
    folk:mapsTo calc:LocalMaximumWithStrongAttraction .

folk:Disruptor a folk:FolkTerm ;
    rdfs:label "Disruptor" ;
    rdfs:comment "Entrant challenging incumbent via different value proposition or cost structure" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "We disrupted the market by 10x-ing cost efficiency" ;
    folk:formula "value_per_dollar_new ≥ 3 × value_per_dollar_incumbent" ;
    folk:mapsTo calc:HigherFitnessPeakOnNeighboringManifold .

folk:InnovatorsIlemma a folk:FolkTerm ;
    rdfs:label "Innovator's Dilemma" ;
    rdfs:comment "Incumbent's inability to respond to disruption due to business model commitment" ;
    folk:category folk:CompetitionCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "They couldn't respond because disruption cannibalized their core business" ;
    folk:formula "margin_defender > margin_disruptor ⟹ incumbent_stuck_at_local_maximum" ;
    folk:mapsTo calc:AsymmetricPotentialBarrier .

# ==============================================================================
# GROWTH TERMS
# ==============================================================================

folk:GrowthCategory a folk:Category ;
    rdfs:label "Growth" ;
    rdfs:comment "Terms describing user/revenue growth patterns" .

folk:Viral a folk:FolkTerm ;
    rdfs:label "Viral" ;
    rdfs:comment "User acquisition primarily through user referrals creating exponential growth" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "The product went viral; we didn't spend marketing budget" ;
    folk:formula "U(t) ∝ e^(k·t) where k = viral_coefficient - 1" ;
    folk:mapsTo calc:ExponentialGrowth .

folk:Organic a folk:FolkTerm ;
    rdfs:label "Organic" ;
    rdfs:comment "User acquisition through word-of-mouth and inbound interest" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "Organic growth was slow but sustainable" ;
    folk:formula "dU/dt = α·satisfaction_score where 0 < α < 1" ;
    folk:mapsTo calc:SlowlyConvergingIntegral .

folk:HockeyStick a folk:FolkTerm ;
    rdfs:label "Hockey Stick" ;
    rdfs:comment "Rapid acceleration from slow linear growth to exponential" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:Mystery ;
    folk:usageExample "Nothing happened for a year, then hockey stick growth overnight" ;
    folk:formula "d²U/dt² = 0 for t < t*, d²U/dt² > 0 for t > t*" ;
    folk:mapsTo calc:InflectionPoint .

folk:SCurve a folk:FolkTerm ;
    rdfs:label "S-Curve" ;
    rdfs:comment "Logistic growth with saturation at market limit" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Growth followed classic S-curve, plateaued at market size" ;
    folk:formula "dU/dt = r·U·(1 - U/K)" ;
    folk:mapsTo calc:LogisticGrowth .

folk:Plateau a folk:FolkTerm ;
    rdfs:label "Plateau" ;
    rdfs:comment "Growth rate slows to near-zero as market saturation approaches" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Growth plateaued at 40% penetration" ;
    folk:formula "dU/dt → 0 as U → K" ;
    folk:mapsTo calc:SaturationBehavior .

folk:Ceiling a folk:FolkTerm ;
    rdfs:label "Ceiling" ;
    rdfs:comment "Maximum addressable market size" ;
    folk:category folk:GrowthCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "We hit the ceiling; TAM was only $100M" ;
    folk:formula "U_max = TAM × market_penetration_achievable" ;
    folk:mapsTo calc:CarryingCapacity .

# ==============================================================================
# NETWORK EFFECTS TERMS
# ==============================================================================

folk:NetworkCategory a folk:Category ;
    rdfs:label "Network Effects" ;
    rdfs:comment "Terms describing value creation through connectivity" .

folk:CriticalMass a folk:FolkTerm ;
    rdfs:label "Critical Mass" ;
    rdfs:comment "User count threshold where network effects accelerate adoption" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:Mystery ;
    folk:usageExample "We hit critical mass at 50k users; growth then became self-sustaining" ;
    folk:formula "∃ U* where ∂²U/∂t² > 0 for U > U*" ;
    folk:mapsTo calc:TippingPoint .

folk:ChickenEgg a folk:FolkTerm ;
    rdfs:label "Chicken-Egg Problem" ;
    rdfs:comment "Circular dependency where two-sided market needs both sides simultaneously" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:Art ;
    folk:usageExample "Chicken-egg was brutal; sellers wouldn't join without buyers" ;
    folk:formula "∃ circular_dependency ⟹ requires_subsidy_or_unilateral_bootstrapping" ;
    folk:mapsTo calc:UnstableSaddlePoint .

folk:Liquidity a folk:FolkTerm ;
    rdfs:label "Liquidity" ;
    rdfs:comment "Ease of matching between supply and demand in two-sided market" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Liquidity on the platform determines unit economics" ;
    folk:formula "match_rate = P(∃ buyer | seller_active) × P(∃ seller | buyer_active)" ;
    folk:mapsTo calc:CoupledProbabilityInequality .

folk:Platform a folk:FolkTerm ;
    rdfs:label "Platform" ;
    rdfs:comment "Ecosystem where value created through interaction between multiple parties" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "We built a platform; third-party developers created value for users" ;
    folk:formula "value_total = Σ(transaction_value × match_rate)" ;
    folk:mapsTo calc:MultiPartiteGraph .

folk:Ecosystem a folk:FolkTerm ;
    rdfs:label "Ecosystem" ;
    rdfs:comment "Community of complementary products and services creating increasing value" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Once the ecosystem developed, switching cost became prohibitive" ;
    folk:formula "barrier = Σ(integration_depth_i) for all complementary_services_i" ;
    folk:mapsTo calc:ComplementaryAttraction .

folk:LockIn a folk:FolkTerm ;
    rdfs:label "Lock-In" ;
    rdfs:comment "High switching cost preventing customer defection" ;
    folk:category folk:NetworkCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Lock-in was network effects: customer's entire network on our platform" ;
    folk:formula "switching_cost = cost_of_recreating_network + data_migration_loss" ;
    folk:mapsTo calc:DeepPotentialWell .

# ==============================================================================
# FAILURE TERMS
# ==============================================================================

folk:FailureCategory a folk:Category ;
    rdfs:label "Failure Modes" ;
    rdfs:comment "Terms describing failure patterns and demise" .

folk:Runway a folk:FolkTerm ;
    rdfs:label "Runway" ;
    rdfs:comment "Time until capital exhaustion if burn rate continues" ;
    folk:category folk:FailureCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "We had 6 months of runway left" ;
    folk:formula "runway_months = cash_remaining / monthly_burn_rate" ;
    folk:mapsTo calc:LinearDecay .

folk:CouldntFindPMF a folk:FolkTerm ;
    rdfs:label "Couldn't Find PMF" ;
    rdfs:comment "Failed to achieve product-market fit before capital depletion" ;
    folk:category folk:FailureCategory ;
    folk:hasAttribution folk:Mystery ;
    folk:usageExample "We ran out of time before PMF" ;
    folk:formula "∄ t* where PMF_signal(t*) > threshold ∧ t* < t_bankruptcy" ;
    folk:mapsTo calc:UnboundedSearchSpaceWithBoundedResource .

folk:Disrupted a folk:FolkTerm ;
    rdfs:label "Disrupted" ;
    rdfs:comment "Overwhelmed by competitive innovation rendering product obsolete" ;
    folk:category folk:FailureCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "We were disrupted when their AI did what used to take humans" ;
    folk:formula "product_value_incumbent >> product_value_ours after competitor_breakthrough" ;
    folk:mapsTo calc:RapidLandscapeShift .

folk:BurningPlatform a folk:FolkTerm ;
    rdfs:label "Burning Platform" ;
    rdfs:comment "Crisis forcing urgent action or radical change" ;
    folk:category folk:FailureCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "The burning platform forced us to rebuild architecture in 3 weeks" ;
    folk:formula "crisis_urgency > adaptation_cost ⟹ radical_action_possible" ;
    folk:mapsTo calc:PerturbationPastLocalMaximum .

folk:Zombie a folk:FolkTerm ;
    rdfs:label "Zombie" ;
    rdfs:comment "Company with positive unit economics but insufficient growth to justify funding" ;
    folk:category folk:FailureCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "We became a zombie: profitable but growth capped at 10% annually" ;
    folk:formula "unit_economics > 0 ∧ growth_rate < opportunity_cost_of_capital" ;
    folk:mapsTo calc:LocalEquilibriumWithRespectToOppgCost .

# ==============================================================================
# LUCK AND SERENDIPITY TERMS
# ==============================================================================

folk:LuckCategory a folk:Category ;
    rdfs:label "Luck and Serendipity" ;
    rdfs:comment "Terms describing fortune and chance encounters" .

folk:Luck a folk:FolkTerm ;
    rdfs:label "Luck (Event)" ;
    rdfs:comment "Chance occurrence that creates advantage" ;
    folk:category folk:LuckCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "Lucky meeting with investor led to Series A" ;
    folk:formula "random_event_benefit(t) > expected_value_without_event" ;
    folk:mapsTo calc:UnpredictableJump .

folk:Serendipity a folk:FolkTerm ;
    rdfs:label "Serendipity" ;
    rdfs:comment "Happy accident combining preparation with chance" ;
    folk:category folk:LuckCategory ;
    folk:hasAttribution folk:System ;
    folk:usageExample "Serendipity: stumbled on use case while demoing to someone else" ;
    folk:formula "preparation_level × random_encounter = breakthrough" ;
    folk:mapsTo calc:FilteredRandomProcess .

folk:Connections a folk:FolkTerm ;
    rdfs:label "Connections" ;
    rdfs:comment "Network of relationships providing leverage and opportunity" ;
    folk:category folk:LuckCategory ;
    folk:hasAttribution folk:Skill ;
    folk:usageExample "Connections gave us early beta users we never paid for" ;
    folk:formula "opportunity_access ∝ connection_network_density × connection_quality" ;
    folk:mapsTo calc:NetworkCentralityMeasure .

folk:OvernightSuccess a folk:FolkTerm ;
    rdfs:label "Overnight Success" ;
    rdfs:comment "Rapid visibility following years of invisible development" ;
    folk:category folk:LuckCategory ;
    folk:hasAttribution folk:Mystery ;
    folk:usageExample "Overnight success after 7 years of work nobody noticed" ;
    folk:formula "visibility(t) = step_function(t - t*) where t* >> 0" ;
    folk:mapsTo calc:SharpDynamicsChange .

folk:Unlucky a folk:FolkTerm ;
    rdfs:label "Unlucky" ;
    rdfs:comment "Chance misfortune reducing odds of success" ;
    folk:category folk:LuckCategory ;
    folk:hasAttribution folk:Luck ;
    folk:usageExample "Unlucky: launched during 2008 financial crisis" ;
    folk:formula "random_event_harm(t) < 0 ∧ |harm| > recovery_capacity" ;
    folk:mapsTo calc:UnpredictablePerturbationAwayFromAttractor .

# ==============================================================================
# STRATEGY AND EXECUTION TERMS
# ==============================================================================

folk:StrategyCategory a folk:Category ;
    rdfs:label "Strategy and Execution" ;
    rdfs:comment "Terms describing planning, vision, and implementation" .

folk:Vision a folk:FolkTerm ;
