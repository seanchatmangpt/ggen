  rdfs:range [ a owl:Class ; owl:unionOf (alpha:Agent1 alpha:Agent2 alpha:Agent3 alpha:Agent4 alpha:Agent5 alpha:Agent6 alpha:Agent7 alpha:Agent8 alpha:Agent9 alpha:Agent10) ] ;
