@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix alpha: <http://ggen.io/alpha#> .

# ============================================================================
# UNIFIED SIGNAL ONTOLOGY
# ============================================================================
# Core base class for all alpha signals, providing common properties for
# financial market intelligence synthesis across 10 parallel agent streams.
# ============================================================================

alpha:Signal
  a owl:Class ;
  rdfs:label "Unified Alpha Signal" ;
  rdfs:comment "Abstract base class for all alpha-generating signals synthesized from diverse data sources. Provides common properties: timestamp (when observed), confidence (0.0-1.0 probability), tier (data quality level), source agent (originating agent ID), and validity windows." ;
  owl:equivalentClass [ a owl:Class ; owl:unionOf (alpha:SentimentSignal alpha:RegulatorySignal alpha:SupplyChainSignal alpha:SectorMomentumSignal alpha:BasisSignal alpha:CreditStressSignal alpha:RealEstateSignal alpha:InnovationSignal alpha:GeopoliticalRiskSignal) ] .

alpha:timestamp
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Signal Timestamp" ;
  rdfs:comment "Exact date and time when signal was observed or published, in UTC." .

alpha:confidence
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Confidence Score" ;
  rdfs:comment "Probability 0.0-1.0 representing data quality and assertion confidence. Higher values indicate more reliable signals." .

alpha:signalType
  a owl:ObjectProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range [ a owl:Class ; owl:unionOf (alpha:SentimentSignal alpha:RegulatorySignal alpha:SupplyChainSignal alpha:SectorMomentumSignal alpha:BasisSignal alpha:CreditStressSignal alpha:RealEstateSignal alpha:InnovationSignal alpha:GeopoliticalRiskSignal) ] ;
  rdfs:label "Signal Type" ;
  rdfs:comment "Semantic classification of signal (sentiment, regulatory, supply chain, momentum, basis, credit, real estate, innovation, or geopolitical)." .

alpha:dataSourceAgent
  a owl:ObjectProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range [ a owl:Class ; owl:unionOf (alpha:Agent1 alpha:Agent2 alpha:Agent3 alpha:Agent4 alpha:Agent5 alpha:Agent6 alpha:Agent7 alpha:Agent8 alpha:Agent9 alpha:Agent10) ] ;
  rdfs:label "Originating Agent" ;
  rdfs:comment "Which of the 10 parallel agents produced this signal." .

alpha:confidenceTier
  a owl:ObjectProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range [ a owl:Class ; owl:unionOf (alpha:TIER_A alpha:TIER_B alpha:TIER_C) ] ;
  rdfs:label "Data Quality Tier" ;
  rdfs:comment "Classification of data source quality (TIER_A: high-quality curated, TIER_B: medium-quality diverse, TIER_C: low-quality sparse)." .

alpha:validFrom
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Validity Start" ;
  rdfs:comment "Earliest date/time for which this signal is valid. Defaults to timestamp." .

alpha:validUntil
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Signal ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Validity End" ;
  rdfs:comment "Latest date/time for which this signal is valid. Indicates expiration or refresh cycle." .

# ============================================================================
# CONFIDENCE TIER SYSTEM
# ============================================================================
# Three-tier classification system for data source quality, determining
# aggregation weights and signal priority in synthesis.
# ============================================================================

alpha:ConfidenceTier
  a owl:Class ;
  rdfs:label "Confidence Tier Definition" ;
  rdfs:comment "Metadata tier specifying data source quality, applicable agents, confidence ranges, and aggregation weights." .

alpha:minConfidence
  a owl:DatatypeProperty ;
  rdfs:domain alpha:ConfidenceTier ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Minimum Confidence" ;
  rdfs:comment "Lower bound of confidence scores typical for this tier." .

alpha:maxConfidence
  a owl:DatatypeProperty ;
  rdfs:domain alpha:ConfidenceTier ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Maximum Confidence" ;
  rdfs:comment "Upper bound of confidence scores typical for this tier." .

alpha:weight
  a owl:DatatypeProperty ;
  rdfs:domain alpha:ConfidenceTier ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] ) ] ;
  rdfs:label "Aggregation Weight" ;
  rdfs:comment "Relative weight in composite scoring when merging signals from multiple tiers. TIER_A=2.0, TIER_B=1.0, TIER_C=0.7." .

alpha:agents
  a owl:ObjectProperty ;
  rdfs:domain alpha:ConfidenceTier ;
  rdfs:range [ a owl:Class ; owl:unionOf (alpha:Agent1 alpha:Agent2 alpha:Agent3 alpha:Agent4 alpha:Agent5 alpha:Agent6 alpha:Agent7 alpha:Agent8 alpha:Agent9 alpha:Agent10) ] ;
  rdfs:label "Member Agents" ;
  rdfs:comment "Which agents produce signals in this tier." .

# Tier A: High-Quality Financial Data
alpha:TIER_A
  a alpha:ConfidenceTier ;
  rdfs:label "TIER_A: High-Quality Financial Data" ;
  rdfs:comment "Standardized, curated data sources with strong institutional backing (Bloomberg, SEC EDGAR, Reuters, CME). Used for primary trading signals." ;
  alpha:minConfidence "0.50"^^xsd:decimal ;
  alpha:maxConfidence "0.95"^^xsd:decimal ;
  alpha:weight "2.0"^^xsd:decimal ;
  alpha:agents alpha:Agent1, alpha:Agent2, alpha:Agent6, alpha:Agent7 .

# Tier B: Medium-Quality Data
alpha:TIER_B
  a alpha:ConfidenceTier ;
  rdfs:label "TIER_B: Medium-Quality Data" ;
  rdfs:comment "Diverse sources with moderate aggregation/preprocessing (social media, supply chain indices, sector momentum). Useful for context and confirmation." ;
  alpha:minConfidence "0.35"^^xsd:decimal ;
  alpha:maxConfidence "0.90"^^xsd:decimal ;
  alpha:weight "1.0"^^xsd:decimal ;
  alpha:agents alpha:Agent3, alpha:Agent4, alpha:Agent5, alpha:Agent9, alpha:Agent10 .

# Tier C: Lower-Quality Data
alpha:TIER_C
  a alpha:ConfidenceTier ;
  rdfs:label "TIER_C: Lower-Quality Data" ;
  rdfs:comment "Sparse, infrequent updates with longer observation cycles (real estate quarterly, innovation semi-annual). Low priority in real-time synthesis." ;
  alpha:minConfidence "0.45"^^xsd:decimal ;
  alpha:maxConfidence "0.82"^^xsd:decimal ;
  alpha:weight "0.7"^^xsd:decimal ;
  alpha:agents alpha:Agent8 .

# ============================================================================
# AGENT SPECIFICATIONS
# ============================================================================
# Formal specification of all 10 parallel agents with CONSTRUCT queries,
# data sources, confidence ranges, frequencies, and uniqueness constraints.
# ============================================================================

alpha:Agent
  a owl:Class ;
  rdfs:label "LLM Agent" ;
  rdfs:comment "One of 10 parallel agents responsible for observing domain signals and producing alpha-generating constructs." .

alpha:agentId
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range xsd:string ;
  rdfs:label "Agent ID" ;
  rdfs:comment "Unique identifier (Agent1-Agent10)." .

alpha:constructName
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range xsd:string ;
  rdfs:label "CONSTRUCT Name" ;
  rdfs:comment "Name of RDF construct produced by this agent." .

alpha:dataSourceList
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range rdf:List ;
  rdfs:label "Data Sources" ;
  rdfs:comment "List of external data sources consumed by agent." .

alpha:frequency
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range xsd:duration ;
  rdfs:label "Update Frequency" ;
  rdfs:comment "Typical update cycle (e.g., PT6H for 6 hours)." .

alpha:minConfidenceAgent
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Minimum Confidence" ;
  rdfs:comment "Lower bound of confidence range for this agent." .

alpha:maxConfidenceAgent
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Maximum Confidence" ;
  rdfs:comment "Upper bound of confidence range for this agent." .

alpha:typicalConfidenceAgent
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Typical Confidence" ;
  rdfs:comment "Most common/expected confidence score for this agent." .

alpha:uniquenessConstraint
  a owl:DatatypeProperty ;
  rdfs:domain alpha:Agent ;
  rdfs:range xsd:string ;
  rdfs:label "Uniqueness Constraint" ;
  rdfs:comment "Describes what combinations of fields must be unique per observation window." .

# Agent 1: Financial News Sentiment
alpha:Agent1
  a alpha:Agent ;
  rdfs:label "Agent 1: Financial News Sentiment" ;
  alpha:agentId "Agent1" ;
  alpha:constructName "financial_sentiment" ;
  alpha:dataSourceList (
    "Bloomberg"
    "Reuters"
    "MarketWatch"
    "Earnings Calls"
  ) ;
  alpha:frequency "PT6H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.40"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.95"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.65"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_A ;
  alpha:uniquenessConstraint "1 per company per day" ;
  rdfs:comment "Analyzes tone, sentiment score, confidence level, publication source, and timestamp from financial news sources. Produces SentimentSignal with tone property." .

# Agent 2: Regulatory Filing Patterns
alpha:Agent2
  a alpha:Agent ;
  rdfs:label "Agent 2: Regulatory Filing Patterns" ;
  alpha:agentId "Agent2" ;
  alpha:constructName "regulatory_event" ;
  alpha:dataSourceList (
    "SEC EDGAR"
    "FINRA"
    "State Insurance"
  ) ;
  alpha:frequency "PT24H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.50"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.95"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.70"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_A ;
  alpha:uniquenessConstraint "1 per company per day per filing_type" ;
  rdfs:comment "Detects regulatory filings with anomaly scoring and significance assessment. Produces RegulatorySignal." .

# Agent 3: Social Sentiment
alpha:Agent3
  a alpha:Agent ;
  rdfs:label "Agent 3: Social Sentiment" ;
  alpha:agentId "Agent3" ;
  alpha:constructName "social_sentiment" ;
  alpha:dataSourceList (
    "Twitter"
    "Reddit"
    "StockTwits"
    "YouTube"
    "TikTok"
  ) ;
  alpha:frequency "PT1H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.35"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.85"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.55"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_B ;
  alpha:uniquenessConstraint "aggregate to daily buckets, engagement >= 10, bot_probability <= 0.3" ;
  rdfs:comment "Aggregates social media sentiment (-1.0 to +1.0, normalized to 0.0-1.0), platform, engagement metrics, bot probability. Produces SentimentSignal." .

# Agent 4: Supply Chain Constraints
alpha:Agent4
  a alpha:Agent ;
  rdfs:label "Agent 4: Supply Chain Constraints" ;
  alpha:agentId "Agent4" ;
  alpha:constructName "supply_chain_signal" ;
  alpha:dataSourceList (
    "Freightos"
    "IHS Markit"
    "World Bank"
    "Port Authorities"
  ) ;
  alpha:frequency "PT12H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.45"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.90"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.65"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_B ;
  alpha:uniquenessConstraint "delay_hours >= 24, data freshness <= 24h" ;
  rdfs:comment "Measures supply chain delays (hours), cost inflation percentages, disruption indices. Produces SupplyChainSignal." .

# Agent 5: Sector Momentum
alpha:Agent5
  a alpha:Agent ;
  rdfs:label "Agent 5: Sector Momentum" ;
  alpha:agentId "Agent5" ;
  alpha:constructName "sector_momentum" ;
  alpha:dataSourceList (
    "IEX Cloud"
    "Barchart"
    "OptionChain"
  ) ;
  alpha:frequency "PT1H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.50"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.88"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.68"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_B ;
  alpha:uniquenessConstraint "aggregate to daily, vol_percentile >= 0.65 for high-conviction" ;
  rdfs:comment "Tracks momentum score (-1.0 to +1.0, normalized to 0.0-1.0), flow direction, volume percentiles. Produces SectorMomentumSignal." .

# Agent 6: Currency & Commodity Basis
alpha:Agent6
  a alpha:Agent ;
  rdfs:label "Agent 6: Currency & Commodity Basis" ;
  alpha:agentId "Agent6" ;
  alpha:constructName "basis_spread" ;
  alpha:dataSourceList (
    "Bloomberg"
    "CME FuturesOnline"
    "FRED"
  ) ;
  alpha:frequency "PT4H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.55"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.92"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.72"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_A ;
  alpha:uniquenessConstraint "basis_points >= 0, carry_bps magnitude >= 10" ;
  rdfs:comment "Monitors basis points spread, carry basis points, term premium for currency/commodity markets. Produces BasisSignal." .

# Agent 7: Credit Stress Indicators
alpha:Agent7
  a alpha:Agent ;
  rdfs:label "Agent 7: Credit Stress Indicators" ;
  alpha:agentId "Agent7" ;
  alpha:constructName "credit_stress" ;
  alpha:dataSourceList (
    "ICE Data Indices"
    "Thomson Reuters"
    "MarketAxess"
    "S&P Global"
  ) ;
  alpha:frequency "PT2H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.60"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.94"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.76"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_A ;
  alpha:uniquenessConstraint "spread_bps >= 0, spread_percentile >= 0.75 for stress signal" ;
  rdfs:comment "Measures credit spreads (basis points), relative spread percentile, default probability. Produces CreditStressSignal." .

# Agent 8: Real Estate & Property Signals
alpha:Agent8
  a alpha:Agent ;
  rdfs:label "Agent 8: Real Estate & Property Signals" ;
  alpha:agentId "Agent8" ;
  alpha:constructName "real_estate_signal" ;
  alpha:dataSourceList (
    "US Census"
    "Zillow"
    "CoStar"
    "BLS"
  ) ;
  alpha:frequency "PT720H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.45"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.82"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.60"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_C ;
  alpha:uniquenessConstraint "price_yoy_change >= 2%, permit_count >= 0, vacancy_rate in [0.0, 1.0]" ;
  rdfs:comment "Quarterly real estate data: year-over-year price changes (%), permit counts, vacancy rates. Produces RealEstateSignal with monthly validity windows." .

# Agent 9: Technology & Innovation Signals
alpha:Agent9
  a alpha:Agent ;
  rdfs:label "Agent 9: Technology & Innovation Signals" ;
  alpha:agentId "Agent9" ;
  alpha:constructName "innovation_signal" ;
  alpha:dataSourceList (
    "USPTO"
    "GitHub"
    "Crunchbase"
    "npm Registry"
    "ArXiv"
  ) ;
  alpha:frequency "PT168H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.50"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.87"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.65"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_B ;
  alpha:uniquenessConstraint "github_stars_yoy >= 100, disruption_risk in [0.0, 1.0]" ;
  rdfs:comment "Weekly technology signals: patent velocity, GitHub stars year-over-year growth, disruption risk assessment. Produces InnovationSignal." .

# Agent 10: Geopolitical & Macro Risk
alpha:Agent10
  a alpha:Agent ;
  rdfs:label "Agent 10: Geopolitical & Macro Risk" ;
  alpha:agentId "Agent10" ;
  alpha:constructName "geopolitical_risk" ;
  alpha:dataSourceList (
    "ACLED"
    "GDELT"
    "OFAC"
    "Federal Reserve EPU"
    "World Bank Trade"
  ) ;
  alpha:frequency "PT6H"^^xsd:duration ;
  alpha:minConfidenceAgent "0.55"^^xsd:decimal ;
  alpha:maxConfidenceAgent "0.89"^^xsd:decimal ;
  alpha:typicalConfidenceAgent "0.70"^^xsd:decimal ;
  alpha:confidenceTier alpha:TIER_B ;
  alpha:uniquenessConstraint "event_type in (conflict|trade|sanctions|policy), risk_level in [0.0, 1.0], recency < 7 days" ;
  rdfs:comment "Monitors geopolitical events with risk levels, affected regions/sectors. Produces GeopoliticalRiskSignal." .

# ============================================================================
# SIGNAL TYPE HIERARCHY
# ============================================================================
# Semantic subclasses of alpha:Signal, one per agent domain.
# Each preserves domain-specific properties while inheriting base signal behavior.
# ============================================================================

alpha:SentimentSignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Sentiment Signal" ;
  rdfs:comment "Sentiment from financial news (Agent 1) or social media (Agent 3). Properties: tone, source publication, article text." .

alpha:tone
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SentimentSignal ;
  rdfs:range [ a owl:Class ; owl:oneOf ("bullish" "neutral" "bearish") ] ;
  rdfs:label "Sentiment Tone" ;
  rdfs:comment "Qualitative tone: bullish, neutral, or bearish." .

alpha:sentimentScore
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SentimentSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Sentiment Score" ;
  rdfs:comment "Normalized sentiment score 0.0-1.0 (0=most bearish, 1=most bullish)." .

alpha:sourcePublication
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SentimentSignal ;
  rdfs:range xsd:string ;
  rdfs:label "Source Publication" ;
  rdfs:comment "Where sentiment was published (Bloomberg, Reuters, Twitter, Reddit, etc.)." .

alpha:RegulatorySignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Regulatory Signal" ;
  rdfs:comment "Regulatory filings and anomalies detected from SEC, FINRA, state insurance. Properties: filing type, anomaly score, significance." .

alpha:filingType
  a owl:DatatypeProperty ;
  rdfs:domain alpha:RegulatorySignal ;
  rdfs:range xsd:string ;
  rdfs:label "Filing Type" ;
  rdfs:comment "Type of regulatory filing (10-K, 8-K, proxy, etc.)." .

alpha:anomalyScore
  a owl:DatatypeProperty ;
  rdfs:domain alpha:RegulatorySignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Anomaly Score" ;
  rdfs:comment "How unusual this filing is compared to historical norms (0.0=normal, 1.0=highly anomalous)." .

alpha:significance
  a owl:DatatypeProperty ;
  rdfs:domain alpha:RegulatorySignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Significance" ;
  rdfs:comment "Assessed impact/importance of this regulatory event (0.0=minor, 1.0=transformational)." .

alpha:SupplyChainSignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Supply Chain Signal" ;
  rdfs:comment "Supply chain constraints from freight indices, port authorities. Properties: delay hours, cost inflation, disruption index." .

alpha:delayHours
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SupplyChainSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:integer ; owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ;
  rdfs:label "Delay Hours" ;
  rdfs:comment "Estimated delay in supply chain fulfillment (hours)." .

alpha:costInflation
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SupplyChainSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] ) ] ;
  rdfs:label "Cost Inflation %" ;
  rdfs:comment "Percentage increase in supply chain costs." .

alpha:disruptionIndex
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SupplyChainSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Disruption Index" ;
  rdfs:comment "Overall supply chain disruption level (0.0=stable, 1.0=severe chaos)." .

alpha:SectorMomentumSignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Sector Momentum Signal" ;
  rdfs:comment "Sector momentum and flow indicators. Properties: momentum score (unipolar 0.0-1.0), flow direction, volume percentile." .

alpha:momentumScore
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SectorMomentumSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Momentum Score (Unipolar)" ;
  rdfs:comment "Normalized momentum 0.0-1.0 (0=most bearish, 1=most bullish), converted from agent bipolar (-1.0 to +1.0) via formula: (bipolar + 1.0) / 2.0." .

alpha:flowDirection
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SectorMomentumSignal ;
  rdfs:range [ a owl:Class ; owl:oneOf ("bullish_flow" "neutral_flow" "bearish_flow") ] ;
  rdfs:label "Flow Direction" ;
  rdfs:comment "Direction of capital flow (bullish_flow, neutral_flow, bearish_flow)." .

alpha:volumePercentile
  a owl:DatatypeProperty ;
  rdfs:domain alpha:SectorMomentumSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ; owl:withRestrictions ( [ xsd:minInclusive 0.0 ] [ xsd:maxInclusive 1.0 ] ) ] ;
  rdfs:label "Volume Percentile" ;
  rdfs:comment "Relative volume compared to historical distribution (0.0=lowest decile, 1.0=highest decile). High-conviction signals require >= 0.65." .

alpha:BasisSignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Basis Signal" ;
  rdfs:comment "Currency and commodity basis signals from Bloomberg, CME. Properties: basis points, carry basis points, term premium." .

alpha:basisPoints
  a owl:DatatypeProperty ;
  rdfs:domain alpha:BasisSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:integer ; owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ;
  rdfs:label "Basis Points Spread" ;
  rdfs:comment "Spread basis points (absolute, non-negative)." .

alpha:carryBasisPoints
  a owl:DatatypeProperty ;
  rdfs:domain alpha:BasisSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:integer ] ;
  rdfs:label "Carry Basis Points" ;
  rdfs:comment "Carry cost in basis points (can be positive or negative)." .

alpha:termPremium
  a owl:DatatypeProperty ;
  rdfs:domain alpha:BasisSignal ;
  rdfs:range [ a rdfs:Datatype ; owl:onDatatype xsd:decimal ] ;
  rdfs:label "Term Premium" ;
  rdfs:comment "Term structure premium expressed as percentage or basis points." .

alpha:CreditStressSignal
  a owl:Class ;
  rdfs:subClassOf alpha:Signal ;
  rdfs:label "Credit Stress Signal" ;
  rdfs:comment "Credit market stress indicators. Properties: spread basis points, spread percentile, default probability." .

alpha:spreadBasisPoints
  a owl:DatatypeProperty ;
