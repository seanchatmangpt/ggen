@prefix cli: <http://ggen.org/cli#> .
@prefix paas: <http://ggen.org/paas#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# ============================================================================
# CLASS DEFINITIONS - CLI Ontology Schema
# ============================================================================
# Core semantic classes that define CLI structure and validation

cli:Command
  a owl:Class ;
  rdfs:label "CLI Command" ;
  rdfs:comment "A command that can be executed via the CLI interface" ;
  .

cli:Option
  a owl:Class ;
  rdfs:label "CLI Option" ;
  rdfs:comment "A command-line option (flag) with optional values" ;
  .

cli:PositionalArgument
  a owl:Class ;
  rdfs:label "Positional Argument" ;
  rdfs:comment "A positional argument to a command" ;
  .

cli:SLO
  a owl:Class ;
  rdfs:label "Service Level Objective" ;
  rdfs:comment "Performance target for a command execution" ;
  .

cli:Category
  a owl:Class ;
  rdfs:label "Command Category" ;
  rdfs:comment "Logical grouping of related commands" ;
  .

cli:GlobalOption
  a owl:Class ;
  rdfs:label "Global Option" ;
  rdfs:comment "Option available to all commands" ;
  .

cli:Ontology
  a owl:Class ;
  rdfs:label "CLI Ontology" ;
  rdfs:comment "The CLI specification ontology itself" ;
  .

# ============================================================================
# PROPERTY DEFINITIONS - CLI Ontology Properties
# ============================================================================
# Properties with explicit domain/range for type safety

cli:handler
  a owl:DatatypeProperty ;
  rdfs:label "handler" ;
  rdfs:domain cli:Command ;
  rdfs:range xsd:string ;
  rdfs:comment "Path to handler implementation file" ;
  .

cli:test
  a owl:DatatypeProperty ;
  rdfs:label "test" ;
  rdfs:domain cli:Command ;
  rdfs:range xsd:string ;
  rdfs:comment "Path to test file for command" ;
  .

cli:category
  a owl:ObjectProperty ;
  rdfs:label "category" ;
  rdfs:domain cli:Command ;
  rdfs:range cli:Category ;
  rdfs:comment "Category this command belongs to" ;
  .

cli:slo
  a owl:ObjectProperty ;
  rdfs:label "slo" ;
  rdfs:domain cli:Command ;
  rdfs:range cli:SLO ;
  rdfs:comment "SLO constraints for command execution" ;
  .

cli:type
  a owl:DatatypeProperty ;
  rdfs:label "type" ;
  rdfs:domain [ owl:unionOf (cli:Option cli:PositionalArgument) ] ;
  rdfs:range xsd:string ;
  rdfs:comment "Data type of the option or argument" ;
  .

cli:longForm
  a owl:DatatypeProperty ;
  rdfs:label "longForm" ;
  rdfs:domain [ owl:unionOf (cli:Option cli:GlobalOption) ] ;
  rdfs:range xsd:string ;
  rdfs:comment "Long form flag (e.g., --verbose)" ;
  .

cli:shortForm
  a owl:DatatypeProperty ;
  rdfs:label "shortForm" ;
  rdfs:domain [ owl:unionOf (cli:Option cli:GlobalOption) ] ;
  rdfs:range xsd:string ;
  rdfs:comment "Short form flag (e.g., -v)" ;
  .

cli:maxExecutionTimeMs
  a owl:DatatypeProperty ;
  rdfs:label "maxExecutionTimeMs" ;
  rdfs:domain cli:SLO ;
  rdfs:range xsd:integer ;
  rdfs:comment "Maximum execution time in milliseconds" ;
  .

cli:position
  a owl:DatatypeProperty ;
  rdfs:label "position" ;
  rdfs:domain cli:PositionalArgument ;
  rdfs:range xsd:integer ;
  rdfs:comment "Position of argument in command line" ;
  .

cli:required
  a owl:DatatypeProperty ;
  rdfs:label "required" ;
  rdfs:domain [ owl:unionOf (cli:Option cli:PositionalArgument) ] ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Whether this parameter is required" ;
  .

cli:option
  a owl:ObjectProperty ;
  rdfs:label "option" ;
  rdfs:domain cli:Command ;
  rdfs:range cli:Option ;
  rdfs:comment "Options available for this command" ;
  .

cli:argument
  a owl:ObjectProperty ;
  rdfs:label "argument" ;
  rdfs:domain cli:Command ;
  rdfs:range cli:PositionalArgument ;
  rdfs:comment "Positional arguments for this command" ;
  .

cli:appliesToCommand
  a owl:ObjectProperty ;
  rdfs:label "appliesToCommand" ;
  rdfs:domain cli:GlobalOption ;
  rdfs:range cli:Command ;
  rdfs:comment "Commands this global option applies to" ;
  .

cli:aliases
  a owl:DatatypeProperty ;
  rdfs:label "aliases" ;
  rdfs:domain cli:Command ;
  rdfs:range xsd:string ;
  rdfs:comment "Comma-separated aliases for command" ;
  .

cli:version
  a owl:DatatypeProperty ;
  rdfs:label "version" ;
  rdfs:domain cli:Command ;
  rdfs:range xsd:string ;
  rdfs:comment "Version of the command specification" ;
  .

cli:examples
  a owl:DatatypeProperty ;
  rdfs:label "examples" ;
  rdfs:domain cli:Command ;
  rdfs:range xsd:string ;
  rdfs:comment "Example command usage" ;
  .

# ============================================================================
# CLI Ontology: Command Definitions for ggen-paas CLI
# ============================================================================
# Purpose: Define all ggen paas commands in RDF format
# Each command is a specification-driven entity that will be:
#   1. Validated via SHACL schema (.specify/cli-schema.ttl)
#   2. Used to generate command handlers (lib/commands/<command>.js)
#   3. Used to generate help text (auto-generated docs)
#   4. Used to generate tests (tests/commands/<command>.test.js)
# ============================================================================

# ============================================================================
# COMMAND: generate
# Purpose: Generate infrastructure artifacts from RDF specifications
# ============================================================================

cli:GenerateCommand
  a cli:Command ;
  rdfs:label "generate" ;
  rdfs:comment "Generate infrastructure artifacts from RDF specifications" ;
  cli:aliases "gen", "g" ;
  cli:category cli:CategoryGeneration ;
  cli:positionalArgs cli:GenerateArtifactArg ;
  cli:options cli:GenerateOutputOption, cli:GenerateValidateOption, cli:GenerateWatchOption, cli:GenerateDryRunOption ;
  cli:slo cli:GenerateSLO ;
  cli:examples """
    ggen paas generate docker
    ggen paas generate kubernetes --output yaml
    ggen paas generate terraform --validate
    ggen paas generate all --watch
    ggen paas generate openapi --dry-run
    """ ;
  dcterms:created "2026-01-08T00:00:00Z" ;
  cli:version "1.0.0" ;
  cli:handler "lib/commands/generate.js" ;
  cli:test "tests/commands/generate.test.js" ;
  .

cli:GenerateArtifactArg
  a cli:PositionalArgument ;
  rdfs:label "artifact" ;
  cli:position 0 ;
  cli:type xsd:string ;
  cli:required true ;
  cli:choices "docker", "kubernetes", "terraform", "openapi", "all" ;
  rdfs:comment "Type of artifact to generate" ;
  .

cli:GenerateOutputOption
  a cli:Option ;
  rdfs:label "output" ;
  cli:shortForm "-o" ;
  cli:longForm "--output" ;
  cli:type xsd:string ;
  cli:default "text" ;
  cli:choices "text", "json", "yaml" ;
  rdfs:comment "Output format for generation results" ;
  .

cli:GenerateValidateOption
  a cli:Option ;
  rdfs:label "validate" ;
  cli:longForm "--validate" ;
  cli:type xsd:boolean ;
  cli:default true ;
  rdfs:comment "Validate generated artifacts before returning" ;
  .

cli:GenerateWatchOption
  a cli:Option ;
  rdfs:label "watch" ;
  cli:shortForm "-w" ;
  cli:longForm "--watch" ;
  cli:type xsd:boolean ;
  cli:default false ;
  rdfs:comment "Watch for spec changes and auto-regenerate" ;
  .

cli:GenerateDryRunOption
  a cli:Option ;
  rdfs:label "dryRun" ;
  cli:longForm "--dry-run" ;
  cli:type xsd:boolean ;
  cli:default false ;
  rdfs:comment "Show what would be generated without writing files" ;
  .

cli:GenerateSLO
  a cli:SLO ;
  cli:maxDurationMs 10000 ;
  cli:expectedPassRate 99.0 ;
  rdfs:comment "SLA: 10 second max, 99% success rate" ;
  .

