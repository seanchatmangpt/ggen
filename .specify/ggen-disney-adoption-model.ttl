@prefix ex: <http://ggen.example.org/disney#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# ggen-disney: Adoption Model Specification
# Closed via C-Suite EPIC 9 Simulation (10 parallel agents)
# Consensus on critical path: Gap 2 → Gap 1 + Gap 7 (parallel) → Wave 2 → Wave 3

# ============================================================================
# THE EIGHT ADOPTION GAPS (Ordered by Executive Consensus)
# ============================================================================

ex:Gap_1_KillerWorkflow a owl:Class ;
  rdfs:label "Gap 1: Killer Workflow" ;
  rdfs:comment "Reverse-engineer O from existing state; prove automated spec-driven ops in <15min" ;
  ex:priority 1 ;
  ex:executiveConsensusRank 1.8 ;
  ex:agreementCount 9 ;
  ex:blockingGaps () ;
  ex:unblocksGaps ( ex:Gap_3_IntegrationAdapters ex:Gap_5_WorkObjectModel ex:Gap_6_FailureContainment ) ;
  ex:year1BenefitUSD 25000000 ;
  ex:year1BenefitUSD_max 50000000 ;
  ex:ownerRole "COO" ;
  ex:ownerRole "CPO" .

ex:Gap_2_AuthorityModel a owl:Class ;
  rdfs:label "Gap 2: Authority Model" ;
  rdfs:comment "Signed policy rules; immutable audit trail; fiduciary governance" ;
  ex:priority 2 ;
  ex:executiveConsensusRank 2.1 ;
  ex:agreementCount 8 ;
  ex:blockingGaps () ;
  ex:unblocksGaps ( ex:Gap_1_KillerWorkflow ex:Gap_6_FailureContainment ex:Gap_8_LegitimacyShell ) ;
  ex:year1BenefitUSD 11000000 ;
  ex:ownerRole "CISO" ;
  ex:ownerRole "GeneralCounsel" ;
  ex:ownerRole "ChiefCompliance" .

ex:Gap_3_IntegrationAdapters a owl:Class ;
  rdfs:label "Gap 3: Integration Adapters" ;
  rdfs:comment "Read-only multi-tool ingest; clean exit strategy; normalized event stream" ;
  ex:priority 3 ;
  ex:executiveConsensusRank 5.2 ;
  ex:blockingGaps ( ex:Gap_1_KillerWorkflow ex:Gap_5_WorkObjectModel ) ;
  ex:unblocksGaps () ;
  ex:year1BenefitUSD 3000000 ;
  ex:ownerRole "CTO" ;
  ex:ownerRole "Architect" .

ex:Gap_4_TelemetryBridge a owl:Class ;
  rdfs:label "Gap 4: Telemetry Bridge" ;
  rdfs:comment "OTEL → policy decisions; not dashboards; control actions on measurements" ;
  ex:priority 8 ;
  ex:executiveConsensusRank 6.5 ;
  ex:blockingGaps ( ex:Gap_1_KillerWorkflow ex:Gap_5_WorkObjectModel ) ;
  ex:unblocksGaps () ;
  ex:year1BenefitUSD 0 ;
  ex:comment "Deferred to Wave 3 (post-MVP); premature observability wastes money" ;
  ex:ownerRole "CTO" .

ex:Gap_5_WorkObjectModel a owl:Class ;
  rdfs:label "Gap 5: Work Object Model" ;
  rdfs:comment "Universal artifact typing; merge algebra; cross-domain schema unification" ;
  ex:priority 5 ;
  ex:executiveConsensusRank 5.0 ;
  ex:blockingGaps ( ex:Gap_1_KillerWorkflow ex:Gap_3_IntegrationAdapters ) ;
  ex:unblocksGaps () ;
  ex:year1BenefitUSD 2000000 ;
  ex:comment "Two-phase: Phase 1 (Wave 1) minimal schema for Gap 1 coverage; Phase 2 (Wave 3) generalization" ;
  ex:ownerRole "Architect" ;
  ex:ownerRole "CTO" .

ex:Gap_6_FailureContainment a owl:Class ;
  rdfs:label "Gap 6: Failure Containment" ;
  rdfs:comment "Staged authority; rollback guarantees; blast radius limits; liability mapping" ;
  ex:priority 6 ;
  ex:executiveConsensusRank 5.8 ;
  ex:blockingGaps ( ex:Gap_2_AuthorityModel ex:Gap_1_KillerWorkflow ex:Gap_5_WorkObjectModel ) ;
  ex:unblocksGaps ( ex:Gap_8_LegitimacyShell ) ;
  ex:year1BenefitUSD 0 ;
  ex:comment "Risk containment, not direct ROI; enables 10x automation scale" ;
  ex:ownerRole "Architect" ;
  ex:ownerRole "CISO" .

ex:Gap_7_IncentiveCover a owl:Class ;
  rdfs:label "Gap 7: Incentive Cover" ;
  rdfs:comment "Preserve manager status while removing power; role redesign as upgrade not elimination" ;
  ex:priority 7 ;
  ex:executiveConsensusRank 3.5 ;
  ex:blockingGaps ( ex:Gap_1_KillerWorkflow ) ;
  ex:unblocksGaps () ;
  ex:year1BenefitUSD 6000000 ;
  ex:year1BenefitUSD_max 9000000 ;
  ex:comment "Adoption enabler; prevents sabotage; intangible but mission-critical" ;
  ex:ownerRole "ChiefTalent" ;
  ex:ownerRole "CEO" ;
  ex:ownerRole "CPO" .

ex:Gap_8_LegitimacyShell a owl:Class ;
  rdfs:label "Gap 8: Legitimacy Shell" ;
  rdfs:comment "SSO; RBAC; audit logs; data retention; vendor risk story; SOC 2" ;
  ex:priority 8 ;
  ex:executiveConsensusRank 4.8 ;
  ex:blockingGaps ( ex:Gap_2_AuthorityModel ex:Gap_6_FailureContainment ) ;
  ex:unblocksGaps () ;
  ex:year1BenefitUSD 6500000 ;
  ex:ownerRole "CISO" ;
  ex:ownerRole "GeneralCounsel" .

# ============================================================================
# CRITICAL PATH (Consensus Blocker Chain)
# ============================================================================

ex:CriticalPath a owl:Class ;
  rdfs:label "Critical Path for ggen-disney Adoption" ;
  rdfs:comment "Derived from 10-agent EPIC 9 convergence" ;
  ex:sequence [
    rdf:_1 ex:Gap_2_AuthorityModel ;
    rdf:_2 ex:Gap_1_KillerWorkflow ;
    rdf:_3 ex:Gap_7_IncentiveCover ;
    rdf:_4 ex:Gap_3_IntegrationAdapters ;
    rdf:_5 ex:Gap_5_WorkObjectModel ;
    rdf:_6 ex:Gap_6_FailureContainment ;
    rdf:_7 ex:Gap_8_LegitimacyShell ;
    rdf:_8 ex:Gap_4_TelemetryBridge
  ] ;
  ex:comment "Gap 2, 1, and 7 run in parallel (Gate 1 is Gap 2 authority model approval)" ;
  ex:comment "Gap 4 deferred post-MVP" .

# ============================================================================
# EXECUTION PLAN (3 WAVES)
# ============================================================================

ex:Wave_1_Foundation a owl:Class ;
  rdfs:label "Wave 1: Foundation (Weeks 1-8)" ;
  rdfs:comment "Parallel: Authority Model + Killer Workflow + Role Redesign + Minimal Schema" ;
  ex:duration "8 weeks" ;
  ex:investmentUSD 2000000 ;
  ex:parallelGaps ( ex:Gap_2_AuthorityModel ex:Gap_1_KillerWorkflow ex:Gap_7_IncentiveCover ex:Gap_5_WorkObjectModel_Phase1 ) ;
  ex:exitGate "Gap 2 authority model approved by Legal/Compliance; Gap 1 ontology validated by ops team; Gap 7 role redesign announced" ;
  ex:deliverables (
    "Signed policy rules schema with immutable audit trail"
    "Reverse-engineered ontology for park opening checklist (1 real process)"
    "Work object type hierarchy (shift, task, incident, approval)"
    "Ops engineer 2.0 job description and compensation framework"
  ) .

ex:Wave_2_Validation a owl:Class ;
  rdfs:label "Wave 2: Validation (Weeks 9-20)" ;
  rdfs:comment "Scale Gap 1 to 3 more processes; adapters working; staged rollback proven; 20 ops trained" ;
  ex:duration "12 weeks" ;
  ex:investmentUSD 3000000 ;
  ex:parallelGaps ( ex:Gap_1_KillerWorkflow ex:Gap_3_IntegrationAdapters ex:Gap_6_FailureContainment ex:Gap_8_LegitimacyShell_Prep ex:Gap_7_IncentiveCover_Pilot ) ;
  ex:exitGate "Adapters working with clean rollback; 20 ops engineers trained and report satisfaction; audit trail functioning at scale" ;
  ex:deliverables (
    "4 total process ontologies (park opening, incident response, capacity planning, scheduling)"
    "Adapters for Workday, SAP, Slack (read-only with rollback)"
    "Staged authority + immutable events architecture proven in test"
    "SSO integration and RBAC framework documented"
    "30% reduction in cycle time for 2 processes (quick credibility win)"
  ) .

ex:Wave_3_Production a owl:Class ;
  rdfs:label "Wave 3: Production (Weeks 21-52)" ;
  rdfs:comment "Full scale-out: 8 critical processes, all integrations expanded, SOC 2 certified, org redesign complete" ;
  ex:duration "32 weeks" ;
  ex:investmentUSD 5000000 ;
  ex:parallelGaps ( ex:Gap_1_KillerWorkflow ex:Gap_3_IntegrationAdapters ex:Gap_8_LegitimacyShell ex:Gap_5_WorkObjectModel_Phase2 ex:Gap_7_IncentiveCover_Scale ex:Gap_6_FailureContainment_Production ) ;
  ex:exitGate "SOC 2 certified; 40% cycle time reduction proven; zero unplanned rollbacks in production; 200+ ops engineers transitioned to v2.0 roles" ;
  ex:deliverables (
    "8 critical processes fully automated (90% of ops workload)"
    "5+ systems integrated (Jira, Salesforce, custom, proven clean exit)"
    "SOC 2 Type II certification, HIPAA/GDPR compliance validated"
    "Cross-domain schema for 80% pattern reuse"
    "Org redesign complete; no retention spike in ops team"
  ) .

# ============================================================================
# FINANCIAL MODEL (Year 1)
# ============================================================================

