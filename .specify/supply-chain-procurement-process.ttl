# ==============================================================================
# EVENTS (Maps to wo:Event)
# ==============================================================================

ex:EventDemandForecastComplete a wo:Event ;
  rdfs:label "Demand Forecast Complete" ;
  rdfs:comment "Signal that daily demand aggregation finished." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-DEMAND-FORECAST-001" ;
  wo:eventType "milestone" ;
  wo:eventMessage "Daily demand forecast generated. 30-day procurement plan updated." ;
  wo:eventSource "SAP Materials Management" ;
  ex:eventTrigger ex:DemandAggregation ;
  ex:notificationChannel "Slack #procurement-ops" .

ex:EventPOsIssued a wo:Event ;
  rdfs:label "Purchase Orders Issued to Vendors" ;
  rdfs:comment "Signal that POs transmitted to all selected vendors." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-POS-ISSUED-001" ;
  wo:eventType "milestone" ;
  wo:eventMessage "Daily POs issued to vendors. Supplier portal updated." ;
  wo:eventSource "Supplier Portal" ;
  ex:eventTrigger ex:PurchaseOrderApproval ;
  ex:notificationChannel "Slack #procurement-team" .

ex:EventStockAlert a wo:Event ;
  rdfs:label "Stock-Out Risk Alert" ;
  rdfs:comment "Alert for potential inventory stock-out." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-STOCKOUT-ALERT-001" ;
  wo:eventType "alert" ;
  wo:eventSource "Inventory Forecasting System" ;
  ex:eventTrigger [
    ex:condition "Forecasted inventory below safety stock within 14 days" ;
    ex:source "SAP Materials Management"
  ] ;
  ex:notificationChannel "Slack #procurement-alerts, procurement-manager@disney.com" .

ex:EventVendorScorecard a wo:Event ;
  rdfs:label "Vendor Performance Update" ;
  rdfs:comment "Vendor performance scorecard updated." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-VENDOR-SCORE-001" ;
  wo:eventType "report" ;
  wo:eventMessage "Weekly vendor scorecard updated. Performance trends analyzed." ;
  wo:eventSource "Vendor Portal" ;
  ex:eventTrigger ex:VendorScorecard ;
  ex:notificationChannel "Slack #vendor-management" .

# ==============================================================================
# PROCESS FLOW & DEPENDENCIES
# ==============================================================================

ex:ProcurementCycleFlow a ex:ProcessFlow ;
  rdfs:label "Supply Chain Procurement Critical Path" ;
  rdfs:comment "Daily procurement cycle from demand through payment." ;
  ex:phase1_Demand "DemandAggregation (start T+0, PT12M)" ;
  ex:phase2_POs "PurchaseOrderGeneration (after demand, PT15M)" ;
  ex:phase3_Review "PoCreationReview (PT8M)" ;
  ex:phase4_Approval "PurchaseOrderApproval (gates vendor notification)" ;
  ex:phase4_Parallel "VendorScorecard (parallel, PT5M)" ;
  ex:phase5_Receipt "ReceiptReconciliation (continuous throughout day)" ;
  ex:phase6_Payment "PaymentProcessing (daily AP run)" ;
  ex:criticalPathDuration "PT45M"^^xsd:duration ;
  ex:targetCompleteTime "45 minutes per daily cycle" ;
  ex:parallelizationOpportunity "Vendor scorecard and receipt reconciliation can run in parallel during the day" .

# ==============================================================================
# EXTERNAL SYSTEM MAPPINGS
# ==============================================================================

ex:SAPMMMapping a ex:SystemMapping ;
  rdfs:label "SAP Materials Management Integration" ;
  rdfs:comment "Inventory levels, demand forecasts, PO generation." ;
  ex:maps wo:Task ;
  ex:dataFlow "SAP MM → Inventory data, demand signals → PO creation and approval" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForOperations "true" .

ex:JiraMapping a ex:SystemMapping ;
  rdfs:label "Jira Service Management Integration" ;
  rdfs:comment "Maintenance and operations demand tickets." ;
  ex:maps wo:Task ;
  ex:dataFlow "Jira SM → Demand tickets from all departments → Procurement forecast" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" ;
  ex:latencyTarget "Hourly refresh" .

ex:SupplierPortalMapping a ex:SystemMapping ;
  rdfs:label "Supplier Portal Integration (Coupa/Ariba)" ;
  rdfs:comment "Vendor pricing, availability, lead times, and performance data." ;
  ex:maps wo:Task ;
  ex:dataFlow "Supplier Portal ↔ PO submission, delivery tracking, invoice submission, performance metrics" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForOperations "true" .

ex:WMSMapping a ex:SystemMapping ;
  rdfs:label "Warehouse Management System Integration" ;
  rdfs:comment "Goods receipt confirmation, receiving logs, inventory positions." ;
  ex:maps wo:Task ;
  ex:dataFlow "WMS → Goods receipt events → Receipt reconciliation" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:latencyTarget "Real-time" .

ex:OracleAPMapping a ex:SystemMapping ;
  rdfs:label "Oracle Accounts Payable Integration" ;
  rdfs:comment "Invoice matching, payment processing." ;
  ex:maps wo:Task ;
  ex:dataFlow "Oracle AP → Invoice receipt, three-way match, payment approval" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForOperations "true" .

ex:SlackMapping a ex:SystemMapping ;
  rdfs:label "Slack Notifications" ;
  rdfs:comment "Procurement alerts and milestone notifications." ;
  ex:maps wo:Event ;
  ex:channels (
    "#procurement-ops"
    "#procurement-team"
    "#procurement-alerts"
    "#vendor-management"
  ) ;
  ex:messageTemplate "Procurement Update: {event} - {status} (Action: {action})" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" .

# ==============================================================================
# SUCCESS METRICS & CYCLE TIME IMPROVEMENTS
# ==============================================================================

ex:SuccessMetrics a owl:Class ;
  rdfs:label "Supply Chain & Procurement Success Metrics" ;
  rdfs:comment "Key performance indicators for process improvement." ;
  ex:metric_1 "Daily procurement cycle time (target: 45 minutes)" ;
  ex:metric_2 "PO creation and approval turnaround (target: <25 minutes)" ;
  ex:metric_3 "Receipt reconciliation accuracy (target: 99.5% three-way match)" ;
  ex:metric_4 "Vendor on-time delivery rate (target: ≥97%)" ;
  ex:metric_5 "Vendor quality grade (target: ≥95% Grade A)" ;
  ex:metric_6 "Procurement cost savings vs budget (target: ≥3% through optimal sourcing)" ;
  ex:metric_7 "Inventory stock-out incidents (target: <1 per month)" ;
  ex:metric_8 "Invoice payment cycle (target: 15 days vs current 30)" .

ex:CycleTimeImprovement a owl:Class ;
  rdfs:label "Cycle Time Improvements" ;
  rdfs:comment "Reduction vs manual process baseline." ;
  ex:currentManualCycleTime "240 minutes (manual demand collection, spreadsheet POs, paper filing)" ;
  ex:targetAutomatedCycleTime "45 minutes (automated aggregation, system-generated POs, digital matching)" ;
  ex:cycleTimeReduction "81.3% (195 minutes faster per cycle)" ;
  ex:annualTimeRecovery "2,925 procurement hours/year" ;
  ex:businessImpact "50% faster vendor response + 3% procurement savings + zero stock-outs + improved cash flow (15-day payment cycle)" ;
  ex:workingCapitalImprovement "15-day payment deferral on $2M monthly spend = $1M working capital improvement" .

# ==============================================================================
# METADATA
# ==============================================================================

ex:SupplyChainProcurementOntology a owl:Ontology ;
