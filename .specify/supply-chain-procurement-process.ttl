@prefix ex: <http://ggen.example.org/disney/supply-chain-procurement#> .
@prefix wo: <http://ggen.example.org/disney/work-objects#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# ==============================================================================
# Supply Chain & Procurement Process Ontology
# ==============================================================================
# Purpose: Vendor management, purchase order lifecycle, inventory fulfillment, supplier performance
# Audience: Procurement managers, inventory planners, warehouse staff, vendor managers
# Wave: 3 (Strategic process for cost control and supply reliability)
# Status: Mapped to Work Object Model types
# Created: 2026-01-18
# ==============================================================================

ex:SupplyChainProcurementProcess a owl:Class ;
  rdfs:label "Supply Chain & Procurement Process" ;
  rdfs:comment "End-to-end procurement workflow from demand signal through order fulfillment, invoice reconciliation, and vendor performance tracking. Integrates inventory management, purchase orders, and supplier scorecards." ;
  rdfs:isDefinedBy ex:SupplyChainProcurementOntology .

ex:DailyProcurementCycle a ex:SupplyChainProcurementProcess ;
  rdfs:label "Daily Procurement Cycle" ;
  rdfs:comment "Process demand signals into purchase orders, monitor fulfillment, reconcile receipts." ;
  ex:executionFrequency "Daily" ;
  ex:targetCompletionTime "45 minutes per cycle" ;
  ex:estimatedDuration "PT45M"^^xsd:duration ;
  ex:dependencies ex:DemandAggregation, ex:PurchaseOrderApproval, ex:ReceiptReconciliation .

# ==============================================================================
# SHIFTS (Maps to wo:Shift)
# ==============================================================================

ex:ProcurementShift a wo:Shift ;
  rdfs:label "Procurement Operations Shift" ;
  rdfs:comment "Daily procurement and inventory management team." ;
  ex:role "Procurement Manager, Buyer, Inventory Planner, Warehouse Supervisor" ;
  ex:startTime "2026-01-19T07:00:00"^^xsd:dateTime ;
  ex:endTime "2026-01-19T16:00:00"^^xsd:dateTime ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "SHIFT-PROCUREMENT-001" ;
  ex:location ex:ProcurementOperationsCenter ;
  ex:staffingRequirement [
    ex:role "Procurement Manager" ;
    ex:count 1 ;
    ex:source "Workday" ;
    ex:workdayTeamId "PROCUREMENT-MGR-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Buyer" ;
    ex:count 2 ;
    ex:source "Workday" ;
    ex:workdayTeamId "BUYERS-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Inventory Planner" ;
    ex:count 2 ;
    ex:source "Workday" ;
    ex:workdayTeamId "INV-PLANNER-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Warehouse Supervisor" ;
    ex:count 1 ;
    ex:source "Workday" ;
    ex:workdayTeamId "WAREHOUSE-SUP-001"
  ] .

ex:WarehouseReceivingShift a wo:Shift ;
  rdfs:label "Warehouse Receiving Shift" ;
  rdfs:comment "24/7 receiving and fulfillment operations." ;
  ex:role "Receiving Associate, Forklift Operator, Quality Inspector" ;
  ex:startTime "2026-01-19T06:00:00"^^xsd:dateTime ;
  ex:endTime "2026-01-20T06:00:00"^^xsd:dateTime ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "SHIFT-WAREHOUSE-REC-001" ;
  ex:location ex:WarehouseFacility ;
  ex:staffingRequirement [
    ex:role "Receiving Associate" ;
    ex:count 3 ;
    ex:source "Workday" ;
    ex:workdayTeamId "REC-ASSOC-001"
  ] ;
  ex:staffingRequirement [
    ex:role "Forklift Operator" ;
    ex:count 2 ;
    ex:source "Workday" ;
    ex:workdayTeamId "FORKLIFT-OP-001"
  ] .

# ==============================================================================
# TASKS (Maps to wo:Task)
# ==============================================================================

ex:DemandAggregation a wo:Task ;
  rdfs:label "Demand Aggregation & Forecast" ;
  rdfs:comment "Consolidate demand signals from all park operations into procurement forecast." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-DEMAND-AGG-001" ;
  wo:hasTitle "Aggregate Daily Demand and Create Procurement Forecast" ;
  wo:taskDescription "Collect demand from attractions, food service, retail, maintenance; consolidate into 30-day procurement forecast; flag critical items." ;
  wo:taskAssignedTo ex:InventoryPlannerLead ;
  wo:taskEstimatedDuration "PT12M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:DemandAC1, ex:DemandAC2 ;
  ex:executionOrder 1 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:PurchaseOrderGeneration ;
  ex:externalSystemDependency [
    ex:system "SAP Materials Management" ;
    ex:functionality "Inventory levels, demand forecasts from all departments" ;
    ex:requiredBefore "Aggregation"
  ] ;
  ex:externalSystemDependency [
    ex:system "Jira Service Management" ;
    ex:functionality "Maintenance and operations demand tickets" ;
    ex:requiredBefore "Forecast"
  ] .

ex:DemandAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When aggregation complete, then all department demand consolidated and missing data flagged." ;
  wo:criterionVerified "false" .

ex:DemandAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When forecast complete, then 30-day procurement forecast updated in SAP with projected stock-outs identified." ;
  wo:criterionVerified "false" .

ex:PurchaseOrderGeneration a wo:Task ;
  rdfs:label "Purchase Order Generation & Vendor Selection" ;
  rdfs:comment "Generate POs for all demand items; select vendors based on cost, quality, lead time." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-PO-GEN-001" ;
  wo:hasTitle "Generate Purchase Orders for All Demand Items" ;
  wo:taskAssignedTo ex:BuyerLead ;
  wo:taskEstimatedDuration "PT15M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:POAC1, ex:POAC2, ex:POAC3 ;
  wo:dependsOn ex:DemandAggregation ;
  ex:executionOrder 2 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:PoCreationReview ;
  ex:externalSystemDependency [
    ex:system "Supplier Portal" ;
    ex:functionality "Vendor pricing, inventory availability, lead times" ;
    ex:requiredBefore "Vendor selection"
  ] ;
  ex:externalSystemDependency [
    ex:system "SAP Procurement Module" ;
    ex:functionality "PO template generation, vendor master data" ;
    ex:requiredBefore "PO creation"
  ] .

ex:POAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When PO generation complete, then all demand items have corresponding PO entries in SAP." ;
  wo:criterionVerified "false" .

ex:POAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When POs generated, then vendors selected based on approved supplier list (cost + quality scorecard)." ;
  wo:criterionVerified "false" .

ex:POAC3 a wo:AcceptanceCriterion ;
  wo:criterionText "When POs complete, then total procurement spend forecasted and flagged against budget." ;
  wo:criterionVerified "false" .

ex:PoCreationReview a wo:Task ;
  rdfs:label "Purchase Order Review & Approval" ;
  rdfs:comment "Procurement manager reviews POs for compliance and cost." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-PO-REVIEW-001" ;
  wo:hasTitle "Review and Approve Purchase Orders" ;
  wo:taskAssignedTo ex:ProcurementManagerLead ;
  wo:taskEstimatedDuration "PT8M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:ReviewAC1, ex:ReviewAC2 ;
  wo:dependsOn ex:PurchaseOrderGeneration ;
  ex:executionOrder 3 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:PurchaseOrderApproval ;
  ex:externalSystemDependency [
    ex:system "SAP Approval Workflow" ;
    ex:functionality "Purchase order routing and approval status" ;
    ex:requiredBefore "Approval"
  ] .

ex:ReviewAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then all POs meet compliance requirements (correct coding, approved vendors)." ;
  wo:criterionVerified "false" .

ex:ReviewAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When review complete, then total spend within budget; any overages escalated." ;
  wo:criterionVerified "false" .

ex:ReceiptReconciliation a wo:Task ;
  rdfs:label "Receipt & Invoice Reconciliation" ;
  rdfs:comment "Match received goods with POs and invoices (three-way match); process payment." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-RECEIPT-RECON-001" ;
  wo:hasTitle "Reconcile Receipts, POs, and Invoices" ;
  wo:taskAssignedTo ex:ProcurementAnalystLead ;
  wo:taskEstimatedDuration "PT10M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:ReceiptAC1, ex:ReceiptAC2 ;
  ex:executionOrder 4 ;
  ex:criticalPath "true" ;
  ex:blocksSubsequent ex:PaymentProcessing ;
  ex:externalSystemDependency [
    ex:system "Warehouse Management System" ;
    ex:functionality "Goods receipt confirmation, receiving logs" ;
    ex:requiredBefore "Reconciliation"
  ] ;
  ex:externalSystemDependency [
    ex:system "Oracle Accounts Payable" ;
    ex:functionality "Invoice matching, three-way reconciliation" ;
    ex:requiredBefore "Payment"
  ] .

ex:ReceiptAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When reconciliation complete, then all received goods matched to PO and invoice (three-way match)." ;
  wo:criterionVerified "false" .

ex:ReceiptAC2 a wo:AcceptanceCriterion ;
  wo:criterionText "When reconciliation complete, then any discrepancies (quantity, price, quality) flagged and resolved." ;
  wo:criterionVerified "false" .

ex:VendorScorecard a wo:Task ;
  rdfs:label "Vendor Performance Scorecard Update" ;
  rdfs:comment "Track vendor delivery, quality, and cost performance; update scorecards." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "TASK-VENDOR-SCORE-001" ;
  wo:hasTitle "Update Vendor Performance Scorecards" ;
  wo:taskAssignedTo ex:VendorManagerLead ;
  wo:taskEstimatedDuration "PT5M"^^xsd:duration ;
  wo:taskAcceptanceCriteria ex:ScoreAC1 ;
  ex:executionOrder 5 ;
  ex:criticalPath "false" ;
  ex:externalSystemDependency [
    ex:system "Vendor Portal" ;
    ex:functionality "Delivery tracking, quality metrics, cost data" ;
    ex:requiredBefore "Scorecard update"
  ] .

ex:ScoreAC1 a wo:AcceptanceCriterion ;
  wo:criterionText "When update complete, then vendor scorecards reflect on-time delivery, quality grade, and cost performance." ;
  wo:criterionVerified "false" .

# ==============================================================================
# APPROVALS (Maps to wo:Approval)
# ==============================================================================

ex:PurchaseOrderApproval a wo:Approval ;
  rdfs:label "Purchase Order Approval" ;
  rdfs:comment "Procurement manager approval before sending POs to vendors." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "APPROVAL-PO-001" ;
  wo:hasTitle "Procurement Manager PO Approval" ;
  wo:approvalsRequired 1 ;
  wo:approvalsReceived 0 ;
  wo:approvalRequiredFrom ex:ProcurementDirectorRole ;
  ex:gates [
    ex:gateName "PO Review Complete" ;
    ex:dependsOn ex:PoCreationReview ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:gates [
    ex:gateName "Budget Compliance Verified" ;
    ex:requiredEvidence "Spend within approved budget" ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:blocksProgression ex:VendorNotification .

ex:PaymentApproval a wo:Approval ;
  rdfs:label "Invoice Payment Approval" ;
  rdfs:comment "Finance approval for vendor payment after three-way match." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "APPROVAL-PAYMENT-001" ;
  wo:hasTitle "Finance Invoice Payment Approval" ;
  wo:approvalsRequired 1 ;
  wo:approvalsReceived 0 ;
  wo:approvalRequiredFrom ex:FinanceDirectorRole ;
  ex:gates [
    ex:gateName "Three-Way Match Complete" ;
    ex:dependsOn ex:ReceiptReconciliation ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:gates [
    ex:gateName "No Quality Issues" ;
    ex:requiredEvidence "Goods received and accepted" ;
    ex:gateStatus "Awaiting"
  ] ;
  ex:blocksProgression ex:PaymentProcessing .

# ==============================================================================
# INCIDENTS (Maps to wo:Incident)
# ==============================================================================

ex:IncidentTemplate_StockOut a wo:Incident ;
  rdfs:label "Inventory Stock-Out Risk" ;
  rdfs:comment "Forecast indicates potential stock-out within 14 days." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "INC-STOCKOUT-TEMPLATE" ;
  ex:isTemplate "true" ;
  ex:incidentSeverity "High" ;
  ex:incidentDescription "Demand forecast exceeds available inventory; PO lead time may not cover gap." ;
  ex:blocksProcesses ex:DailyProcurementCycle ;
  ex:remediationPath [
    ex:firstStep "Buyer expedites vendor shipment or sources alternative supplier" ;
    ex:createTask ex:ExpeditedPOTask ;
    ex:estimatedDelay "4-48 hours depending on vendor responsiveness"
  ] ;
  ex:escalationPath [
    ex:severity "High" ;
    ex:escalateTo ex:ProcurementManager
  ] .

ex:IncidentTemplate_ReceiptDiscrepancy a wo:Incident ;
  rdfs:label "Receipt Quality or Quantity Discrepancy" ;
  rdfs:comment "Goods received do not match PO (quantity, quality, or specifications)." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "INC-RECEIPT-DISCREP-TEMPLATE" ;
  ex:isTemplate "true" ;
  ex:incidentSeverity "Medium" ;
  ex:incidentDescription "Goods fail quality check or quantity doesn't match PO; requires resolution with vendor." ;
  ex:remediationPath [
    ex:firstStep "Quality inspector documents discrepancy and photos" ;
    ex:createTask ex:VendorClaimTask ;
    ex:estimatedDelay "5-10 business days for resolution"
  ] ;
  ex:escalationPath [
    ex:severity "High" ;
    ex:escalateTo ex:VendorManager
  ] .

ex:IncidentTemplate_VendorPerformance a wo:Incident ;
  rdfs:label "Vendor Performance Degradation" ;
  rdfs:comment "Vendor scorecard shows declining on-time delivery or quality." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "INC-VENDOR-PERF-TEMPLATE" ;
  ex:isTemplate "true" ;
  ex:incidentSeverity "Medium" ;
  ex:incidentDescription "Vendor fails to meet performance targets (on-time delivery <95% or quality grade <A)." ;
  ex:remediationPath [
    ex:firstStep "Vendor manager contacts supplier for remediation discussion" ;
    ex:createTask ex:SupplierImprovementPlanTask ;
    ex:estimatedDelay "2-4 weeks for improvement plan execution"
  ] ;
  ex:escalationPath [
    ex:severity "Medium" ;
    ex:escalateTo ex:ProcurementDirector
  ] .

