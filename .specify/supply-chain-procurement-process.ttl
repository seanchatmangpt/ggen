  owl:versionInfo "1.0" ;
  dcterms:created "2026-01-18"^^xsd:date ;
  ex:phase "Wave 3 - Production Scale" ;
  ex:processCategory "Supply Chain & Procurement" ;
  ex:taskCount 6 ;
  ex:approvalGateCount 2 ;
  ex:externalSystemsIntegrated 5 ;
  ex:targetProcessTime "PT45M"^^xsd:duration ;
  ex:cycleFrequency "Daily" ;
  ex:readyForValidation "true" ;
  ex:mapsToWorkObjectTypes (
    wo:Shift
    wo:Task
    wo:Incident
    wo:Approval
