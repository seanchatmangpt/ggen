# ==============================================================================
# ROLES
# ==============================================================================

ex:ProcurementDirectorRole a ex:ApprovalRole ;
  rdfs:label "Procurement Director" ;
  rdfs:comment "Approves purchase orders." ;
  ex:systemMapping "AD group: PROCUREMENT_DIRECTORS" .

ex:FinanceDirectorRole a ex:ApprovalRole ;
  rdfs:label "Finance Director" ;
  rdfs:comment "Approves vendor payments." ;
  ex:systemMapping "AD group: FINANCE_DIRECTORS" .

ex:InventoryPlannerLead a ex:Person ;
  rdfs:label "Inventory Planner Lead" ;
  ex:workdayId "INV-PLAN-001" .

ex:BuyerLead a ex:Person ;
  rdfs:label "Buyer Lead" ;
  ex:workdayId "BUYER-001" .

