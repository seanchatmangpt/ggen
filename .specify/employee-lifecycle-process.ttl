    ex:item "Learning Management System (LMS)" ;
    ex:quantity 1 ;
    ex:systemId "LMS-PROD"
  ] ;
  ex:requiredItems [
    ex:item "Orientation video library" ;
    ex:quantity 1 ;
    ex:systemId "ORIENTATION-VIDEOS-V3"
  ] ;
  ex:requiredItems [
    ex:item "IT onboarding playbook" ;
    ex:quantity 1 ;
    ex:systemId "ONBOARDING-PLAYBOOK-V2"
  ] ;
  wo:resourceAvailability "Available" ;
  ex:verificationMethod "Daily LMS health check; video playback verification" .

ex:WorkstationEquipment a wo:Resource ;
  rdfs:label "Workstation Equipment" ;
  rdfs:comment "Computers, monitors, phones, furniture." ;
  wo:hasStatus wo:StatePending ;
  wo:hasId "RES-WORKSTATION-001" ;
  wo:resourceType "Equipment" ;
  ex:requiredItems [
    ex:item "Desktop computer with pre-loaded software" ;
    ex:quantity 1 ;
    ex:systemId "DESKTOP-IMAGING-V4"
  ] ;
  ex:requiredItems [
    ex:item "Monitor and peripherals" ;
    ex:quantity 1 ;
    ex:systemId "PERIPHERALS-BUNDLE-V1"
  ] ;
  ex:requiredItems [
    ex:item "Office furniture (desk, chair, shelving)" ;
    ex:quantity 1 ;
    ex:systemId "FURNITURE-STD-001"
  ] ;
  wo:resourceAvailability "Available" ;
  ex:verificationMethod "Physical inventory check; software license verification" .

# ==============================================================================
# EVENTS (Maps to wo:Event)
# ==============================================================================

ex:EventNewHireStarted a wo:Event ;
  rdfs:label "New Employee Started" ;
  rdfs:comment "Signal that new hire completed orientation." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-NEWHIRE-START-001" ;
  wo:eventType "milestone" ;
  wo:eventMessage "New employee completed first day orientation. Welcome to the team!" ;
  wo:eventSource "Workday" ;
  ex:eventTrigger ex:FirstDayOrientation ;
  ex:notificationChannel "Slack #company-announcements, team-specific channel" .

ex:EventTrainingComplete a wo:Event ;
  rdfs:label "Employee Training Complete" ;
  rdfs:comment "Signal that employee completed role-specific training." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-TRAINING-COMPLETE-001" ;
  wo:eventType "milestone" ;
  wo:eventMessage "Employee completed all role-specific training and certifications. Ready for independent work." ;
  wo:eventSource "LMS" ;
  ex:eventTrigger ex:TrainingCompletion ;
  ex:notificationChannel "Slack #hr-operations, manager-specific notification" .

ex:EventEmployeeOffboarded a wo:Event ;
  rdfs:label "Employee Offboarded" ;
  rdfs:comment "Signal that departing employee completely offboarded." ;
  wo:hasStatus wo:StateUndefined ;
  wo:hasId "EVT-OFFBOARD-COMPLETE-001" ;
  wo:eventType "state_change" ;
  wo:eventMessage "Employee offboarding complete. Record marked as separated." ;
  wo:eventSource "Workday" ;
  ex:eventTrigger ex:AccessRevocation ;
  ex:notificationChannel "Slack #hr-operations" .

# ==============================================================================
# PROCESS FLOW & DEPENDENCIES
# ==============================================================================

ex:OnboardingFlow a ex:ProcessFlow ;
  rdfs:label "Onboarding Critical Path" ;
  rdfs:comment "From offer acceptance through day 1 productive contribution." ;
  ex:phase1_BGCheck "BackgroundCheckInitiation (start T+0, <2 days)" ;
  ex:phase2_Prep "OnboardingPreparation (parallel with BGCheck, PT45M)" ;
  ex:phase3_Day1 "FirstDayOrientation (day 1, PT240M)" ;
  ex:phase4_Training "RoleSpecificTraining (days 1-10, PT3600M)" ;
  ex:phase5_Complete "TrainingCompletion (day 10, PT30M)" ;
  ex:phase6_Confirm "DepartmentReadiness (day 10, PT15M)" ;
  ex:criticalPathDuration "PT10D"^^xsd:duration ;
  ex:targetCompleteTime "Day 10 (10 business days)" ;
  ex:parallelizationOpportunity "Workstation prep can run in parallel with background check" .

ex:OffboardingFlow a ex:ProcessFlow ;
  rdfs:label "Offboarding Critical Path" ;
  rdfs:comment "From separation notice through complete access revocation." ;
  ex:phase1_Exit "ExitInterview (day 1 of separation, PT60M)" ;
  ex:phase2_Assets "AssetRecovery (day 1-2, PT45M)" ;
  ex:phase3_Access "AccessRevocation (day 2-3, PT30M)" ;
  ex:criticalPathDuration "PT3D"^^xsd:duration ;
  ex:targetCompleteTime "Day 3 (3 business days from notice)" .

# ==============================================================================
# EXTERNAL SYSTEM MAPPINGS
# ==============================================================================

ex:WorkdayMapping a ex:SystemMapping ;
  rdfs:label "Workday HCM Integration" ;
  rdfs:comment "Employee data, onboarding workflows, benefits administration." ;
  ex:maps wo:Task ;
  ex:dataFlow "Workday ↔ New hire records, training tracking, employee status, benefits enrollment" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForOperations "true" .

ex:LMSMapping a ex:SystemMapping ;
  rdfs:label "Learning Management System Integration" ;
  rdfs:comment "Training content, assessments, certification tracking." ;
  ex:maps wo:Task ;
  ex:dataFlow "LMS → Training curriculum assignment, assessment scores, certification tracking" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" .

ex:ActiveDirectoryMapping a ex:SystemMapping ;
  rdfs:label "Active Directory Integration" ;
  rdfs:comment "User account provisioning and deprovisioning." ;
  ex:maps wo:Task ;
  ex:dataFlow "AD ↔ User account creation, email provisioning, group membership, access revocation" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:criticalForOperations "true" .

ex:BackgroundCheckVendorMapping a ex:SystemMapping ;
  rdfs:label "Background Check Vendor Integration" ;
  rdfs:comment "Third-party background check processing." ;
  ex:maps wo:Task ;
  ex:dataFlow "Vendor API ↔ Candidate submission, background check results, clearance status" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "false" ;
  ex:latencyTarget "3-5 business days for results" .

ex:SlackMapping a ex:SystemMapping ;
  rdfs:label "Slack Notifications" ;
  rdfs:comment "Employee lifecycle milestone notifications." ;
  ex:maps wo:Event ;
  ex:channels (
    "#company-announcements"
    "#hr-operations"
    "#team-specific"
  ) ;
  ex:messageTemplate "{Employee} milestone: {event} on {date}" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" .

# ==============================================================================
# SUCCESS METRICS & CYCLE TIME IMPROVEMENTS
# ==============================================================================

ex:SuccessMetrics a owl:Class ;
  rdfs:label "Employee Lifecycle Success Metrics" ;
  rdfs:comment "Key performance indicators for process improvement." ;
  ex:metric_1 "Time-to-productivity (target: 10 business days from offer to independent contribution)" ;
  ex:metric_2 "Onboarding completion rate (target: 100% by day 10)" ;
  ex:metric_3 "Training pass rate (target: 95% first attempt, ≥80% score)" ;
  ex:metric_4 "Offboarding cycle time (target: 3 business days)" ;
  ex:metric_5 "Training content accuracy (target: Zero compliance gaps)" ;
  ex:metric_6 "New hire 90-day retention (target: ≥90%)" ;
  ex:metric_7 "Background check pass rate (target: ≥99% of candidates)" ;
  ex:metric_8 "IT access provisioning time (target: <4 hours from approval)" .

ex:CycleTimeImprovement a owl:Class ;
  rdfs:label "Cycle Time Improvements" ;
