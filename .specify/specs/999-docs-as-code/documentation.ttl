@prefix doc: <https://ggen.example.com/docs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# DOCUMENTATION STRUCTURE (RDF Ontology)

# Document Types
doc:Document a rdfs:Class ;
  rdfs:label "Document" ;
  rdfs:comment "Base documentation entity" .

doc:Guide a rdfs:Class ;
  rdfs:subClassOf doc:Document ;
  rdfs:label "Guide" ;
  rdfs:comment "Learning-oriented documentation" .

doc:Reference a rdfs:Class ;
  rdfs:subClassOf doc:Document ;
  rdfs:label "Reference" ;
  rdfs:comment "Quick lookup documentation" .

doc:Example a rdfs:Class ;
  rdfs:subClassOf doc:Document ;
  rdfs:label "Example" ;
  rdfs:comment "Code example with usage" .

doc:Gap a rdfs:Class ;
  rdfs:label "Documentation Gap" ;
  rdfs:comment "Missing documentation" .

# Properties
doc:title a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string .

doc:description a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string .

doc:pages a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:integer .

doc:effort a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string ;
  rdfs:comment "S, M, L, XL" .

doc:priority a rdf:Property ;
  rdfs:domain doc:Gap ;
  rdfs:range xsd:string ;
  rdfs:comment "P0, P1, P2, P3" .

doc:category a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string .

doc:outputFile a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string .

doc:template a rdf:Property ;
  rdfs:domain doc:Document ;
  rdfs:range xsd:string .

