# ============================================================================
# KEY ASSUMPTIONS (Replaces Interview Data)
# ============================================================================

:AssumptionSet a process:AssumptionSet ;
    rdfs:label "Park Opening Process Assumptions" ;
    rdfs:comment "Derived from typical theme park operations; to be validated by ops team" ;

    process:assumption "TIMING: Daily opening at 09:00 AM; preparation starts 05:30 AM (4-hour window)" ;
    process:assumption "STAFFING: 250+ staff required; 50+ in critical roles (security, maintenance, operations)" ;
    process:assumption "SECURITY: Perimeter includes 12 gates; 47 security cameras; full patrol required" ;
    process:assumption "EQUIPMENT: 25 rides; 40+ concessions; 50+ restrooms; all must be operational" ;
    process:assumption "CRITICALITY: Park cannot open without all 6 gates passed (security, staffing, equipment, safety, systems, executive)" ;
    process:assumption "ESCALATION: Any gate failure escalates to COO within 5 minutes; no independent work-arounds" ;
    process:assumption "INTEGRATION: Workday, SAP, Slack are system of record for staffing, assets, decisions" ;
    process:assumption "ROLLBACK: Manual procedures exist for all critical system failures (ticketing, parking, ride control)" ;
    process:assumption "WEATHER: External weather assessment occurs in Phase 5 (final executive review)" ;
    process:assumption "COMMUNICATION: All decisions logged in Slack #incident-ops for audit trail and transparency" ;
    process:assumption "INCIDENT: Any safety incident automatically escalates to incident command; gate 6 triggered" ;
    process:assumption "FREQUENCY: This process runs once per day, every operating day of the year (365+ days)" .

# ============================================================================
# RISK ASSESSMENT (FMEA-Style)
# ============================================================================

:RiskAssessment a process:RiskAssessment ;
    rdfs:label "Park Opening Process Risk Assessment" ;

    process:riskItem "Security sweep incomplete (perimeter not fully patrolled)" ;
    process:severity 9 ;
    process:probability 2 ;
    process:rpn 18 ;
    process:mitigation "Use automated gate counts + GPS tracking; require photo evidence of patrol start/end" ;
    process:mitigationOwner :Actor_DirectorSecurity ;

    process:riskItem "Critical staff no-show; positions not filled despite callouts" ;
    process:severity 8 ;
    process:probability 3 ;
    process:rpn 24 ;
    process:mitigation "Cross-train backup staff; maintain 20% surge capacity; hire temporary agency staff pre-positioned" ;
    process:mitigationOwner :Actor_DirectorParkOps ;

    process:riskItem "Ride system test failure; unable to clear equipment in time" ;
    process:severity 9 ;
    process:probability 1 ;
    process:rpn 9 ;
    process:mitigation "Pre-run ride tests at 22:00 previous night; flag any issues 7+ hours in advance" ;
    process:mitigationOwner :Actor_DirectorMaintenance ;

    process:riskItem "Ticketing system down at opening time" ;
    process:severity 7 ;
    process:probability 2 ;
    process:rpn 14 ;
    process:mitigation "Maintain manual ticketing station; hand-written tickets; reconciliation after system recovery" ;
    process:mitigationOwner :Actor_ITOperations ;

    process:riskItem "Workday API timeout; unable to pull staffing list" ;
    process:severity 5 ;
    process:probability 2 ;
    process:rpn 10 ;
    process:mitigation "Cache previous day's schedule; phone-based staff confirmation as backup" ;
    process:mitigationOwner :Actor_HROperations ;

    process:riskItem "Weather event (severe storm, lightning) overnight; forces park closure" ;
    process:severity 10 ;
    process:probability 1 ;
    process:rpn 10 ;
    process:mitigation "Weather monitoring (NOAA feed); decision made at 06:00 AM; communication plan pre-staged" ;
    process:mitigationOwner :Actor_VPOperations ;

    process:riskItem "Safety inspection overdue; compliance audit flag prevents opening" ;
    process:severity 9 ;
    process:probability 1 ;
    process:rpn 9 ;
    process:mitigation "Automated compliance calendar; alerts 14 days before expiration; proactive scheduling" ;
    process:mitigationOwner :Actor_DirectorMaintenance .
