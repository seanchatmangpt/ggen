@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix a2a: <https://ggen.ai/a2a/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Task Breakdown for A2A Integration Implementation
# Following EPIC 9 Big Bang 80/20 Pattern

a2a:A2ATaskBreakdown a prov:Plan ;
    rdfs:label "A2A Integration Task Breakdown" ;
    prov:startedAtTime "2026-02-05T00:00:00Z"^^xsd:dateTime .

# Phase 1: Specification Foundation (Critical Path)
a2a:task-001 a prov:Activity ;
    rdfs:label "Fix Syntax Errors in entities.ttl" ;
    prov:used a2a:entities-ttl ;
    prov:generated a2a:entities-ttl-fixed ;
    prov:endedAtTime "2026-02-05T00:01:00Z"^^xsd:dateTime ;
    a2a:task-type "bug-fix" ;
    a2a:priority "critical" ;
    a2a:estimated-duration "1m" ;
    a2a:critical-path true .

a2a:task-002 a prov:Activity ;
    rdfs:label "Create Complete SHACL Validation Shapes" ;
    prov:used a2a:a2a-ontology-ttl ;
    prov:generated a2a:complete-shacl-shapes ;
    prov:endedAtTime "2026-02-05T00:05:00Z"^^xsd:dateTime ;
    a2a:task-type "validation" ;
    a2a:priority "high" ;
    a2a:estimated-duration "4m" ;
    a2a:critical-path true .

# Phase 2: Core Implementation (High Impact)
a2a:task-003 a prov:Activity ;
    rdfs:label "Implement Converged Message Structure" ;
    prov:used a2a:message-specification ;
    prov:generated a2a:converged-message-types ;
    prov:endedAtTime "2026-02-05T00:20:00Z"^^xsd:dateTime ;
    a2a:task-type "implementation" ;
    a2a:priority "high" ;
    a2a:estimated-duration "15m" ;
    a2a:critical-path true ;
    a2a:overlap-impact "85% reduction in message duplication" .

a2a:task-004 a prov:Activity ;
    rdfs:label "Implement Unified Agent Interface" ;
    prov:used a2a:agent-specification ;
