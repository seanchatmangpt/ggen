# ==============================================================================
# PROCESS FLOW & DEPENDENCIES
# ==============================================================================

ex:MaintenanceSchedulingFlow a ex:ProcessFlow ;
  rdfs:label "Annual Maintenance Scheduling Critical Path" ;
  rdfs:comment "Multi-phase planning from asset audit through execution." ;
  ex:phase1_Identification "Weeks 1-3: Equipment audit, history analysis, predictive setup" ;
  ex:phase2_Planning "Weeks 3-5: Schedule creation, parts planning, downtime optimization" ;
  ex:phase3_Coordination "Weeks 5-6: Technician assignment, schedule publication" ;
  ex:phase4_Execution "Weeks 6-52: Daily dispatch, task execution, emergency response" ;
  ex:phase5_Monitoring "Weeks 6-52 (parallel): Quality audits, performance reporting" ;
  ex:totalDuration "PT52W"^^xsd:duration ;
  ex:approvalGates 3 ;
  ex:parallelPaths 2 .

ex:DailyMaintenanceFlow a ex:ProcessFlow ;
  rdfs:label "Daily Maintenance Execution" ;
  rdfs:comment "Real-time task dispatch and execution coordination." ;
  ex:step1_Dispatch "06:00: Retrieve scheduled PM work orders from SAP" ;
  ex:step2_Brief "06:15: Safety briefing and task assignment to technicians" ;
  ex:step3_Execute "06:30-14:00: Technicians perform assigned maintenance tasks" ;
  ex:step4_Document "Continuous: Work documentation in SAP (labor, parts, findings)" ;
  ex:step5_Verify "Upon completion: Supervisor quality check and sign-off" ;
  ex:step6_RTS "Task complete: Return equipment to operation after verification" ;
  ex:step7_Emergency "24/7: Emergency repairs for unplanned failures" ;
  ex:totalDuration "PT8H"^^xsd:duration ;
  ex:continuousActivity "true" .

