  ex:purpose "Manage technician schedules and assignments" ;
  ex:dataFlow "Technician roster → skill matrix → availability constraints → schedule creation" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" .

ex:SlackMaintenanceMapping a ex:SystemMapping ;
  rdfs:label "Slack Maintenance Coordination" ;
