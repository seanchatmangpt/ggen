ex:SAPPMMapping a ex:SystemMapping ;
  rdfs:label "SAP Plant Maintenance (PM) Module" ;
  rdfs:comment "Core maintenance planning, scheduling, and execution system." ;
  ex:maps wo:Task ;
  ex:maps ex:MaintenanceSchedulingProcess ;
  ex:purpose "Manage PM orders, parts, labor, and maintenance history" ;
  ex:dataFlow "Schedule → SAP PM orders → technician assignment → work execution → labor/parts tracking" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" ;
  ex:modules [
    ex:module "PM02: Create PM order" ;
    ex:module "PM03: Maintain PM order" ;
    ex:module "IW39: PM schedule board" ;
    ex:module "IW48: PM history queries"
  ] .

ex:IoTSensorMappingMaint a ex:SystemMapping ;
  rdfs:label "IoT Condition Monitoring System" ;
  rdfs:comment "Real-time equipment condition monitoring and predictive alerts." ;
  ex:maps ex:PredictiveMaintenanceEnablementTask ;
  ex:purpose "Early detection of equipment degradation before failure" ;
  ex:sensors [
    ex:sensor "Vibration monitoring" ;
    ex:count 15
  ] ;
  ex:sensors [
    ex:sensor "Temperature monitoring" ;
    ex:count 10
  ] ;
  ex:sensors [
    ex:sensor "Performance/load monitoring" ;
    ex:count 5
  ] ;
  ex:alertThreshold "Anomalous readings trigger SAP alert within <5 minutes" ;
  ex:readOnly "true" ;
  ex:rollbackCapability "false" ;
  ex:newToParkOperations "true" .

ex:SAPMMMapping a ex:SystemMapping ;
  rdfs:label "SAP Materials Management (MM) Module" ;
  rdfs:comment "Spare parts procurement and inventory management." ;
  ex:maps ex:SpareParts InventoryPlanTask ;
  ex:purpose "Manage spare parts purchasing, delivery, and stock levels" ;
  ex:dataFlow "Parts requirement → SAP PO creation → supplier delivery → goods receipt → inventory" ;
  ex:readOnly "false" ;
  ex:rollbackCapability "true" .

ex:WorkdayMaintMapping a ex:SystemMapping ;
  rdfs:label "Workday Maintenance Staffing" ;
  rdfs:comment "Technician roster, skills, availability, and shift assignment." ;
