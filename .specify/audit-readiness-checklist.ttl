    spec:description "Test results, test procedures, and control effectiveness evidence" ;
    spec:expectedCount 45 ;
    spec:location "quality-assurance-system" ;
    spec:owner "Chief Audit Officer" .

# ===== AUDITOR COORDINATION =====

audit:auditor-engagement a spec:AuditorEngagement ;
    spec:name "SOC 2 Type II Auditor Engagement" ;
    spec:status "Vendor Selection Phase" ;
    spec:targetSelectionDate "2026-03-15"^^xsd:date ;
    spec:engagementStartDate "2026-06-01"^^xsd:date ;
    spec:fieldworkStartDate "2026-08-01"^^xsd:date ;
    spec:fieldworkEndDate "2026-10-31"^^xsd:date ;
    spec:reportDeliveryDate "2026-11-30"^^xsd:date ;
    spec:preferredAuditors "Deloitte, EY, KPMG, PwC, CliftonLarsonAllen" ;
    spec:requiredQualifications "Big 4 or equivalent; 50+ SOC 2 Type II audits; SaaS platform experience; current inspection good standing" ;
    spec:estimatedCost 75000 ;
    spec:currency "USD" ;
    spec:hasDeliverable audit:rfi-response, audit:auditor-selection, audit:engagement-letter,
                         audit:fieldwork-plan, audit:management-representation-letter, audit:final-soc2-report .

audit:rfi-response a spec:DeliverableItem ;
    spec:name "Auditor RFI Response" ;
    spec:dueDate "2026-02-15"^^xsd:date ;
    spec:owner "CFO/General Counsel" ;
    spec:description "Response to auditor request for information during selection process" .

audit:auditor-selection a spec:DeliverableItem ;
    spec:name "Auditor Selection & Contracting" ;
    spec:dueDate "2026-03-15"^^xsd:date ;
    spec:owner "CFO" ;
    spec:description "Auditor selected, SOW signed, engagement letter executed" .

audit:engagement-letter a spec:DeliverableItem ;
    spec:name "Engagement Letter" ;
    spec:dueDate "2026-03-31"^^xsd:date ;
    spec:owner "General Counsel" ;
    spec:description "Signed engagement letter with auditor outlining scope and terms" .

audit:fieldwork-plan a spec:DeliverableItem ;
    spec:name "Fieldwork Plan" ;
    spec:dueDate "2026-07-15"^^xsd:date ;
    spec:owner "Chief Compliance Officer" ;
    spec:description "Detailed fieldwork plan provided by auditor with testing schedule" .

audit:management-representation-letter a spec:DeliverableItem ;
    spec:name "Management Representation Letter" ;
    spec:dueDate "2026-10-15"^^xsd:date ;
    spec:owner "CEO/CFO" ;
    spec:description "Management representation letter confirming assertions about controls" .

audit:final-soc2-report a spec:DeliverableItem ;
    spec:name "Final SOC 2 Type II Report" ;
    spec:dueDate "2026-11-30"^^xsd:date ;
    spec:owner "Chief Compliance Officer" ;
    spec:description "Final SOC 2 Type II certification report with auditor sign-off" .

