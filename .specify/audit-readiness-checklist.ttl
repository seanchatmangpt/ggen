# ===== EVIDENCE GATHERING REQUIREMENTS =====

audit:evidence-repository a spec:EvidenceManagement ;
    spec:name "SOC 2 Evidence Repository" ;
    spec:description "Centralized storage for all audit evidence organized by control area" ;
    spec:location "/evidence/soc2-audit/" ;
    spec:accessControl "Role-based; auditor read access granted" ;
    spec:retentionPeriod "3 years post-certification (minimum)" ;
    spec:hasEvidenceCategory audit:policies-procedures, audit:system-evidence, audit:operational-evidence,
                              audit:training-evidence, audit:incident-evidence, audit:testing-evidence .

audit:policies-procedures a spec:EvidenceCategory ;
    spec:name "Policies & Procedures" ;
    spec:description "All documented policies, procedures, and standards" ;
    spec:expectedCount 40 ;
    spec:location "governance-repository" ;
    spec:owner "Chief Compliance Officer" .

audit:system-evidence a spec:EvidenceCategory ;
    spec:name "System & Technical Evidence" ;
    spec:description "System configurations, logs, and technical controls evidence" ;
    spec:expectedCount 60 ;
    spec:location "siem-and-security-logs" ;
    spec:owner "Chief Information Security Officer" .

audit:operational-evidence a spec:EvidenceCategory ;
    spec:name "Operational Evidence" ;
    spec:description "Logs, records, and documentation of operational activities" ;
    spec:expectedCount 50 ;
    spec:location "operations-management-system" ;
    spec:owner "Chief Operating Officer" .

audit:training-evidence a spec:EvidenceCategory ;
    spec:name "Training & Competence Evidence" ;
    spec:description "Training records, certifications, and competence assessments" ;
    spec:expectedCount 30 ;
    spec:location "learning-management-system" ;
    spec:owner "Chief HR Officer" .

audit:incident-evidence a spec:EvidenceCategory ;
    spec:name "Incident & Risk Evidence" ;
    spec:description "Incident logs, investigations, and risk assessments" ;
    spec:expectedCount 25 ;
    spec:location "incident-management-system" ;
    spec:owner "Chief Risk Officer" .

audit:testing-evidence a spec:EvidenceCategory ;
