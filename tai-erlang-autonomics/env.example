# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Application Configuration
PORT=8080
PUBSUB_SUBSCRIPTION=erlang-autonomics-signals-sub
RECEIPT_LEDGER_BACKEND=ets

# Observability
METRICS_COLLECTION_INTERVAL_MS=10000
TRACING_ENABLED=true

# Docker Compose
COMPOSE_PROJECT_NAME=tai-autonomics

# Terraform (for local development)
TF_VAR_project_id=${GCP_PROJECT_ID}
TF_VAR_region=${GCP_REGION}
TF_VAR_environment=dev
TF_VAR_image_tag=latest

# Cloud Run Service URL (for integration tests)
CLOUD_RUN_SERVICE_URL=https://tai-autonomics-xxxxx-uc.a.run.app
