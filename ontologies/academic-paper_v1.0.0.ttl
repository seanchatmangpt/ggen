ap:issue a owl:DatatypeProperty ;
    rdfs:domain ap:BibliographyEntry ;
    rdfs:range xsd:string ;
    rdfs:label "issue" ;
    rdfs:comment "Journal issue" .

ap:pages a owl:DatatypeProperty ;
    rdfs:domain ap:BibliographyEntry ;
    rdfs:range xsd:string ;
    rdfs:label "pages" ;
    rdfs:comment "Page numbers" .

ap:doi a owl:DatatypeProperty ;
    rdfs:domain ap:BibliographyEntry ;
    rdfs:range xsd:string ;
    rdfs:label "DOI" ;
    rdfs:comment "Digital Object Identifier" .

ap:url a owl:DatatypeProperty ;
    rdfs:domain ap:BibliographyEntry ;
    rdfs:range xsd:anyURI ;
    rdfs:label "URL" ;
    rdfs:comment "URL to resource" .

ap:bibType a owl:DatatypeProperty ;
    rdfs:domain ap:BibliographyEntry ;
    rdfs:range [ rdf:type rdfs:Datatype ;
        owl:oneOf ( "article" "book" "inproceedings" "misc" "thesis" "techreport" ) ] ;
    rdfs:label "bibliography type" ;
    rdfs:comment "BibTeX entry type" .

# ============================================================================
# SUBMISSION & REVIEW PROPERTIES
# ============================================================================

ap:submissionVenue a owl:ObjectProperty ;
    rdfs:domain ap:Submission ;
    rdfs:range ap:Venue ;
