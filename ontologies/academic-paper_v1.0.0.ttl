    rdfs:range xsd:string ;
    rdfs:label "equation LaTeX" ;
    rdfs:comment "LaTeX equation code" ;
    owl:minCardinality 1 .

ap:equationOrder a owl:DatatypeProperty ;
    rdfs:domain ap:Equation ;
    rdfs:range xsd:integer ;
    rdfs:label "equation order" ;
    rdfs:comment "Position in document" .

