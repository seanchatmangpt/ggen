ap:Physics a ap:Discipline ;
    rdfs:label "Physics" ;
    rdfs:comment "Physics and theoretical physics" .

ap:Biology a ap:Discipline ;
    rdfs:label "Biology" ;
    rdfs:comment "Biology and life sciences" .

ap:Medicine a ap:Discipline ;
    rdfs:label "Medicine" ;
