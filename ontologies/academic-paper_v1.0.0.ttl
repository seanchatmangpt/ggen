# ============================================================================
# INFERENCE RULES
# ============================================================================

# Papers with published submissions are marked as published
[] a owl:Axiom ;
    owl:subject ap:Paper ;
    owl:predicate ap:status ;
    owl:object "published" ;
    rdfs:comment "If paper has submission with published status, paper is published" .

# Corresponding author is first author if not specified
[] a owl:Axiom ;
    owl:subject ap:Author ;
    owl:predicate ap:correspondingAuthor ;
    rdfs:comment "Corresponding author defaults to first author" .

# Word count auto-calculated from sections
[] a owl:Axiom ;
    owl:subject ap:Paper ;
    owl:predicate ap:wordCount ;
    rdfs:comment "Word count calculated from section content" .
