ap:reviewDate a owl:DatatypeProperty ;
    rdfs:domain ap:PeerReview ;
    rdfs:range xsd:dateTime ;
    rdfs:label "review date" ;
    rdfs:comment "Date review was submitted" .

ap:reviewContent a owl:DatatypeProperty ;
