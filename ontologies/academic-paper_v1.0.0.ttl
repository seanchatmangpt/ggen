ap:decisionDate a owl:DatatypeProperty ;
    rdfs:domain ap:Submission ;
    rdfs:range xsd:dateTime ;
    rdfs:label "decision date" ;
    rdfs:comment "Date decision was made" .

ap:hasPeerReview a owl:ObjectProperty ;
