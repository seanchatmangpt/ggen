@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ap: <http://ggen.dev/schema/academic-paper#> .
@prefix bib: <http://purl.org/net/biblio#> .

# ============================================================================
# ACADEMIC PAPER ONTOLOGY v1.0.0
# ============================================================================
#
# Complete ontology for academic paper generation, management, and publishing.
#
# Features:
# - Paper metadata (title, abstract, authors, keywords)
# - Document structure (sections, figures, tables, equations)
# - Bibliography and citations (BibTeX integration)
# - Lifecycle tracking (draft, review, revision, published)
# - Submission management (venue, status, reviews)
# - Collaboration (multi-author, roles, contributions)
# - Publishing integration (arXiv, DOI, journal submission)
#

# ============================================================================
# ONTOLOGY DEFINITION
# ============================================================================

ap:AcademicPaperOntology a owl:Ontology ;
    rdfs:label "Academic Paper v1.0.0 Ontology Schema" ;
    rdfs:comment "Complete RDF schema for academic paper generation and lifecycle management" ;
    owl:versionInfo "1.0.0" ;
    owl:imports <http://purl.org/net/biblio#> ;
    owl:imports <http://ggen.dev/schema/cli#> .

# ============================================================================
# CORE CLASSES
# ============================================================================

ap:Paper a owl:Class ;
    rdfs:label "Paper" ;
    rdfs:comment "An academic research paper" .

ap:Author a owl:Class ;
    rdfs:label "Author" ;
    rdfs:comment "Author of an academic paper" .

ap:Section a owl:Class ;
    rdfs:label "Section" ;
    rdfs:comment "A section of the paper" .

ap:Figure a owl:Class ;
    rdfs:label "Figure" ;
    rdfs:comment "A figure or diagram in the paper" .

ap:Table a owl:Class ;
    rdfs:label "Table" ;
    rdfs:comment "A table in the paper" .

ap:Equation a owl:Class ;
    rdfs:label "Equation" ;
    rdfs:comment "A mathematical equation" .

ap:Citation a owl:Class ;
    rdfs:label "Citation" ;
    rdfs:comment "A bibliographic citation" .

ap:BibliographyEntry a owl:Class ;
    rdfs:label "Bibliography Entry" ;
    rdfs:comment "An entry in the bibliography" ;
    rdfs:subClassOf bib:Document .

ap:Submission a owl:Class ;
    rdfs:label "Submission" ;
    rdfs:comment "Paper submission to venue" .

ap:PeerReview a owl:Class ;
    rdfs:label "Peer Review" ;
    rdfs:comment "Peer review feedback" .

ap:Revision a owl:Class ;
    rdfs:label "Revision" ;
    rdfs:comment "A revision of the paper" .

ap:Venue a owl:Class ;
    rdfs:label "Venue" ;
    rdfs:comment "Publication venue (conference, journal)" .

ap:Discipline a owl:Class ;
    rdfs:label "Discipline" ;
    rdfs:comment "Academic discipline category" .

ap:Affiliation a owl:Class ;
    rdfs:label "Affiliation" ;
    rdfs:comment "Institutional affiliation" .

ap:Contribution a owl:Class ;
    rdfs:label "Contribution" ;
    rdfs:comment "Author contribution statement" .

# ============================================================================
# PAPER PROPERTIES
# ============================================================================

ap:title a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "title" ;
    rdfs:comment "Paper title" ;
    owl:minCardinality 1 .

ap:abstract a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "abstract" ;
    rdfs:comment "Paper abstract (150-300 words)" .

ap:keywords a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "keywords" ;
    rdfs:comment "Paper keywords (comma-separated)" .

ap:discipline a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Discipline ;
    rdfs:label "discipline" ;
    rdfs:comment "Academic discipline" .

ap:hasAuthor a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Author ;
    rdfs:label "has author" ;
    rdfs:comment "Authors of the paper" .

ap:hasSection a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Section ;
    rdfs:label "has section" ;
    rdfs:comment "Sections in the paper" .

ap:hasFigure a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Figure ;
    rdfs:label "has figure" ;
    rdfs:comment "Figures in the paper" .

ap:hasTable a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Table ;
    rdfs:label "has table" ;
    rdfs:comment "Tables in the paper" .

ap:hasBibliography a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:BibliographyEntry ;
    rdfs:label "has bibliography" ;
    rdfs:comment "Bibliography entries" .

ap:hasSubmission a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Submission ;
    rdfs:label "has submission" ;
    rdfs:comment "Submission history" .

ap:hasRevision a owl:ObjectProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range ap:Revision ;
    rdfs:label "has revision" ;
    rdfs:comment "Revisions of the paper" .

ap:createdDate a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:dateTime ;
    rdfs:label "created date" ;
    rdfs:comment "Date paper was created" .

ap:modifiedDate a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:dateTime ;
    rdfs:label "modified date" ;
    rdfs:comment "Date paper was last modified" .

ap:status a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range [ rdf:type rdfs:Datatype ;
        owl:oneOf ( "draft" "under-review" "revision-requested" "accepted" "published" ) ] ;
    rdfs:label "status" ;
    rdfs:comment "Current status: draft, under-review, revision-requested, accepted, published" .

ap:pageCount a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:integer ;
    rdfs:label "page count" ;
    rdfs:comment "Total number of pages" .

ap:wordCount a owl:DatatypeProperty ;
    rdfs:domain ap:Paper ;
    rdfs:range xsd:integer ;
    rdfs:label "word count" ;
    rdfs:comment "Total word count" .

# ============================================================================
# AUTHOR PROPERTIES
# ============================================================================

ap:authorName a owl:DatatypeProperty ;
    rdfs:domain ap:Author ;
    rdfs:range xsd:string ;
    rdfs:label "author name" ;
    rdfs:comment "Full name of author" ;
    owl:minCardinality 1 .

ap:email a owl:DatatypeProperty ;
    rdfs:domain ap:Author ;
    rdfs:range xsd:string ;
    rdfs:label "email" ;
    rdfs:comment "Email address" .

ap:orcid a owl:DatatypeProperty ;
    rdfs:domain ap:Author ;
    rdfs:range xsd:string ;
    rdfs:label "ORCID" ;
    rdfs:comment "ORCID identifier" .

ap:authorOrder a owl:DatatypeProperty ;
    rdfs:domain ap:Author ;
    rdfs:range xsd:integer ;
    rdfs:label "author order" ;
    rdfs:comment "Position in author list" .

ap:correspondingAuthor a owl:DatatypeProperty ;
    rdfs:domain ap:Author ;
    rdfs:range xsd:boolean ;
    rdfs:label "corresponding author" ;
    rdfs:comment "Is this the corresponding author?" .

ap:hasAffiliation a owl:ObjectProperty ;
    rdfs:domain ap:Author ;
    rdfs:range ap:Affiliation ;
    rdfs:label "has affiliation" ;
    rdfs:comment "Institutional affiliation" .

ap:hasContribution a owl:ObjectProperty ;
    rdfs:domain ap:Author ;
    rdfs:range ap:Contribution ;
    rdfs:label "has contribution" ;
    rdfs:comment "Contribution statement (CRediT taxonomy)" .

# ============================================================================
# AFFILIATION PROPERTIES
# ============================================================================

ap:affiliationName a owl:DatatypeProperty ;
    rdfs:domain ap:Affiliation ;
    rdfs:range xsd:string ;
    rdfs:label "affiliation name" ;
    rdfs:comment "Name of institution" ;
    owl:minCardinality 1 .

ap:department a owl:DatatypeProperty ;
    rdfs:domain ap:Affiliation ;
    rdfs:range xsd:string ;
    rdfs:label "department" ;
    rdfs:comment "Department or faculty" .

ap:country a owl:DatatypeProperty ;
    rdfs:domain ap:Affiliation ;
    rdfs:range xsd:string ;
    rdfs:label "country" ;
    rdfs:comment "Country" .

# ============================================================================
# CONTRIBUTION PROPERTIES
# ============================================================================

ap:contributionRole a owl:DatatypeProperty ;
    rdfs:domain ap:Contribution ;
    rdfs:range [ rdf:type rdfs:Datatype ;
        owl:oneOf ( "Conceptualization" "Data curation" "Formal analysis"
                    "Funding acquisition" "Investigation" "Methodology"
                    "Project administration" "Resources" "Software"
                    "Supervision" "Validation" "Visualization" "Writing" ) ] ;
    rdfs:label "contribution role" ;
    rdfs:comment "CRediT taxonomy contribution role" .

# ============================================================================
# SECTION PROPERTIES
# ============================================================================

ap:sectionTitle a owl:DatatypeProperty ;
    rdfs:domain ap:Section ;
    rdfs:range xsd:string ;
    rdfs:label "section title" ;
    rdfs:comment "Section heading" ;
    owl:minCardinality 1 .

ap:sectionType a owl:DatatypeProperty ;
    rdfs:domain ap:Section ;
    rdfs:range [ rdf:type rdfs:Datatype ;
        owl:oneOf ( "introduction" "related-work" "methods" "results"
                    "discussion" "conclusion" "acknowledgments" "appendix" ) ] ;
    rdfs:label "section type" ;
    rdfs:comment "Type of section" .

ap:sectionOrder a owl:DatatypeProperty ;
    rdfs:domain ap:Section ;
    rdfs:range xsd:integer ;
    rdfs:label "section order" ;
    rdfs:comment "Position in document" .

ap:sectionContent a owl:DatatypeProperty ;
    rdfs:domain ap:Section ;
    rdfs:range xsd:string ;
    rdfs:label "section content" ;
    rdfs:comment "Markdown or LaTeX content" .

ap:subsectionOf a owl:ObjectProperty ;
    rdfs:domain ap:Section ;
    rdfs:range ap:Section ;
    rdfs:label "subsection of" ;
    rdfs:comment "Parent section for subsections" .

