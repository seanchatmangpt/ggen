{
  "name": "@ggen/paas",
  "version": "1.0.0",
  "description": "ggen PaaS - Infrastructure generation and management CLI",
  "type": "module",
  "bin": {
    "paas": "./bin/paas"
  },
  "main": "./lib/cli-dispatcher.js",
  "exports": {
    ".": "./lib/cli-dispatcher.js",
    "./dispatcher": "./lib/cli-dispatcher.js",
    "./ontology": "./lib/utils/ontology.js",
    "./logger": "./lib/utils/logger.js"
  },
  "scripts": {
    "start": "node bin/paas",
    "help": "node bin/paas --help",
    "test": "vitest",
    "lint": "eslint lib/",
    "format": "prettier --write lib/",
    "precommit": "npm run lint && npm run format"
  },
  "keywords": [
    "ggen",
    "paas",
    "infrastructure",
    "cli",
    "rdf",
    "ontology"
  ],
  "author": "ggen contributors",
  "license": "MIT",
  "dependencies": {
    "n3": "^1.17.0",
    "chalk": "^5.0.0"
  },
  "devDependencies": {
    "vitest": "^0.34.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanchatmangpt/ggen.git",
    "directory": "ggen-paas"
  }
}
