@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix reasoner: <http://ggen.ruv.io/ontology/reasoner#> .
@prefix cnv: <http://ggen.ruv.io/ontology/clap-noun-verb#> .

# ============================================================================
# OWL Ontology for reasoner-cli - OWL Inference Engine and Classification
# ============================================================================

<http://ggen.ruv.io/ontology/reasoner> a owl:Ontology ;
    dcterms:title "Reasoner CLI Ontology" ;
    dcterms:description "OWL inference engine and ontology classification using clap-noun-verb" ;
    dcterms:creator "ggen marketplace" ;
    dcterms:created "2025-01-09"^^xsd:date ;
    owl:versionInfo "1.0.0" .

# ============================================================================
# NOUN 1: Classifier - Taxonomy building and instance classification
# ============================================================================

reasoner:Classifier a owl:Class ;
    rdfs:label "Classifier" ;
    rdfs:comment "Builds taxonomies and classifies ontology instances" ;
    rdfs:subClassOf cnv:Noun .

# Classifier Properties
reasoner:hasClassificationTask a owl:ObjectProperty ;
    rdfs:domain reasoner:Classifier ;
    rdfs:range reasoner:ClassificationTask ;
    rdfs:label "has classification task" .

reasoner:usesReasoner a owl:ObjectProperty ;
    rdfs:domain reasoner:Classifier ;
    rdfs:range reasoner:ReasonerEngine ;
    rdfs:label "uses reasoner" .

reasoner:hasOWLProfile a owl:ObjectProperty ;
    rdfs:domain reasoner:Classifier ;
    rdfs:range reasoner:OWLProfile ;
    rdfs:label "has OWL profile" .

reasoner:classificationTime a owl:DatatypeProperty ;
    rdfs:domain reasoner:Classifier ;
    rdfs:range xsd:duration ;
    rdfs:label "classification time" .

reasoner:taxonomyDepth a owl:DatatypeProperty ;
    rdfs:domain reasoner:Classifier ;
    rdfs:range xsd:integer ;
    rdfs:label "taxonomy depth" .

# Classifier Verbs
reasoner:classify a owl:Class ;
    rdfs:label "classify" ;
    rdfs:comment "Classify ontology and build class hierarchy" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Classifier .

reasoner:realize a owl:Class ;
    rdfs:label "realize" ;
    rdfs:comment "Compute instance types and realize ABox" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Classifier .

reasoner:materialize a owl:Class ;
    rdfs:label "materialize" ;
    rdfs:comment "Materialize inferred triples" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Classifier .

reasoner:profile a owl:Class ;
    rdfs:label "profile" ;
    rdfs:comment "Profile classification performance" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Classifier .

reasoner:configure a owl:Class ;
    rdfs:label "configure" ;
    rdfs:comment "Configure classifier settings" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Classifier .

# ============================================================================
# NOUN 2: Ontology - OWL ontology management and operations
# ============================================================================

reasoner:Ontology a owl:Class ;
    rdfs:label "Ontology" ;
    rdfs:comment "Manages OWL ontologies and their operations" ;
    rdfs:subClassOf cnv:Noun .

# Ontology Properties
reasoner:hasFormat a owl:ObjectProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range reasoner:OntologyFormat ;
    rdfs:label "has format" .

reasoner:hasImport a owl:ObjectProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range reasoner:Ontology ;
    rdfs:label "has import" .

reasoner:hasAxiomCount a owl:DatatypeProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range xsd:integer ;
    rdfs:label "has axiom count" .

reasoner:hasClassCount a owl:DatatypeProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range xsd:integer ;
    rdfs:label "has class count" .

reasoner:hasPropertyCount a owl:DatatypeProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range xsd:integer ;
    rdfs:label "has property count" .

reasoner:hasIndividualCount a owl:DatatypeProperty ;
    rdfs:domain reasoner:Ontology ;
    rdfs:range xsd:integer ;
    rdfs:label "has individual count" .

# Ontology Verbs
reasoner:load a owl:Class ;
    rdfs:label "load" ;
    rdfs:comment "Load ontology from file or URI" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Ontology .

reasoner:merge a owl:Class ;
    rdfs:label "merge" ;
    rdfs:comment "Merge multiple ontologies" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Ontology .

reasoner:modularize a owl:Class ;
    rdfs:label "modularize" ;
    rdfs:comment "Extract ontology modules" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Ontology .

reasoner:explain a owl:Class ;
    rdfs:label "explain" ;
    rdfs:comment "Explain ontology entailments" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Ontology .

reasoner:repair a owl:Class ;
    rdfs:label "repair" ;
    rdfs:comment "Repair inconsistent ontology" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Ontology .

# ============================================================================
# NOUN 3: Inference - Logical inference and entailment
# ============================================================================

reasoner:Inference a owl:Class ;
    rdfs:label "Inference" ;
    rdfs:comment "Performs logical inference and entailment checking" ;
    rdfs:subClassOf cnv:Noun .

# Inference Properties
reasoner:hasInferenceRule a owl:ObjectProperty ;
    rdfs:domain reasoner:Inference ;
    rdfs:range reasoner:InferenceRule ;
    rdfs:label "has inference rule" .

reasoner:hasEntailmentRegime a owl:ObjectProperty ;
    rdfs:domain reasoner:Inference ;
    rdfs:range reasoner:EntailmentRegime ;
    rdfs:label "has entailment regime" .

reasoner:inferenceComplexity a owl:DatatypeProperty ;
    rdfs:domain reasoner:Inference ;
    rdfs:range xsd:string ;
    rdfs:label "inference complexity" .

reasoner:inferredTriplesCount a owl:DatatypeProperty ;
    rdfs:domain reasoner:Inference ;
    rdfs:range xsd:integer ;
    rdfs:label "inferred triples count" .

# Inference Verbs
reasoner:derive a owl:Class ;
    rdfs:label "derive" ;
    rdfs:comment "Derive new facts from existing knowledge" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Inference .

reasoner:entail a owl:Class ;
    rdfs:label "entail" ;
    rdfs:comment "Check if statement is entailed" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Inference .

reasoner:subsume a owl:Class ;
    rdfs:label "subsume" ;
    rdfs:comment "Check subsumption relationships" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Inference .

reasoner:test a owl:Class ;
    rdfs:label "test" ;
    rdfs:comment "Test inference correctness" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Inference .

reasoner:equivalent a owl:Class ;
    rdfs:label "equivalent" ;
    rdfs:comment "Check equivalence relationships" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Inference .

# ============================================================================
# NOUN 4: Validator - Ontology validation and consistency checking
# ============================================================================

reasoner:Validator a owl:Class ;
    rdfs:label "Validator" ;
    rdfs:comment "Validates ontologies and checks consistency" ;
    rdfs:subClassOf cnv:Noun .

# Validator Properties
reasoner:hasValidationLevel a owl:ObjectProperty ;
    rdfs:domain reasoner:Validator ;
    rdfs:range reasoner:ValidationLevel ;
    rdfs:label "has validation level" .

reasoner:hasInconsistency a owl:ObjectProperty ;
    rdfs:domain reasoner:Validator ;
    rdfs:range reasoner:Inconsistency ;
    rdfs:label "has inconsistency" .

reasoner:isConsistent a owl:DatatypeProperty ;
    rdfs:domain reasoner:Validator ;
    rdfs:range xsd:boolean ;
    rdfs:label "is consistent" .

reasoner:validationTime a owl:DatatypeProperty ;
    rdfs:domain reasoner:Validator ;
    rdfs:range xsd:duration ;
    rdfs:label "validation time" .

# Validator Verbs
reasoner:check a owl:Class ;
    rdfs:label "check" ;
    rdfs:comment "Check ontology consistency" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Validator .

reasoner:validate a owl:Class ;
    rdfs:label "validate" ;
    rdfs:comment "Validate against OWL profile" ;
    rdfs:subClassOf cnv:Verb ;
    cnv:appliesToNoun reasoner:Validator .

# ============================================================================
# Supporting Classes
# ============================================================================

reasoner:ClassificationTask a owl:Class ;
    rdfs:label "Classification Task" ;
    rdfs:comment "Task for ontology classification" .

reasoner:ReasonerEngine a owl:Class ;
    rdfs:label "Reasoner Engine" ;
    rdfs:comment "Reasoning engine implementation" .

reasoner:OWLProfile a owl:Class ;
    rdfs:label "OWL Profile" ;
    rdfs:comment "OWL 2 profile (DL, EL, QL, RL)" .

reasoner:OWL2DL a reasoner:OWLProfile ;
    rdfs:label "OWL 2 DL" ;
    rdfs:comment "Full OWL 2 Description Logic" .

reasoner:OWL2EL a reasoner:OWLProfile ;
    rdfs:label "OWL 2 EL" ;
    rdfs:comment "Existential Logic profile for large ontologies" .

reasoner:OWL2QL a reasoner:OWLProfile ;
    rdfs:label "OWL 2 QL" ;
    rdfs:comment "Query Language profile for databases" .

reasoner:OWL2RL a reasoner:OWLProfile ;
    rdfs:label "OWL 2 RL" ;
    rdfs:comment "Rule Language profile for rule engines" .

reasoner:OntologyFormat a owl:Class ;
    rdfs:label "Ontology Format" ;
    rdfs:comment "Serialization format for ontologies" .

reasoner:RDFXML a reasoner:OntologyFormat ;
    rdfs:label "RDF/XML" ;
    rdfs:comment "RDF/XML serialization" .

reasoner:Turtle a reasoner:OntologyFormat ;
    rdfs:label "Turtle" ;
    rdfs:comment "Turtle serialization" .

reasoner:NTriples a reasoner:OntologyFormat ;
    rdfs:label "N-Triples" ;
    rdfs:comment "N-Triples serialization" .

reasoner:InferenceRule a owl:Class ;
    rdfs:label "Inference Rule" ;
    rdfs:comment "Rule for logical inference" .

reasoner:EntailmentRegime a owl:Class ;
    rdfs:label "Entailment Regime" ;
    rdfs:comment "Entailment regime (RDFS, OWL DL, etc.)" .

reasoner:RDFSEntailment a reasoner:EntailmentRegime ;
    rdfs:label "RDFS Entailment" .

reasoner:OWLDLEntailment a reasoner:EntailmentRegime ;
    rdfs:label "OWL DL Entailment" .

reasoner:ValidationLevel a owl:Class ;
    rdfs:label "Validation Level" ;
    rdfs:comment "Level of validation rigor" .

reasoner:SyntaxValidation a reasoner:ValidationLevel ;
    rdfs:label "Syntax Validation" .

reasoner:SemanticValidation a reasoner:ValidationLevel ;
    rdfs:label "Semantic Validation" .

reasoner:ConsistencyValidation a reasoner:ValidationLevel ;
    rdfs:label "Consistency Validation" .

reasoner:Inconsistency a owl:Class ;
    rdfs:label "Inconsistency" ;
    rdfs:comment "Logical inconsistency in ontology" .

# ============================================================================
# Command Line Interface Structure
# ============================================================================

reasoner:ClassifierCommand a owl:Class ;
    rdfs:label "Classifier Command" ;
    rdfs:comment "CLI command for classification operations" ;
    cnv:hasNoun reasoner:Classifier ;
    cnv:hasVerb reasoner:classify, reasoner:realize, reasoner:materialize,
                reasoner:profile, reasoner:configure .

reasoner:OntologyCommand a owl:Class ;
    rdfs:label "Ontology Command" ;
    rdfs:comment "CLI command for ontology operations" ;
    cnv:hasNoun reasoner:Ontology ;
    cnv:hasVerb reasoner:load, reasoner:merge, reasoner:modularize,
                reasoner:explain, reasoner:repair .

reasoner:InferenceCommand a owl:Class ;
    rdfs:label "Inference Command" ;
    rdfs:comment "CLI command for inference operations" ;
    cnv:hasNoun reasoner:Inference ;
    cnv:hasVerb reasoner:derive, reasoner:entail, reasoner:subsume,
                reasoner:test, reasoner:equivalent .

reasoner:ValidatorCommand a owl:Class ;
    rdfs:label "Validator Command" ;
    rdfs:comment "CLI command for validation operations" ;
    cnv:hasNoun reasoner:Validator ;
    cnv:hasVerb reasoner:check, reasoner:validate .

# ============================================================================
# Usage Examples
# ============================================================================

reasoner:example1 a owl:NamedIndividual ;
    rdfs:label "Classification Example" ;
    rdfs:comment "reasoner-cli classifier classify --ontology pizza.owl --profile DL" .

reasoner:example2 a owl:NamedIndividual ;
    rdfs:label "Inference Example" ;
    rdfs:comment "reasoner-cli inference derive --ontology medical.ttl --regime OWL-DL" .

reasoner:example3 a owl:NamedIndividual ;
    rdfs:label "Validation Example" ;
    rdfs:comment "reasoner-cli validator check --ontology schema.rdf --level semantic" .

reasoner:example4 a owl:NamedIndividual ;
    rdfs:label "Ontology Merge Example" ;
    rdfs:comment "reasoner-cli ontology merge --inputs ont1.ttl ont2.ttl --output merged.ttl" .
