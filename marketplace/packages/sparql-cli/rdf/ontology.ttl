@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix cnv: <http://clap-noun-verb.org/ontology#> .
@prefix sparql: <http://sparql-cli.org/ontology#> .

# ============================================================================
# SPARQL CLI Ontology
# Complete RDF/OWL ontology for SPARQL query execution, optimization, and federation
# ============================================================================

# Ontology Metadata
sparql: a owl:Ontology ;
    rdfs:label "SPARQL CLI Ontology" ;
    rdfs:comment "Ontology for SPARQL query execution, optimization, and federation using clap-noun-verb" ;
    owl:versionInfo "1.0.0" .

# ============================================================================
# NOUN DEFINITIONS
# ============================================================================

# Noun: Query
sparql:Query a cnv:Noun ;
    rdfs:label "query" ;
    rdfs:comment "SPARQL query execution and management" ;
    cnv:hasVerb sparql:execute, sparql:explain, sparql:validate,
                sparql:format, sparql:parse ;
    cnv:supportsFormat "application/sparql-query", "text/turtle", "application/json" .

# Noun: Endpoint
sparql:Endpoint a cnv:Noun ;
    rdfs:label "endpoint" ;
    rdfs:comment "SPARQL endpoint management and testing" ;
    cnv:hasVerb sparql:register, sparql:list, sparql:test,
                sparql:benchmark, sparql:health ;
    cnv:requiresAuth true ;
    cnv:supportsCache true .

# Noun: Federation
sparql:Federation a cnv:Noun ;
    rdfs:label "federation" ;
    rdfs:comment "Federated SPARQL query execution across multiple endpoints" ;
    cnv:hasVerb sparql:merge, sparql:distribute, sparql:cache, sparql:sync ;
    cnv:requiresCoordination true ;
    cnv:supportsParallel true .

# Noun: Optimization
sparql:Optimization a cnv:Noun ;
    rdfs:label "optimization" ;
    rdfs:comment "SPARQL query optimization and analysis" ;
    cnv:hasVerb sparql:analyze, sparql:rewrite, sparql:index, sparql:stats ;
    cnv:requiresStatistics true ;
    cnv:supportsCostModel true .

# ============================================================================
# QUERY VERBS
# ============================================================================

# Verb: execute
sparql:execute a cnv:Verb ;
    rdfs:label "execute" ;
    rdfs:comment "Execute a SPARQL query against an endpoint" ;
    cnv:hasNoun sparql:Query ;
    cnv:hasArgument sparql:queryArg, sparql:endpointArg, sparql:formatArg,
                    sparql:timeoutArg, sparql:limitArg ;
    cnv:hasOption sparql:namedGraphOpt, sparql:defaultGraphOpt, sparql:inferenceOpt ;
    cnv:returns sparql:QueryResult ;
    cnv:httpMethod "POST" ;
    cnv:cacheable true .

sparql:queryArg a cnv:Argument ;
    rdfs:label "--query" ;
    rdfs:comment "SPARQL query string or file path" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string ;
    cnv:example "SELECT * WHERE { ?s ?p ?o } LIMIT 10" .

sparql:endpointArg a cnv:Argument ;
    rdfs:label "--endpoint" ;
    rdfs:comment "SPARQL endpoint URL" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:anyURI ;
    cnv:example "http://dbpedia.org/sparql" .

sparql:formatArg a cnv:Argument ;
    rdfs:label "--format" ;
    rdfs:comment "Output format (json, xml, csv, tsv, turtle)" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "json", "xml", "csv", "tsv", "turtle" ;
    cnv:defaultValue "json" .

sparql:timeoutArg a cnv:Argument ;
    rdfs:label "--timeout" ;
    rdfs:comment "Query timeout in seconds" ;
    cnv:argumentType "integer" ;
    cnv:required false ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "30" ;
    cnv:minValue "1" ;
    cnv:maxValue "300" .

sparql:limitArg a cnv:Argument ;
    rdfs:label "--limit" ;
    rdfs:comment "Maximum number of results" ;
    cnv:argumentType "integer" ;
    cnv:required false ;
    cnv:valueType xsd:integer ;
    cnv:minValue "1" .

sparql:namedGraphOpt a cnv:Option ;
    rdfs:label "--named-graph" ;
    rdfs:comment "Named graph URI for query context" ;
    cnv:valueType xsd:anyURI ;
    cnv:multiValue true .

sparql:defaultGraphOpt a cnv:Option ;
    rdfs:label "--default-graph" ;
    rdfs:comment "Default graph URI for query context" ;
    cnv:valueType xsd:anyURI ;
    cnv:multiValue true .

sparql:inferenceOpt a cnv:Option ;
    rdfs:label "--inference" ;
    rdfs:comment "Enable RDFS/OWL inference" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# Verb: explain
sparql:explain a cnv:Verb ;
    rdfs:label "explain" ;
    rdfs:comment "Show query execution plan and optimization steps" ;
    cnv:hasNoun sparql:Query ;
    cnv:hasArgument sparql:queryArg, sparql:detailLevelArg ;
    cnv:hasOption sparql:showCostOpt, sparql:showStatsOpt ;
    cnv:returns sparql:ExplainPlan ;
    cnv:requiresOptimizer true .

sparql:detailLevelArg a cnv:Argument ;
    rdfs:label "--detail" ;
    rdfs:comment "Detail level (basic, full, verbose)" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "basic", "full", "verbose" ;
    cnv:defaultValue "full" .

sparql:showCostOpt a cnv:Option ;
    rdfs:label "--show-cost" ;
    rdfs:comment "Show estimated query execution cost" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:showStatsOpt a cnv:Option ;
    rdfs:label "--show-stats" ;
    rdfs:comment "Show statistics for each operation" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# Verb: validate
sparql:validate a cnv:Verb ;
    rdfs:label "validate" ;
    rdfs:comment "Validate SPARQL query syntax and semantics" ;
    cnv:hasNoun sparql:Query ;
    cnv:hasArgument sparql:queryArg, sparql:strictModeArg ;
    cnv:hasOption sparql:checkVocabOpt, sparql:warnDeprecatedOpt ;
    cnv:returns sparql:ValidationResult ;
    cnv:requiresParser true .

sparql:strictModeArg a cnv:Argument ;
    rdfs:label "--strict" ;
    rdfs:comment "Enable strict SPARQL 1.1 compliance checking" ;
    cnv:argumentType "boolean" ;
    cnv:required false ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

sparql:checkVocabOpt a cnv:Option ;
    rdfs:label "--check-vocabulary" ;
    rdfs:comment "Check if predicates exist in endpoint" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

sparql:warnDeprecatedOpt a cnv:Option ;
    rdfs:label "--warn-deprecated" ;
    rdfs:comment "Warn about deprecated SPARQL features" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

# Verb: format
sparql:format a cnv:Verb ;
    rdfs:label "format" ;
    rdfs:comment "Format and beautify SPARQL query" ;
    cnv:hasNoun sparql:Query ;
    cnv:hasArgument sparql:queryArg, sparql:indentArg ;
    cnv:hasOption sparql:uppercaseOpt, sparql:alignOpt ;
    cnv:returns sparql:FormattedQuery .

sparql:indentArg a cnv:Argument ;
    rdfs:label "--indent" ;
    rdfs:comment "Number of spaces for indentation" ;
    cnv:argumentType "integer" ;
    cnv:required false ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "2" ;
    cnv:minValue "0" ;
    cnv:maxValue "8" .

sparql:uppercaseOpt a cnv:Option ;
    rdfs:label "--uppercase-keywords" ;
    rdfs:comment "Use uppercase for SPARQL keywords" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:alignOpt a cnv:Option ;
    rdfs:label "--align" ;
    rdfs:comment "Align triple patterns vertically" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

# Verb: parse
sparql:parse a cnv:Verb ;
    rdfs:label "parse" ;
    rdfs:comment "Parse SPARQL query into abstract syntax tree" ;
    cnv:hasNoun sparql:Query ;
    cnv:hasArgument sparql:queryArg, sparql:outputFormatArg ;
    cnv:returns sparql:ParseTree .

sparql:outputFormatArg a cnv:Argument ;
    rdfs:label "--output" ;
    rdfs:comment "Output format (json, yaml, sexp)" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "json", "yaml", "sexp" ;
    cnv:defaultValue "json" .

# ============================================================================
# ENDPOINT VERBS
# ============================================================================

# Verb: register
sparql:register a cnv:Verb ;
    rdfs:label "register" ;
    rdfs:comment "Register a SPARQL endpoint for reuse" ;
    cnv:hasNoun sparql:Endpoint ;
    cnv:hasArgument sparql:nameArg, sparql:urlArg, sparql:descriptionArg ;
    cnv:hasOption sparql:authOpt, sparql:defaultOpt ;
    cnv:returns sparql:EndpointConfig ;
    cnv:persistsConfig true .

sparql:nameArg a cnv:Argument ;
    rdfs:label "--name" ;
    rdfs:comment "Endpoint name/alias" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string ;
    cnv:pattern "^[a-zA-Z0-9_-]+$" .

sparql:urlArg a cnv:Argument ;
    rdfs:label "--url" ;
    rdfs:comment "Endpoint URL" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:anyURI .

sparql:descriptionArg a cnv:Argument ;
    rdfs:label "--description" ;
    rdfs:comment "Endpoint description" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string .

sparql:authOpt a cnv:Option ;
    rdfs:label "--auth" ;
    rdfs:comment "Authentication method (basic, bearer, digest)" ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "basic", "bearer", "digest", "none" ;
    cnv:defaultValue "none" .

sparql:defaultOpt a cnv:Option ;
    rdfs:label "--set-default" ;
    rdfs:comment "Set as default endpoint" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# Verb: list
sparql:list a cnv:Verb ;
    rdfs:label "list" ;
    rdfs:comment "List all registered endpoints" ;
    cnv:hasNoun sparql:Endpoint ;
    cnv:hasArgument sparql:filterArg ;
    cnv:hasOption sparql:verboseOpt ;
    cnv:returns sparql:EndpointList .

sparql:filterArg a cnv:Argument ;
    rdfs:label "--filter" ;
    rdfs:comment "Filter endpoints by name pattern" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string .

sparql:verboseOpt a cnv:Option ;
    rdfs:label "--verbose" ;
    rdfs:comment "Show detailed endpoint information" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# Verb: test
sparql:test a cnv:Verb ;
    rdfs:label "test" ;
    rdfs:comment "Test endpoint connectivity and response" ;
    cnv:hasNoun sparql:Endpoint ;
    cnv:hasArgument sparql:endpointArg ;
    cnv:hasOption sparql:retriesOpt, sparql:timeoutTestOpt ;
    cnv:returns sparql:TestResult .

sparql:retriesOpt a cnv:Option ;
    rdfs:label "--retries" ;
    rdfs:comment "Number of retry attempts" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "3" ;
    cnv:minValue "0" ;
    cnv:maxValue "10" .

sparql:timeoutTestOpt a cnv:Option ;
    rdfs:label "--timeout" ;
    rdfs:comment "Connection timeout in seconds" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "10" .

# Verb: benchmark
sparql:benchmark a cnv:Verb ;
    rdfs:label "benchmark" ;
    rdfs:comment "Benchmark endpoint performance with test queries" ;
    cnv:hasNoun sparql:Endpoint ;
    cnv:hasArgument sparql:endpointArg, sparql:iterationsArg ;
    cnv:hasOption sparql:warmupOpt, sparql:concurrentOpt ;
    cnv:returns sparql:BenchmarkResult .

sparql:iterationsArg a cnv:Argument ;
    rdfs:label "--iterations" ;
    rdfs:comment "Number of benchmark iterations" ;
    cnv:argumentType "integer" ;
    cnv:required false ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "100" ;
    cnv:minValue "1" .

sparql:warmupOpt a cnv:Option ;
    rdfs:label "--warmup" ;
    rdfs:comment "Number of warmup iterations" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "10" .

sparql:concurrentOpt a cnv:Option ;
    rdfs:label "--concurrent" ;
    rdfs:comment "Number of concurrent requests" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "1" ;
    cnv:minValue "1" ;
    cnv:maxValue "100" .

# Verb: health
sparql:health a cnv:Verb ;
    rdfs:label "health" ;
    rdfs:comment "Check endpoint health status" ;
    cnv:hasNoun sparql:Endpoint ;
    cnv:hasArgument sparql:endpointArg ;
    cnv:returns sparql:HealthStatus .

# ============================================================================
# FEDERATION VERBS
# ============================================================================

# Verb: merge
sparql:merge a cnv:Verb ;
    rdfs:label "merge" ;
    rdfs:comment "Merge results from multiple federated queries" ;
    cnv:hasNoun sparql:Federation ;
    cnv:hasArgument sparql:queryArg, sparql:endpointsArg, sparql:strategyArg ;
    cnv:hasOption sparql:deduplicateOpt, sparql:orderByOpt ;
    cnv:returns sparql:MergedResult ;
    cnv:requiresCoordinator true .

sparql:endpointsArg a cnv:Argument ;
    rdfs:label "--endpoints" ;
    rdfs:comment "Comma-separated list of endpoint names or URLs" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string ;
    cnv:multiValue true ;
    cnv:separator "," .

sparql:strategyArg a cnv:Argument ;
    rdfs:label "--strategy" ;
    rdfs:comment "Federation strategy" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "hash", "round-robin", "cost-based", "priority" ;
    cnv:defaultValue "cost-based" .

sparql:deduplicateOpt a cnv:Option ;
    rdfs:label "--deduplicate" ;
    rdfs:comment "Remove duplicate results" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:orderByOpt a cnv:Option ;
    rdfs:label "--order-by" ;
    rdfs:comment "Variable to order merged results" ;
    cnv:valueType xsd:string .

# Verb: distribute
sparql:distribute a cnv:Verb ;
    rdfs:label "distribute" ;
    rdfs:comment "Distribute query execution across endpoints" ;
    cnv:hasNoun sparql:Federation ;
    cnv:hasArgument sparql:queryArg, sparql:endpointsArg ;
    cnv:hasOption sparql:parallelOpt, sparql:failoverOpt ;
    cnv:returns sparql:DistributedResult .

sparql:parallelOpt a cnv:Option ;
    rdfs:label "--parallel" ;
    rdfs:comment "Execute queries in parallel" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:failoverOpt a cnv:Option ;
    rdfs:label "--failover" ;
    rdfs:comment "Enable automatic failover on endpoint failure" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

# Verb: cache
sparql:cache a cnv:Verb ;
    rdfs:label "cache" ;
    rdfs:comment "Manage federation result cache" ;
    cnv:hasNoun sparql:Federation ;
    cnv:hasArgument sparql:actionArg, sparql:cacheKeyArg ;
    cnv:hasOption sparql:ttlOpt, sparql:maxSizeOpt ;
    cnv:returns sparql:CacheStatus .

sparql:actionArg a cnv:Argument ;
    rdfs:label "--action" ;
    rdfs:comment "Cache action (get, set, clear, stats)" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "get", "set", "clear", "stats" .

sparql:cacheKeyArg a cnv:Argument ;
    rdfs:label "--key" ;
    rdfs:comment "Cache key (query hash)" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:string .

sparql:ttlOpt a cnv:Option ;
    rdfs:label "--ttl" ;
    rdfs:comment "Cache TTL in seconds" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "3600" .

sparql:maxSizeOpt a cnv:Option ;
    rdfs:label "--max-size" ;
    rdfs:comment "Maximum cache size in MB" ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "100" .

# Verb: sync
sparql:sync a cnv:Verb ;
    rdfs:label "sync" ;
    rdfs:comment "Synchronize data across federated endpoints" ;
    cnv:hasNoun sparql:Federation ;
    cnv:hasArgument sparql:sourceArg, sparql:targetArg ;
    cnv:hasOption sparql:incrementalOpt, sparql:bidirectionalOpt ;
    cnv:returns sparql:SyncResult .

sparql:sourceArg a cnv:Argument ;
    rdfs:label "--source" ;
    rdfs:comment "Source endpoint name" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string .

sparql:targetArg a cnv:Argument ;
    rdfs:label "--target" ;
    rdfs:comment "Target endpoint name" ;
    cnv:argumentType "string" ;
    cnv:required true ;
    cnv:valueType xsd:string .

sparql:incrementalOpt a cnv:Option ;
    rdfs:label "--incremental" ;
    rdfs:comment "Sync only new/modified triples" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:bidirectionalOpt a cnv:Option ;
    rdfs:label "--bidirectional" ;
    rdfs:comment "Enable bidirectional synchronization" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# ============================================================================
# OPTIMIZATION VERBS
# ============================================================================

# Verb: analyze
sparql:analyze a cnv:Verb ;
    rdfs:label "analyze" ;
    rdfs:comment "Analyze query performance characteristics" ;
    cnv:hasNoun sparql:Optimization ;
    cnv:hasArgument sparql:queryArg ;
    cnv:hasOption sparql:collectStatsOpt, sparql:estimateCostOpt ;
    cnv:returns sparql:AnalysisReport ;
    cnv:requiresStatistics true .

sparql:collectStatsOpt a cnv:Option ;
    rdfs:label "--collect-stats" ;
    rdfs:comment "Collect execution statistics" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:estimateCostOpt a cnv:Option ;
    rdfs:label "--estimate-cost" ;
    rdfs:comment "Estimate query execution cost" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

# Verb: rewrite
sparql:rewrite a cnv:Verb ;
    rdfs:label "rewrite" ;
    rdfs:comment "Rewrite query for better performance" ;
    cnv:hasNoun sparql:Optimization ;
    cnv:hasArgument sparql:queryArg, sparql:optimizationLevelArg ;
    cnv:hasOption sparql:preserveSemanticsOpt, sparql:aggressiveOpt ;
    cnv:returns sparql:RewrittenQuery .

sparql:optimizationLevelArg a cnv:Argument ;
    rdfs:label "--level" ;
    rdfs:comment "Optimization level (1-3)" ;
    cnv:argumentType "integer" ;
    cnv:required false ;
    cnv:valueType xsd:integer ;
    cnv:defaultValue "2" ;
    cnv:minValue "1" ;
    cnv:maxValue "3" .

sparql:preserveSemanticsOpt a cnv:Option ;
    rdfs:label "--preserve-semantics" ;
    rdfs:comment "Ensure semantic equivalence" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "true" .

sparql:aggressiveOpt a cnv:Option ;
    rdfs:label "--aggressive" ;
    rdfs:comment "Enable aggressive optimizations" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# Verb: index
sparql:index a cnv:Verb ;
    rdfs:label "index" ;
    rdfs:comment "Manage indexes for query optimization" ;
    cnv:hasNoun sparql:Optimization ;
    cnv:hasArgument sparql:actionArg, sparql:predicateArg ;
    cnv:hasOption sparql:indexTypeOpt ;
    cnv:returns sparql:IndexStatus .

sparql:predicateArg a cnv:Argument ;
    rdfs:label "--predicate" ;
    rdfs:comment "Predicate URI to index" ;
    cnv:argumentType "string" ;
    cnv:required false ;
    cnv:valueType xsd:anyURI .

sparql:indexTypeOpt a cnv:Option ;
    rdfs:label "--type" ;
    rdfs:comment "Index type (btree, hash, full-text)" ;
    cnv:valueType xsd:string ;
    cnv:allowedValues "btree", "hash", "full-text" ;
    cnv:defaultValue "btree" .

# Verb: stats
sparql:stats a cnv:Verb ;
    rdfs:label "stats" ;
    rdfs:comment "Collect and display query statistics" ;
    cnv:hasNoun sparql:Optimization ;
    cnv:hasArgument sparql:endpointArg ;
    cnv:hasOption sparql:refreshOpt, sparql:histogramOpt ;
    cnv:returns sparql:Statistics .

sparql:refreshOpt a cnv:Option ;
    rdfs:label "--refresh" ;
    rdfs:comment "Refresh statistics from endpoint" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

sparql:histogramOpt a cnv:Option ;
    rdfs:label "--histogram" ;
    rdfs:comment "Include value distribution histograms" ;
    cnv:valueType xsd:boolean ;
    cnv:defaultValue "false" .

# ============================================================================
# RETURN TYPES
# ============================================================================

sparql:QueryResult a cnv:ReturnType ;
    rdfs:label "QueryResult" ;
    rdfs:comment "SPARQL query execution result" ;
    cnv:hasField sparql:bindings, sparql:head, sparql:metadata .

sparql:ExplainPlan a cnv:ReturnType ;
    rdfs:label "ExplainPlan" ;
    rdfs:comment "Query execution plan with optimization details" ;
    cnv:hasField sparql:plan, sparql:estimatedCost, sparql:operators .

sparql:ValidationResult a cnv:ReturnType ;
    rdfs:label "ValidationResult" ;
    rdfs:comment "Query validation result with errors and warnings" ;
    cnv:hasField sparql:isValid, sparql:errors, sparql:warnings .

sparql:FormattedQuery a cnv:ReturnType ;
    rdfs:label "FormattedQuery" ;
    rdfs:comment "Formatted SPARQL query string" ;
    cnv:hasField sparql:query, sparql:lines .

sparql:ParseTree a cnv:ReturnType ;
    rdfs:label "ParseTree" ;
    rdfs:comment "Abstract syntax tree of parsed query" ;
    cnv:hasField sparql:ast, sparql:queryType, sparql:variables .

sparql:EndpointConfig a cnv:ReturnType ;
    rdfs:label "EndpointConfig" ;
    rdfs:comment "Registered endpoint configuration" ;
    cnv:hasField sparql:name, sparql:url, sparql:auth .

sparql:EndpointList a cnv:ReturnType ;
    rdfs:label "EndpointList" ;
    rdfs:comment "List of registered endpoints" ;
    cnv:hasField sparql:endpoints, sparql:count .

sparql:TestResult a cnv:ReturnType ;
    rdfs:label "TestResult" ;
    rdfs:comment "Endpoint connectivity test result" ;
    cnv:hasField sparql:success, sparql:latency, sparql:error .

sparql:BenchmarkResult a cnv:ReturnType ;
    rdfs:label "BenchmarkResult" ;
    rdfs:comment "Endpoint performance benchmark results" ;
    cnv:hasField sparql:avgLatency, sparql:p95, sparql:p99, sparql:throughput .

sparql:HealthStatus a cnv:ReturnType ;
    rdfs:label "HealthStatus" ;
    rdfs:comment "Endpoint health status" ;
    cnv:hasField sparql:healthy, sparql:version, sparql:uptime .

sparql:MergedResult a cnv:ReturnType ;
    rdfs:label "MergedResult" ;
    rdfs:comment "Merged results from federated queries" ;
    cnv:hasField sparql:results, sparql:sources, sparql:mergeTime .

sparql:DistributedResult a cnv:ReturnType ;
    rdfs:label "DistributedResult" ;
    rdfs:comment "Results from distributed query execution" ;
    cnv:hasField sparql:results, sparql:timing, sparql:distribution .

sparql:CacheStatus a cnv:ReturnType ;
    rdfs:label "CacheStatus" ;
    rdfs:comment "Cache operation status" ;
    cnv:hasField sparql:hit, sparql:size, sparql:entries .

sparql:SyncResult a cnv:ReturnType ;
    rdfs:label "SyncResult" ;
    rdfs:comment "Data synchronization result" ;
    cnv:hasField sparql:synced, sparql:added, sparql:modified, sparql:deleted .

sparql:AnalysisReport a cnv:ReturnType ;
    rdfs:label "AnalysisReport" ;
    rdfs:comment "Query performance analysis report" ;
    cnv:hasField sparql:complexity, sparql:selectivity, sparql:joinCost .

sparql:RewrittenQuery a cnv:ReturnType ;
    rdfs:label "RewrittenQuery" ;
    rdfs:comment "Optimized query after rewriting" ;
    cnv:hasField sparql:original, sparql:rewritten, sparql:improvements .

sparql:IndexStatus a cnv:ReturnType ;
    rdfs:label "IndexStatus" ;
    rdfs:comment "Index operation status" ;
    cnv:hasField sparql:indexed, sparql:count, sparql:size .

sparql:Statistics a cnv:ReturnType ;
    rdfs:label "Statistics" ;
    rdfs:comment "Query and endpoint statistics" ;
    cnv:hasField sparql:tripleCount, sparql:distinctSubjects, sparql:predicateStats .
