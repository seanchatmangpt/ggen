bank:description a owl:DatatypeProperty ;
    rdfs:domain bank:Transaction ;
    rdfs:range xsd:string ;
    rdfs:label "description" .

bank:transactionType a owl:DatatypeProperty ;
    rdfs:domain bank:Transaction ;
    rdfs:range xsd:string ;
    rdfs:label "transaction type" .

bank:routingNumber a owl:DatatypeProperty ;
    rdfs:domain bank:ACHTransfer ;
    rdfs:range xsd:string ;
