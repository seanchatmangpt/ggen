bank:accruedInterest a owl:DatatypeProperty ;
    rdfs:domain bank:InterestAccrual ;
    rdfs:range xsd:decimal ;
    rdfs:label "accrued interest" .

bank:limitAmount a owl:DatatypeProperty ;
    rdfs:domain bank:Limit ;
    rdfs:range xsd:decimal ;
    rdfs:label "limit amount" .

bank:limitType a owl:DatatypeProperty ;
    rdfs:domain bank:Limit ;
    rdfs:range xsd:string ;
    rdfs:label "limit type" .

bank:reportId a owl:DatatypeProperty ;
    rdfs:domain bank:RegulatoryReport ;
    rdfs:range xsd:string ;
