---
to: "lib/definitions/index.mjs"
description: Index of all workflow specifications
vars:
  specId: string
metadata:
  category: definitions
---
{# =============================================================================
   WORKFLOW INDEX TEMPLATE
   DO NOT EDIT - auto-generated by ggen sync
   ============================================================================= #}
/**
 * @fileoverview YAWL Workflow Definitions Index
 * DO NOT EDIT - auto-generated by ggen sync
 * @module definitions
 */

export * from './tasks.mjs';
export * from './flows.mjs';

export const WORKFLOW_INDEX = [
{%- for row in sparql_results %}
{%- set specId = row["?specId"] | default(value="") %}
{%- set specName = row["?specName"] | default(value="") %}
{%- set specVersion = row["?specVersion"] | default(value="1.0.0") %}
{%- set specComment = row["?specComment"] | default(value="") %}
{%- set taskCount = row["?taskCount"] | default(value="0") %}
{%- set flowCount = row["?flowCount"] | default(value="0") %}
{%- if specId != "" %}
  {
    specId: '{{ specId }}',
    specName: '{{ specName }}',
    version: '{{ specVersion }}',
{%- if specComment != "" %}
    description: '{{ specComment | replace(from="'", to="\\'") }}',
{%- endif %}
    taskCount: {{ taskCount }},
    flowCount: {{ flowCount }},
  },
{%- endif %}
{%- endfor %}
];

/**
 * @param {string} specId
 * @returns {object|undefined}
 */
export function getWorkflowSummary(specId) {
  return WORKFLOW_INDEX.find(w => w.specId === specId);
}

/**
 * @returns {string[]}
 */
export function listWorkflowIds() {
  return WORKFLOW_INDEX.map(w => w.specId);
}
