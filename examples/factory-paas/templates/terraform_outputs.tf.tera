# Generated Terraform Outputs - DO NOT EDIT
#
# This file is automatically generated by ggen from ontology/infra.ttl
# To make changes, edit the ontology and run: ggen sync

{% for row in sparql_results -%}
{%- set resource_name = row["?resourceName"] -%}
{%- set provider = row["?provider"] -%}

{% if provider == "oci" %}
output "{{ resource_name | replace(from="-", to="_") }}_public_ip" {
  description = "Public IP of {{ resource_name }}"
  value       = oci_core_instance.{{ resource_name | replace(from="-", to="_") }}.public_ip
}

output "{{ resource_name | replace(from="-", to="_") }}_private_ip" {
  description = "Private IP of {{ resource_name }}"
  value       = oci_core_instance.{{ resource_name | replace(from="-", to="_") }}.private_ip
}
{% endif %}

{% endfor %}
