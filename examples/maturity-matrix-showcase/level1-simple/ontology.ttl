@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ggen: <http://ggen.io/ontology#> .
@prefix ex: <http://example.org/maturity-matrix/level1#> .

# Level 1: Simple Ontology (< 100 triples)
# Demonstrates basic RDF structure for simple file generation

ex:User a rdfs:Class ;
    rdfs:label "User" ;
    rdfs:comment "A simple user class" .

ex:userName a rdf:Property ;
    rdfs:domain ex:User ;
    rdfs:range rdfs:Literal ;
    rdfs:label "User Name" .

ex:userEmail a rdf:Property ;
    rdfs:domain ex:User ;
    rdfs:range rdfs:Literal ;
    rdfs:label "User Email" .

# Simple example instance
ex:alice a ex:User ;
    ex:userName "Alice" ;
    ex:userEmail "alice@example.com" .

