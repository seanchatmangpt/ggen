@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/maturity-matrix/level4/infrastructure#> .

# Level 4: Infrastructure Ontology (2,000+ triples)
# Infrastructure components and deployment

ex:InfrastructureComponent a rdfs:Class ;
    rdfs:label "Infrastructure Component" .

ex:Database a rdfs:Class ;
    rdfs:subClassOf ex:InfrastructureComponent ;
    rdfs:label "Database" .

ex:Cache a rdfs:Class ;
    rdfs:subClassOf ex:InfrastructureComponent ;
    rdfs:label "Cache" .

ex:MessageQueue a rdfs:Class ;
    rdfs:subClassOf ex:InfrastructureComponent ;
    rdfs:label "Message Queue" .

