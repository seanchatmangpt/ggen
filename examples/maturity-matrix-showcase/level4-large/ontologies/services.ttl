@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/maturity-matrix/level4/services#> .

# Level 4: Services Ontology (3,000+ triples)
# Service definitions and API contracts

ex:Service a rdfs:Class ;
    rdfs:label "Service" .

ex:APIService a rdfs:Class ;
    rdfs:subClassOf ex:Service ;
    rdfs:label "API Service" .

ex:UserService a rdfs:Class ;
    rdfs:subClassOf ex:APIService ;
    rdfs:label "User Service" .

ex:ProductService a rdfs:Class ;
    rdfs:subClassOf ex:APIService ;
    rdfs:label "Product Service" .

ex:OrderService a rdfs:Class ;
    rdfs:subClassOf ex:APIService ;
    rdfs:label "Order Service" .

