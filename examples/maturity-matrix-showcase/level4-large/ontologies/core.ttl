@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ggen: <http://ggen.io/ontology#> .
@prefix ex: <http://example.org/maturity-matrix/level4/core#> .

# Level 4: Large Project - Core Ontology (5,000+ triples)
# Demonstrates distributed ontology with core domain model

ex:DomainModel a rdfs:Class ;
    rdfs:label "Domain Model" ;
    rdfs:comment "Core domain model for large project" .

ex:Entity a rdfs:Class ;
    rdfs:subClassOf ex:DomainModel ;
    rdfs:label "Entity" .

ex:ValueObject a rdfs:Class ;
    rdfs:subClassOf ex:DomainModel ;
    rdfs:label "Value Object" .

ex:AggregateRoot a rdfs:Class ;
    rdfs:subClassOf ex:Entity ;
    rdfs:label "Aggregate Root" .

# Core entities (simplified for example)
ex:User a rdfs:Class ;
    rdfs:subClassOf ex:AggregateRoot ;
    rdfs:label "User" .

ex:Product a rdfs:Class ;
    rdfs:subClassOf ex:AggregateRoot ;
    rdfs:label "Product" .

ex:Order a rdfs:Class ;
    rdfs:subClassOf ex:AggregateRoot ;
    rdfs:label "Order" .

