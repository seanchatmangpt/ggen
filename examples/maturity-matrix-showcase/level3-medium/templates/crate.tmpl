---
to: generated/{{ crate_name }}/Cargo.toml
rdf:
  - ontology/domain.ttl
sparql:
  crate_info: |
    PREFIX ex: <http://example.org/maturity-matrix/level3#>
    SELECT ?name ?path ?deps WHERE {
      ?crate a ex:Crate .
      ?crate ex:crateName ?name .
      ?crate ex:cratePath ?path .
      OPTIONAL { ?crate ex:crateDependencies ?deps }
    }
    ORDER BY ?name
vars:
  crate_name: core
---

# Level 3: Crate Cargo.toml
# AUTO-GENERATED from ontology - DO NOT EDIT

[package]
name = "{{ crate_name }}"
version = "1.0.0"
edition = "2021"

[dependencies]
{% for row in sparql_results.crate_info %}
{% if row.deps %}
{{ row.deps }}
{% endif %}
{% endfor %}

