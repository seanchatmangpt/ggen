@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix chatman: <http://ggen.ai/ontology/chatman#> .
@prefix ggen: <http://ggen.ai/ontology/core#> .

# Ontology Definition
chatman:Ontology a owl:Ontology ;
    rdfs:label "ChatMan CLI Ontology" ;
    rdfs:comment "Semantic model for chat automation and conversation management" ;
    owl:versionInfo "1.0.0" ;
    ggen:generatedBy "ggen/chatman-cli" .

# Core Classes
chatman:Conversation a owl:Class ;
    rdfs:label "Conversation" ;
    rdfs:comment "A conversation session containing messages and context" .

chatman:Message a owl:Class ;
    rdfs:label "Message" ;
    rdfs:comment "A single message in a conversation" .

chatman:User a owl:Class ;
    rdfs:label "User" ;
    rdfs:comment "A user participating in conversations" .

chatman:Assistant a owl:Class ;
    rdfs:label "Assistant" ;
    rdfs:comment "An AI assistant responding to users" .

chatman:KnowledgeHook a owl:Class ;
    rdfs:label "Knowledge Hook" ;
    rdfs:comment "A semantic knowledge integration point" .

chatman:AIProvider a owl:Class ;
    rdfs:label "AI Provider" ;
    rdfs:comment "An AI service provider (OpenAI, Anthropic, etc.)" .

chatman:ConversationContext a owl:Class ;
    rdfs:label "Conversation Context" ;
    rdfs:comment "Contextual information about a conversation" .

chatman:Session a owl:Class ;
    rdfs:label "Session" ;
    rdfs:comment "A chat session with persistence" .

# Message Types
chatman:UserMessage a owl:Class ;
    rdfs:subClassOf chatman:Message ;
    rdfs:label "User Message" ;
    rdfs:comment "A message from a user" .

chatman:AssistantMessage a owl:Class ;
    rdfs:subClassOf chatman:Message ;
    rdfs:label "Assistant Message" ;
    rdfs:comment "A message from an AI assistant" .

chatman:SystemMessage a owl:Class ;
    rdfs:subClassOf chatman:Message ;
    rdfs:label "System Message" ;
    rdfs:comment "A system-level message" .

# Properties
chatman:hasMessage a owl:ObjectProperty ;
    rdfs:label "has message" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range chatman:Message .

chatman:hasContent a owl:DatatypeProperty ;
    rdfs:label "has content" ;
    rdfs:domain chatman:Message ;
    rdfs:range xsd:string .

chatman:hasRole a owl:DatatypeProperty ;
    rdfs:label "has role" ;
    rdfs:domain chatman:Message ;
    rdfs:range xsd:string .

chatman:hasTimestamp a owl:DatatypeProperty ;
    rdfs:label "has timestamp" ;
    rdfs:domain chatman:Message ;
    rdfs:range xsd:dateTime .

chatman:hasParticipant a owl:ObjectProperty ;
    rdfs:label "has participant" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range [ owl:unionOf (chatman:User chatman:Assistant) ] .

chatman:hasContext a owl:ObjectProperty ;
    rdfs:label "has context" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range chatman:ConversationContext .

chatman:usesProvider a owl:ObjectProperty ;
    rdfs:label "uses provider" ;
    rdfs:domain chatman:Assistant ;
    rdfs:range chatman:AIProvider .

chatman:hasKnowledgeHook a owl:ObjectProperty ;
    rdfs:label "has knowledge hook" ;
    rdfs:domain chatman:Assistant ;
    rdfs:range chatman:KnowledgeHook .

chatman:belongsToSession a owl:ObjectProperty ;
    rdfs:label "belongs to session" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range chatman:Session .

# Provider Instances
chatman:OpenAI a chatman:AIProvider ;
    rdfs:label "OpenAI" ;
    chatman:hasModel "gpt-3.5-turbo", "gpt-4", "gpt-4-turbo" ;
    chatman:supportsStreaming true .

chatman:Anthropic a chatman:AIProvider ;
    rdfs:label "Anthropic" ;
    chatman:hasModel "claude-3-opus", "claude-3-sonnet", "claude-3-haiku" ;
    chatman:supportsStreaming true .

chatman:LocalLLM a chatman:AIProvider ;
    rdfs:label "Local LLM" ;
    chatman:hasModel "llama-2", "mistral", "custom" ;
    chatman:supportsStreaming false .

# Conversation Patterns
chatman:InteractivePattern a owl:Class ;
    rdfs:subClassOf chatman:ConversationContext ;
    rdfs:label "Interactive Pattern" ;
    rdfs:comment "Real-time interactive conversation pattern" .

chatman:BatchPattern a owl:Class ;
    rdfs:subClassOf chatman:ConversationContext ;
    rdfs:label "Batch Pattern" ;
    rdfs:comment "Batch processing conversation pattern" .

chatman:StreamingPattern a owl:Class ;
    rdfs:subClassOf chatman:ConversationContext ;
    rdfs:label "Streaming Pattern" ;
    rdfs:comment "Real-time streaming conversation pattern" .

chatman:ScriptedPattern a owl:Class ;
    rdfs:subClassOf chatman:ConversationContext ;
    rdfs:label "Scripted Pattern" ;
    rdfs:comment "Pre-scripted conversation pattern" .

# Data Properties
chatman:hasModel a owl:DatatypeProperty ;
    rdfs:label "has model" ;
    rdfs:domain chatman:AIProvider ;
    rdfs:range xsd:string .

chatman:supportsStreaming a owl:DatatypeProperty ;
    rdfs:label "supports streaming" ;
    rdfs:domain chatman:AIProvider ;
    rdfs:range xsd:boolean .

chatman:hasMaxHistory a owl:DatatypeProperty ;
    rdfs:label "has max history" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range xsd:integer .

chatman:hasTimeout a owl:DatatypeProperty ;
    rdfs:label "has timeout" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range xsd:integer .

chatman:hasRetryAttempts a owl:DatatypeProperty ;
    rdfs:label "has retry attempts" ;
    rdfs:domain chatman:Conversation ;
    rdfs:range xsd:integer .

# Configuration
chatman:Configuration a owl:Class ;
    rdfs:label "Configuration" ;
    rdfs:comment "Configuration settings for ChatMan CLI" .

chatman:hasConfiguration a owl:ObjectProperty ;
    rdfs:label "has configuration" ;
    rdfs:domain chatman:Session ;
    rdfs:range chatman:Configuration .

# Quality Attributes
chatman:hasQuality a owl:ObjectProperty ;
    rdfs:label "has quality" ;
    rdfs:domain chatman:Message ;
    rdfs:range chatman:MessageQuality .

chatman:MessageQuality a owl:Class ;
    rdfs:label "Message Quality" ;
    rdfs:comment "Quality attributes of a message" .

chatman:hasRelevance a owl:DatatypeProperty ;
    rdfs:label "has relevance" ;
    rdfs:domain chatman:MessageQuality ;
    rdfs:range xsd:float .

chatman:hasCoherence a owl:DatatypeProperty ;
    rdfs:label "has coherence" ;
    rdfs:domain chatman:MessageQuality ;
    rdfs:range xsd:float .

chatman:hasSentiment a owl:DatatypeProperty ;
    rdfs:label "has sentiment" ;
    rdfs:domain chatman:MessageQuality ;
    rdfs:range xsd:string .

# Export Formats
chatman:ExportFormat a owl:Class ;
    rdfs:label "Export Format" ;
    rdfs:comment "Format for exporting conversations" .

chatman:JSONFormat a chatman:ExportFormat ;
    rdfs:label "JSON Format" .

chatman:YAMLFormat a chatman:ExportFormat ;
    rdfs:label "YAML Format" .

chatman:MarkdownFormat a chatman:ExportFormat ;
    rdfs:label "Markdown Format" .

chatman:TextFormat a chatman:ExportFormat ;
    rdfs:label "Text Format" .
