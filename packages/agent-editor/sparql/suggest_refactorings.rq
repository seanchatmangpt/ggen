PREFIX ed: <http://ggen.io/ontology/editor#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# Query: Get refactoring suggestions with confidence scores
SELECT ?refactoring ?type ?file ?confidence ?description
WHERE {
  ?operation a ed:Refactoring ;
             ed:refactoringType ?type ;
             ed:targetFile ?file ;
             ed:confidence ?confidence ;
             rdfs:comment ?description .

  BIND(?operation AS ?refactoring)

  FILTER(?confidence > 0.7)
}
ORDER BY DESC(?confidence)
