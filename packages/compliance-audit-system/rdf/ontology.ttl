# Framework Properties
cas:frameworkName a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceFramework ;
    rdfs:range xsd:string ;
    rdfs:label "framework name" .

cas:frameworkVersion a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceFramework ;
    rdfs:range xsd:string ;
    rdfs:label "framework version" .

cas:requirementId a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceFramework ;
    rdfs:range xsd:string ;
    rdfs:label "requirement ID" .

# Access Review Properties
cas:reviewId a owl:DatatypeProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range xsd:string ;
    rdfs:label "review ID" .

cas:reviewDate a owl:DatatypeProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range xsd:date ;
    rdfs:label "review date" .

cas:dueDate a owl:DatatypeProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range xsd:date ;
    rdfs:label "due date" .

cas:completionDate a owl:DatatypeProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range xsd:date ;
    rdfs:label "completion date" .

cas:reviewStatus a owl:DatatypeProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range xsd:string ;
    rdfs:label "review status" .

cas:decision a owl:DatatypeProperty ;
    rdfs:domain cas:AccessDecision ;
    rdfs:range xsd:string ;
    rdfs:label "decision" .

cas:justification a owl:DatatypeProperty ;
    rdfs:domain cas:AccessDecision ;
    rdfs:range xsd:string ;
    rdfs:label "justification" .

# Risk Properties
cas:riskLevel a owl:DatatypeProperty ;
    rdfs:domain cas:RiskFinding ;
    rdfs:range xsd:string ;
    rdfs:label "risk level" .

cas:likelihood a owl:DatatypeProperty ;
    rdfs:domain cas:RiskFinding ;
    rdfs:range xsd:decimal ;
    rdfs:label "likelihood" .

cas:impact a owl:DatatypeProperty ;
    rdfs:domain cas:RiskFinding ;
    rdfs:range xsd:decimal ;
    rdfs:label "impact" .

cas:residualRisk a owl:DatatypeProperty ;
    rdfs:domain cas:RiskMitigation ;
    rdfs:range xsd:decimal ;
    rdfs:label "residual risk" .

# Incident Properties
cas:incidentId a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range xsd:string ;
    rdfs:label "incident ID" .

cas:severity a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range xsd:string ;
    rdfs:label "severity" .

cas:discoveredAt a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range xsd:dateTime ;
    rdfs:label "discovered at" .

cas:resolvedAt a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range xsd:dateTime ;
    rdfs:label "resolved at" .

cas:affectedRecords a owl:DatatypeProperty ;
    rdfs:domain cas:DataBreach ;
    rdfs:range xsd:integer ;
    rdfs:label "affected records" .

# Evidence Properties
cas:evidenceId a owl:DatatypeProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range xsd:string ;
    rdfs:label "evidence ID" .

cas:evidenceType a owl:DatatypeProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range xsd:string ;
    rdfs:label "evidence type" .

cas:collectedAt a owl:DatatypeProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range xsd:dateTime ;
    rdfs:label "collected at" .

cas:collectedBy a owl:DatatypeProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range xsd:string ;
    rdfs:label "collected by" .

cas:hashValue a owl:DatatypeProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range xsd:string ;
    rdfs:label "hash value" .

# Report Properties
cas:reportId a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceReport ;
    rdfs:range xsd:string ;
    rdfs:label "report ID" .

cas:reportDate a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceReport ;
    rdfs:range xsd:date ;
    rdfs:label "report date" .

cas:reportPeriod a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceReport ;
    rdfs:range xsd:string ;
    rdfs:label "report period" .

cas:complianceScore a owl:DatatypeProperty ;
    rdfs:domain cas:ComplianceMetrics ;
    rdfs:range xsd:decimal ;
    rdfs:label "compliance score" .

# Object Properties
cas:relatedToFramework a owl:ObjectProperty ;
    rdfs:domain cas:Policy ;
    rdfs:range cas:ComplianceFramework ;
    rdfs:label "related to framework" .

cas:hasEvidence a owl:ObjectProperty ;
    rdfs:domain cas:AuditEvent ;
    rdfs:range cas:Evidence ;
    rdfs:label "has evidence" .

cas:violatesPolicy a owl:ObjectProperty ;
    rdfs:domain cas:AuditEvent ;
    rdfs:range cas:Policy ;
    rdfs:label "violates policy" .

cas:triggeredBy a owl:ObjectProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range cas:AuditEvent ;
    rdfs:label "triggered by" .

cas:hasResponse a owl:ObjectProperty ;
    rdfs:domain cas:ComplianceIncident ;
    rdfs:range cas:IncidentResponse ;
    rdfs:label "has response" .

cas:reviewedBy a owl:ObjectProperty ;
    rdfs:domain cas:AccessReview ;
    rdfs:range cas:Reviewer ;
    rdfs:label "reviewed by" .

cas:mitigatedBy a owl:ObjectProperty ;
    rdfs:domain cas:RiskFinding ;
    rdfs:range cas:RiskMitigation ;
    rdfs:label "mitigated by" .

cas:classifiedAs a owl:ObjectProperty ;
    rdfs:domain cas:Evidence ;
    rdfs:range cas:DataClassification ;
    rdfs:label "classified as" .
