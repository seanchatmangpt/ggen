cas:expirationDate a owl:DatatypeProperty ;
    rdfs:domain cas:Policy ;
    rdfs:range xsd:date ;
    rdfs:label "expiration date" .

cas:retentionPeriod a owl:DatatypeProperty ;
    rdfs:domain cas:DataRetentionPolicy ;
