    thesis:orderIndex 1 ;
    thesis:labelId "tab:pattern-mapping" ;
    thesis:caption "Transformation pattern mappings from RDF to Rust" ;
    thesis:hasRow :Table3_1_Row1, :Table3_1_Row2, :Table3_1_Row3, :Table3_1_Row4, :Table3_1_Row5 .

:Table3_1_Row1 a thesis:TableRow ;
