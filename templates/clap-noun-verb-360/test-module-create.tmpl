---
to: "tests/integration/test_{{ noun | snake_case }}_{{ verb | snake_case }}.rs"
vars:
  noun: "Module"
  verb: "Create"
---

#[tokio::test]
async fn test_Module_LOWER_Create_LOWER() {
    // Setup
    let noun = "Module";
    let verb = "Create";

    // Execute
    let result = execute_noun_verb(noun, verb).await;

    // Assert
    assert!(result.is_ok());
}

#[tokio::test]
async fn test_Module_LOWER_Create_LOWER_invalid_input() {
    // Test with invalid input
    let result = execute_noun_verb("", "Create").await;
    assert!(result.is_err());
}

async fn execute_noun_verb(noun: &str, verb: &str) -> Result<String, Box<dyn std::error::Error>> {
    Ok(format!("{}({})", verb, noun))
}
