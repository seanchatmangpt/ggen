{
  "name": "@ggen/noun-verb-cli-generator",
  "version": "1.0.0",
  "description": "Generate production-ready noun-verb pattern CLIs following industry best practices from kubectl, docker, aws-cli, and gcloud",
  "keywords": [
    "cli",
    "generator",
    "noun-verb",
    "clap",
    "rust",
    "production",
    "kubectl",
    "docker",
    "aws-cli",
    "code-generation"
  ],
  "author": "ggen-ai <noreply@ggen.io>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/seanchatmangpt/ggen.git",
    "directory": "templates/cli/noun-verb-cli"
  },
  "homepage": "https://github.com/seanchatmangpt/ggen/tree/master/templates/cli",
  "bugs": {
    "url": "https://github.com/seanchatmangpt/ggen/issues"
  },
  "files": [
    "templates/**/*.tmpl",
    "scripts/**/*.sh",
    "docs/**/*.md",
    "README.md",
    "gpack.toml"
  ],
  "scripts": {
