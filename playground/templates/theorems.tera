% ============================================================================
% THEOREMS AND PROOFS - Generated from RDF Ontology
% ============================================================================

{% for row in sparql_results %}
% Theorem from section: {{ row["?sectionTitle"] }}
\begin{theorem}
  {{ row["?theoremStatement"] }}
  \label{thm:thm{{ loop.index }}}
\end{theorem}

{% if row["?proofSteps"] %}
\begin{proof}
  {{ row["?proofSteps"] }}
\end{proof}
{% endif %}

{% endfor %}
